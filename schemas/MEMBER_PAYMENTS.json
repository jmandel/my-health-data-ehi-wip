{
  "name": "MEMBER_PAYMENTS",
  "description": "This table contains information about payments by the member",
  "primaryKey": [
    {
      "columnName": "BENEFIT_USAGE_ID",
      "ordinalPosition": 1
    },
    {
      "columnName": "CONTACT_DATE_REAL",
      "ordinalPosition": 2
    },
    {
      "columnName": "LINE",
      "ordinalPosition": 3
    }
  ],
  "columns": [
    {
      "ordinalPosition": 1,
      "name": "BENEFIT_USAGE_ID",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "The unique ID of the benefit usage record."
    },
    {
      "ordinalPosition": 2,
      "name": "CONTACT_DATE_REAL",
      "type": "FLOAT",
      "discontinued": false,
      "description": "The date when a benefit usage was recorded."
    },
    {
      "ordinalPosition": 3,
      "name": "LINE",
      "type": "INTEGER",
      "discontinued": false,
      "description": "The line number for the information associated with this record. Multiple pieces of information can be associated with this record."
    },
    {
      "ordinalPosition": 4,
      "name": "PAYMENT_C",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "The payment type.",
      "entries": [
        "Check",
        "Credit Card",
        "Cash",
        "No Payment Due",
        "Other",
        "Unpaid"
      ]
    },
    {
      "ordinalPosition": 5,
      "name": "RECEIPT_NUM",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "The receipt number for the payment."
    },
    {
      "ordinalPosition": 6,
      "name": "AMOUNT_PAID",
      "type": "NUMERIC",
      "discontinued": false,
      "description": "The amount paid in the transaction."
    },
    {
      "ordinalPosition": 7,
      "name": "PAYMENT_NAME",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "The name appearing on check/credit card."
    },
    {
      "ordinalPosition": 8,
      "name": "CHECK_NUMBER",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "The check number."
    },
    {
      "ordinalPosition": 9,
      "name": "CHECK_DATE",
      "type": "DATETIME",
      "discontinued": false,
      "description": "The check date."
    },
    {
      "ordinalPosition": 10,
      "name": "CC_NUMBER",
      "type": "INTEGER",
      "discontinued": false,
      "description": "The credit card number."
    },
    {
      "ordinalPosition": 11,
      "name": "CC_EXPIRE",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "The expiration date for credit card."
    },
    {
      "ordinalPosition": 12,
      "name": "CC_TYPE_C",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "The credit card type.",
      "entries": [
        "Visa",
        "Discover",
        "American Express",
        "Master Card"
      ]
    },
    {
      "ordinalPosition": 13,
      "name": "OTHER_PAYMENT_C",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "The other mode of payment."
    }
  ]
}
