{
  "name": "CL_ORD_FST_LST_SCH",
  "description": "This table stores an order's first and last scheduled date and time, along with the type of review notice for the expire items (i.e. let expire or review).",
  "primaryKey": [
    {
      "columnName": "ORDER_ID",
      "ordinalPosition": 1
    }
  ],
  "columns": [
    {
      "ordinalPosition": 1,
      "name": "ORDER_ID",
      "type": "NUMERIC",
      "discontinued": false,
      "description": "The unique ID of the order record associated with this order."
    },
    {
      "ordinalPosition": 2,
      "name": "DATE_FIRST_ORDEREN",
      "type": "DATETIME",
      "discontinued": false,
      "description": "Date of the first schedule."
    },
    {
      "ordinalPosition": 3,
      "name": "TIME_FIRST_ORDEREN",
      "type": "DATETIME (Local) ",
      "discontinued": false,
      "description": "Time of the first schedule."
    },
    {
      "ordinalPosition": 4,
      "name": "DATE_LAST_ORDERENT",
      "type": "DATETIME",
      "discontinued": false,
      "description": "Date of the last schedule."
    },
    {
      "ordinalPosition": 5,
      "name": "TIME_LAST_ORDERENT",
      "type": "DATETIME (Local) ",
      "discontinued": false,
      "description": "Time of the last schedule."
    },
    {
      "ordinalPosition": 6,
      "name": "LET_EXPIRE_TYPE_C_NAME",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "Determines the type of review notice for the former \"let expire\" items",
      "entries": [
        "Let Expire",
        "Review"
      ]
    }
  ]
}
