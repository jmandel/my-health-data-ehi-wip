{
  "name": "OR_LOG_2",
  "description": "The OR_LOG_2 table contains OR management system log records.",
  "primaryKey": [
    {
      "columnName": "LOG_ID",
      "ordinalPosition": 1
    }
  ],
  "columns": [
    {
      "ordinalPosition": 1,
      "name": "LOG_ID",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "The unique ID of the surgical log record."
    },
    {
      "ordinalPosition": 2,
      "name": "SURGICAL_RISK_C_NAME",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "This column contains the category level of the surgical risk in the log record."
    },
    {
      "ordinalPosition": 3,
      "name": "POTENTL_BLOODLOSS_C_NAME",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "This column contains the category level of the potential blood loss in the log record."
    },
    {
      "ordinalPosition": 4,
      "name": "AWARENESS_DT",
      "type": "DATETIME",
      "discontinued": false,
      "description": "This column contains the date the patient was made aware of the surgery in the log record."
    },
    {
      "ordinalPosition": 5,
      "name": "VIDEO_PIC_TAKEN_YN",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "Indicates whether a video or picture was taken for this log.  Y indicates that a video or picture was taken, and N indicates that a video or picture was not taken.",
      "entries": [
        "No",
        "Yes"
      ]
    },
    {
      "ordinalPosition": 6,
      "name": "VIDEO_PIC_DISP_C_NAME",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "The disposition category number for the picture or video taken in the log."
    },
    {
      "ordinalPosition": 7,
      "name": "OB_CSECT_TYPE_C_NAME",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "The category number for the c-section type as recorded in the surgical log."
    },
    {
      "ordinalPosition": 8,
      "name": "PREOP_HOLD_LOC_C_NAME",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "Stores the location where the patient is being held in preop holding."
    },
    {
      "ordinalPosition": 9,
      "name": "NO_COUNT_NEEDED_YN",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "This item marks the log/procedure as a no counts needed log.",
      "entries": [
        "Counts not needed",
        "Counts needed"
      ]
    },
    {
      "ordinalPosition": 10,
      "name": "INTRAOP_XRAYS_CMT",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "This column displays the comment about intraop x-ray needs."
    },
    {
      "ordinalPosition": 11,
      "name": "IOP_XRAYS_C_NAME",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "The intraop x-rays needed category ID for the log record.",
      "entries": [
        "Yes",
        "No"
      ]
    },
    {
      "ordinalPosition": 12,
      "name": "ARE_PROPH_AB_REQ_C_NAME",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "The prophylactic antibiotics required category ID for the log record.",
      "entries": [
        "Yes",
        "No"
      ]
    },
    {
      "ordinalPosition": 13,
      "name": "CHARGE_BILL_AREA_ID",
      "type": "NUMERIC",
      "discontinued": false,
      "description": "The bill area with which log charges should be associated. This is used for cost center routing."
    },
    {
      "ordinalPosition": 14,
      "name": "CHARGE_BILL_AREA_ID_BILL_AREA_NAME",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "The record name of this bill area, financial subdivision, or financial division."
    },
    {
      "ordinalPosition": 15,
      "name": "RSN_RETURN_OR_C_NAME",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "The reason a patient is returned to the OR."
    },
    {
      "ordinalPosition": 16,
      "name": "NHSN_SPINAL_LEVEL_C_NAME",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "Stores the NHSN spinal level. The category list is the standard list of spinal levels released by NHSN.",
      "entries": [
        "A - Atlas-axis",
        "AC - Atlas-axis/Cervical",
        "C - Cervical",
        "CD - Cervical/Dorsal/Dorsolumbar",
        "D � Dorsal/Dorsolumbar",
        "L � Lumbar/Lumbosacral"
      ]
    },
    {
      "ordinalPosition": 17,
      "name": "HOLDING_BED_ID",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "Stores the bed record for the patient when in the Holding phase."
    },
    {
      "ordinalPosition": 18,
      "name": "HOLDING_BED_ID_BED_LABEL",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "The name of the bed."
    },
    {
      "ordinalPosition": 19,
      "name": "SPEC_NOT_REQUIRED_YN",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "Specimens were not required for this procedure."
    },
