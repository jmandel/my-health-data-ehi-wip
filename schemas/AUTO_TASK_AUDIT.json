{
  "name": "AUTO_TASK_AUDIT",
  "description": "This table contains the audit history for tasks that have been created or updated by the task engine. This table stores the following audit history for a task: triggering event, event source, event contact serial number (CSN), event time, and action taken on the task.",
  "primaryKey": [
    {
      "columnName": "TASK_ID",
      "ordinalPosition": 1
    },
    {
      "columnName": "LINE",
      "ordinalPosition": 2
    }
  ],
  "columns": [
    {
      "ordinalPosition": 1,
      "name": "TASK_ID",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "The unique identifier for the task instance record."
    },
    {
      "ordinalPosition": 2,
      "name": "LINE",
      "type": "INTEGER",
      "discontinued": false,
      "description": "The line number for the information associated with this record. Multiple pieces of information can be associated with this record."
    },
    {
      "ordinalPosition": 3,
      "name": "ADT_EVENT_C_NAME",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "Audit table for automatically created outpatient tasks - the event that created the task.",
