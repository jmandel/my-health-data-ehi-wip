{
  "name": "OTP_INFO_1",
  "description": "This table is a continuation of related table OTP_INFO. It stores additional information about a treatment plan order, such as verbal signing information, inpatient medication information, etc.",
  "primaryKey": [
    {
      "columnName": "OTP_ID",
      "ordinalPosition": 1
    }
  ],
  "columns": [
    {
      "ordinalPosition": 1,
      "name": "OTP_ID",
      "type": "NUMERIC",
      "discontinued": false,
      "description": "The unique identifier for the patient order template record."
    },
    {
      "ordinalPosition": 2,
      "name": "TRANSPORTATION_C_NAME",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "The transportation type associated with the order template in this row."
    },
    {
      "ordinalPosition": 3,
      "name": "IP_DISC_INTERVAL_ID",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "The discrete interval associated with the order template in this row."
    },
    {
      "ordinalPosition": 4,
      "name": "IP_DISC_INTERVAL_ID_FREQ_NAME",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "The name of the frequency record."
    },
    {
      "ordinalPosition": 5,
      "name": "IP_STANDING_COUNT",
      "type": "NUMERIC",
      "discontinued": false,
      "description": "The standing count associated with the order template in this row."
    },
    {
      "ordinalPosition": 6,
      "name": "IP_STAND_CNT_TYPE_C_NAME",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "The inpatient standing count type (Dose/Days/etc) associated with the order template in this row.",
      "entries": [
        "Days",
        "Weeks",
        "Hours",
        "Occurrences"
      ]
    },
    {
      "ordinalPosition": 7,
      "name": "IP_INCLUDE_NOW_C_NAME",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "The frequency associated with the order template in this row, for example, \"Include Now\", \"As Scheduled\", \"At Nurse Discretion.\"",
      "entries": [
        "Include Now",
        "As Scheduled",
        "At Nurse Discretion"
      ]
    },
    {
      "ordinalPosition": 8,
      "name": "SCHED_EXTRA_DOSE_TM",
      "type": "DATETIME (Local) ",
      "discontinued": false,
      "description": "The time at which to schedule the first occurrence of the associated order template in this row."
    },
    {
      "ordinalPosition": 9,
      "name": "COST_CENTER_ID",
      "type": "NUMERIC",
      "discontinued": false,
      "description": "The unique identifier for the cost center associated with the order template in this row."
    },
    {
      "ordinalPosition": 10,
      "name": "COST_CENTER_ID_COST_CENTER_NAME",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "The name of the cost center."
    },
    {
      "ordinalPosition": 11,
      "name": "ORDERING_MODE_C_NAME",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "The ordering mode (inpatient or outpatient) of the order template in this row.",
      "entries": [
        "Outpatient",
        "Inpatient"
      ]
    },
    {
      "ordinalPosition": 12,
      "name": "SELF_ADMIN_YN",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "Indicates whether the order template in this row is self-administered. �Y� indicates that the order template in this row is self-administered. �N� or NULL indicate that the order template in this row is not self-administered.",
      "entries": [
        "Yes",
        "No"
      ]
    },
    {
      "ordinalPosition": 13,
      "name": "PAT_SUPP_MED_YN",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "Indicates whether the order template in this row is a patient supplied medication. �Y� indicates that the order template in this row is a patient supplied medication. �N� or NULL indicate that the order template in this row is not a patient supplied medication."
    },
    {
      "ordinalPosition": 14,
      "name": "PAT_SUPPLIED_DOSES",
      "type": "NUMERIC",
      "discontinued": false,
      "description": "The number of patient supplied doses."
    },
