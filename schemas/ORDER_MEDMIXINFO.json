{
  "name": "ORDER_MEDMIXINFO",
  "description": "This table is used to extract ingredient medication information for mixture orders.",
  "primaryKey": [
    {
      "columnName": "ORDER_MED_ID",
      "ordinalPosition": 1
    },
    {
      "columnName": "LINE",
      "ordinalPosition": 2
    }
  ],
  "columns": [
    {
      "ordinalPosition": 1,
      "name": "ORDER_MED_ID",
      "type": "NUMERIC",
      "discontinued": false,
      "description": "The unique ID of the medication order (prescription) record."
    },
    {
      "ordinalPosition": 2,
      "name": "LINE",
      "type": "INTEGER",
      "discontinued": false,
      "description": "The line number for the information associated with this record. Multiple pieces of information can be associated with this record. Each line represents an ingredient of the mixture."
    },
    {
      "ordinalPosition": 3,
      "name": "MEDICATION_ID",
      "type": "NUMERIC",
      "discontinued": false,
      "description": "The unique identifier of a medication record used as an ingredient in the mixture."
    },
    {
      "ordinalPosition": 4,
      "name": "INGREDIENT_TYPE_C_NAME",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "The ingredient type category ID for an ingredient of a medication mixture, indicating what the type of ingredient such as medication or base.",
      "entries": [
        "Base",
        "Additives",
        "Medications",
        "Electrolytes",
        "Fat Emulsion",
        "Dextrose",
        "Amino Acids",
        "QS Base"
      ]
    },
    {
      "ordinalPosition": 5,
      "name": "MIN_DOSE_AMOUNT",
      "type": "NUMERIC",
      "discontinued": false,
      "description": "The minimum ordered dose amount of the ingredient."
    },
    {
      "ordinalPosition": 6,
      "name": "MAX_DOSE_AMOUNT",
      "type": "NUMERIC",
      "discontinued": false,
      "description": "The maximum ordered dose amount of the ingredient."
    },
