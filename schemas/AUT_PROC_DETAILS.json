    },
    {
      "ordinalPosition": 12,
      "name": "PROC_CODE_ID",
      "type": "NUMERIC",
      "discontinued": false,
      "description": "Item to track authorized Current Procedural Terminology (CPT)/Healthcare Common Procedure Coding System (HCPCS) codes."
