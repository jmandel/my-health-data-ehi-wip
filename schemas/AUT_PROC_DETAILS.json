{
  "name": "AUT_PROC_DETAILS",
  "description": "This table contains procedure-level details about authorizations. The associated authorization information for the procedure can be found in the AUTHORIZATIONS table by joining the tables on the AUTH_ID columns.\n\n\n\n\n\nChild table AUT_PROC_MODS contains multiple rows of additional information related to each row in this table. You may join to this table on the AUTH_ID columns and AUT_PROC_DETAILS.LINE = GROUP_LINE from that table.",
  "primaryKey": [
    {
      "columnName": "AUTH_ID",
      "ordinalPosition": 1
    },
    {
      "columnName": "LINE",
      "ordinalPosition": 2
    }
  ],
  "columns": [
    {
      "ordinalPosition": 1,
      "name": "AUTH_ID",
      "type": "NUMERIC",
      "discontinued": false,
      "description": "The authorization ID for the authorization record."
    },
    {
      "ordinalPosition": 2,
      "name": "LINE",
      "type": "INTEGER",
      "discontinued": false,
      "description": "The line number for the information associated with this record. Multiple pieces of information can be associated with this record."
    },
    {
      "ordinalPosition": 3,
      "name": "PROC_TYPE_C_NAME",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "Type of procedure information being authorized.  A procedure authorization may be collected for a specific code, a code range, or a procedure component.",
      "entries": [
        "Group",
        "Free Text",
        "Range",
        "CPT(R)/HCPCS",
        "Discipline",
        "Default Authorization",
        "Level of Care"
      ]
    },
    {
      "ordinalPosition": 4,
      "name": "PROC_FREE_TEXT",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "Procedure (free text) being authorized."
    },
    {
      "ordinalPosition": 5,
      "name": "PROC_RNG_START",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "Starting code of the code range"
    },
    {
      "ordinalPosition": 6,
      "name": "PROC_RNG_END",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "Ending code of the code range"
    },
    {
      "ordinalPosition": 7,
      "name": "PROC_COMP_ID",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "Procedure component being authorized. This column is frequently used to link to the CL_CMP table.",
      "linksTo": "CL_CMP"
    },
    {
      "ordinalPosition": 8,
      "name": "PROC_COMP_ID_COMPONENT_NAME",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "The name of the component"
