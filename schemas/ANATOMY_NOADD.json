{
  "name": "ANATOMY_NOADD",
  "description": "This table contains the items for dental teeth records.",
  "primaryKey": [
    {
      "columnName": "FINDING_ID",
      "ordinalPosition": 1
    }
  ],
  "columns": [
    {
      "ordinalPosition": 1,
      "name": "FINDING_ID",
      "type": "NUMERIC",
      "discontinued": false,
      "description": "The unique identifier for the finding record"
    },
    {
      "ordinalPosition": 2,
      "name": "DENTAL_TOOTH_STAT_C_NAME",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "Current status of the tooth",
      "entries": [
        "Missing",
        "Erupted",
        "Unerupted",
        "Partially Erupted",
        "Bridged",
        "Implant",
        "Denture",
        "Other"
      ]
    },
    {
      "ordinalPosition": 3,
      "name": "ANATOMY_ACTIVE_FLAG_C_NAME",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "Indicates whether a tooth should be shown in the tooth chart.",
      "entries": [
        "Inactive",
        "Active"
      ]
    },
    {
      "ordinalPosition": 4,
      "name": "ANATOMY_TEMPL_ID",
      "type": "NUMERIC",
      "discontinued": false,
      "description": "Identifier of the anatomy template associated with this record."
    },
    {
      "ordinalPosition": 5,
      "name": "ANATOMY_TEMPL_ID_RECORD_NAME",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "Stores record name (.2)"
    }
  ]
}
