    {
      "ordinalPosition": 4,
      "name": "MEDICATION_ROUTE_C_NAME",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "The route of the med."
    },
    {
      "ordinalPosition": 5,
      "name": "MEDICATION_SITE_C_NAME",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "The site of the med."
