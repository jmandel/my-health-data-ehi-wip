{
  "name": "DTX_ALL",
  "description": "This table contains information common across all detail transaction (DTX) records in the system for filtering purposes.",
  "primaryKey": [
    {
      "columnName": "TX_ID",
      "ordinalPosition": 1
    }
  ],
  "columns": [
    {
      "ordinalPosition": 1,
      "name": "TX_ID",
      "type": "NUMERIC",
      "discontinued": false,
      "description": "The unique identifier for the transaction record."
    },
    {
      "ordinalPosition": 2,
      "name": "TX_TYPE_C_NAME",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "The type of the transaction.",
      "entries": [
        "Incoming Receipt",
        "PCP Capitation",
        "Specialty Capitation",
        "AP Claims Adjustment",
        "Withhold Adjustment",
        "General Adjustment",
        "Other Reimbursement",
        "PCP Capitation Adjustment",
        "Specialty Capitation Adjustment"
      ]
    },
    {
      "ordinalPosition": 3,
      "name": "TX_STATUS_C_NAME",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "The status of the transaction.",
      "entries": [
        "Loaded",
        "Reconciled",
        "Calculated",
        "AP Batched",
        "Paid",
        "Void"
      ]
    }
  ]
}
