{
  "name": "ORDER_MEDINFO",
  "description": "The ORDER_MEDINFO table is an addendum table for ORDER_MED and enables you to report on detail medication information for each order in clinical system (prescriptions). We have also included patient and contact identification information for each record.",
  "primaryKey": [
    {
      "columnName": "ORDER_MED_ID",
      "ordinalPosition": 1
    }
  ],
  "columns": [
    {
      "ordinalPosition": 1,
      "name": "ORDER_MED_ID",
      "type": "NUMERIC",
      "discontinued": false,
      "description": "The unique ID of the medication order (prescription) record."
    },
    {
      "ordinalPosition": 2,
      "name": "MED_LINKED_PROC_ID",
      "type": "NUMERIC",
      "discontinued": false,
      "description": "The linked procedure ID for the medication.  Depending on pharmacy billing configuration, you may have only one procedure ID (code) for all medications or many."
    },
    {
      "ordinalPosition": 3,
      "name": "MED_CNCT_DAT_REAL",
      "type": "FLOAT",
      "discontinued": false,
      "description": "The real medication contact date (DAT) used in this order."
    },
    {
      "ordinalPosition": 4,
      "name": "LAST_ADMIN_INST",
      "type": "DATETIME (Local) ",
      "discontinued": false,
      "description": "The last instant that the medication order is administrated in the Medication Administration Record (MAR)."
    },
    {
      "ordinalPosition": 5,
      "name": "NUMBER_OF_DOSES",
      "type": "INTEGER",
      "discontinued": false,
      "description": "The total number of doses of the medication order that should be given to the patient."
    },
    {
      "ordinalPosition": 6,
      "name": "DOSES_REMAINING",
      "type": "INTEGER",
      "discontinued": false,
      "description": "The total number of the medication order which has not been given to patient."
    },
    {
      "ordinalPosition": 7,
      "name": "RESUME_STATUS_C_NAME",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "The resume status.",
      "entries": [
        "Default Action (Determined by configuration)",
        "Will not be resumed",
        "Resumed (Default)",
        "Auto-Discontinued",
        "Will be resumed",
        "Modified",
        "New",
        "No Change (pending)",
        "No Change"
      ]
    },
    {
      "ordinalPosition": 8,
      "name": "MIN_RATE",
      "type": "NUMERIC",
      "discontinued": false,
      "description": "The minimum rate number."
    },
    {
      "ordinalPosition": 9,
      "name": "MAX_RATE",
      "type": "NUMERIC",
      "discontinued": false,
      "description": "The maximum rate number."
    },
