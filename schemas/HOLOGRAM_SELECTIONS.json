{
  "name": "HOLOGRAM_SELECTIONS",
  "description": "This table stores details about each selection made in a hologram record. Which specific details are stored depends on the type of each row.",
  "primaryKey": [
    {
      "columnName": "HOLOGRAM_ID",
      "ordinalPosition": 1
    },
    {
      "columnName": "CONTACT_DATE_REAL",
      "ordinalPosition": 2
    }
  ],
  "columns": [
    {
      "ordinalPosition": 1,
      "name": "HOLOGRAM_ID",
      "type": "NUMERIC",
      "discontinued": false,
      "description": "The unique identifier (.1 item) for the hologram record."
    },
    {
      "ordinalPosition": 2,
      "name": "CONTACT_DATE_REAL",
      "type": "FLOAT",
      "discontinued": false,
      "description": "A unique contact date in decimal format. The integer portion of the number indicates the date of contact. The digits after the decimal distinguish different contacts on the same date and are unique for each contact on that date. For example, .00 is the first/only contact, .01 is the second contact, etc. Note this does not represent an actual date; it simply serves as a unique line identifier in a specific hologram."
    },
    {
      "ordinalPosition": 3,
      "name": "DISPLAY_NAME",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "Display name of the selection stored in this contact."
    },
    {
      "ordinalPosition": 4,
      "name": "LEVEL_OF_SERVICE_PROC_ID",
      "type": "NUMERIC",
      "discontinued": false,
      "description": "The unique identifier (.1 item) of the procedure for the level of service in this hologram record."
    },
    {
      "ordinalPosition": 5,
      "name": "SMARTTEXT_TYPE_C_NAME",
      "type": "VARCHAR",
