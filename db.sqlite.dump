CREATE TABLE HSP_CLP_REV_CODE ( -- This table contains the revenue code list for the claim print records associated with the hospital account/liability bucket.
  CLAIM_PRINT_ID NUMERIC, -- The unique identifier for the claim record associated with a single hospital account or liability bucket.
  CONTACT_DATE_REAL NUMERIC, -- The contact date for the creation of the record in internal format. (There is only one contact date per claim print record.)
  LINE INTEGER, -- The line number for the information associated with this record. Multiple pieces of information can be associated with this record.
  UB_MIN_SERVICE TEXT, -- The minimum service date for the claim print record.
  UB_MAX_SERVICE TEXT, -- The maximum service date for the claim print record.
  UB_CHARGES NUMERIC, -- The uniform billing charges on the claim
  UB_MODIFIER TEXT, -- The modifier for the claim print record.
  UB_CPT_CODE TEXT, -- The uniform billing current procedural terminology code on the claim print record.
  HSP_ACCOUNT_ID NUMERIC, -- The unique ID of the hospital account associated with this claim print record.
  REV_CODE_EXT TEXT, -- The external uniform billing revenue code.
  UB_REV_CD_DESC TEXT, -- The description of the uniform billing line.
  UB_QTY TEXT, -- The uniform billing line quality.
  UB_NON_CVD_AMT NUMERIC, -- The non-covered amount for the uniform billing line.
  UB_LMRP_CD TEXT, -- The uniform billing local coverage determination code.
  UB_HCPCS_RATE TEXT, -- The uniform billing healthcare common procedure coding system code and modifier or rate.
  UB_CODE_TYPE_C_NAME TEXT, -- The code type for the uniform billing claim line. If there is no code type this field will be blank.
  UB_LINE_SRC_C_NAME TEXT, -- The category of the line source for the uniform billing line.
  UB_SVC_DATE TEXT, -- The service date of the uniform billing service line.
  PRIMARY KEY (CLAIM_PRINT_ID, CONTACT_DATE_REAL, LINE) -- Primary Key, FOREIGN KEY (CLAIM_PRINT_ID, CONTACT_DATE_REAL) REFERENCES HSP_CLAIM_PRINT (CLAIM_PRINT_ID, CONTACT_DATE_REAL)
, FOREIGN KEY (CLAIM_PRINT_ID) REFERENCES HSP_CLAIM_DETAIL1 (CLAIM_PRINT_ID)
, FOREIGN KEY (CLAIM_PRINT_ID) REFERENCES HSP_CLAIM_DETAIL1 (CLAIM_PRINT_ID) -- References HSP_CLAIM_DETAIL1 table
, FOREIGN KEY (HSP_ACCOUNT_ID) REFERENCES HAR_ALL (ACCT_ID) -- References HAR_ALL table
, FOREIGN KEY (HSP_ACCOUNT_ID) REFERENCES HSP_ACCT_CLAIM_HAR (ACCT_ID) -- References HSP_ACCT_CLAIM_HAR table
, FOREIGN KEY (HSP_ACCOUNT_ID) REFERENCES HSP_ACCOUNT (HSP_ACCOUNT_ID) -- References HSP_ACCOUNT table

);
