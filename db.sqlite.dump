CREATE TABLE REFERRAL_HIST ( -- The REFERRAL_HIST table contains information on changes to referrals stored in system.
  REFERRAL_ID NUMERIC, -- The referral ID for the referral record.
  LINE INTEGER, -- The line number of the change to the referral. For example, if the referral is changed twice, the first change will have a line value of 1, while the second change will have a line value of 2.
  CHANGE_DATE TEXT, -- The date of the change to the referral.
  CHANGE_TYPE_C_NAME TEXT, -- The category value indicating the type of referral change.
  CHANGE_USER_ID TEXT, -- The ID number of the user who made the change to the referral.
  CHANGE_USER_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  PREVIOUS_VALUE TEXT, -- Some of the information on the referral is tracked when it changes. This stores the previous value of an item if it is tracked when it changes.
  CHANGE_DATETIME TEXT, -- The date and time of the change to the referral.
  AUTH_HX_EVENT_NO TEXT, -- The event no of the authorization history
  AUTH_HX_NOTE_ID TEXT, -- Referral audit trail item to store note Id
  CHANGE_LOCAL_DTTM TEXT, -- The authorization date and time as an instant in the local time zone
  NEW_RFL_STATUS_C_NAME TEXT, -- If the item being changed is Referral Status (I RFL 50), this item stores the new value for that item.
  NEW_PEND_RSN_C_NAME TEXT, -- If the item being changed is Reason Pending (I RFL 18003), this item stores the new value for that item.
  PRIMARY KEY (REFERRAL_ID, LINE) -- Primary Key, FOREIGN KEY (REFERRAL_ID) REFERENCES REFERRAL (REFERRAL_ID)
, FOREIGN KEY (REFERRAL_ID) REFERENCES REFERRAL (REFERRAL_ID) -- References REFERRAL table
, FOREIGN KEY (CHANGE_USER_ID) REFERENCES CLARITY_EMP (USER_ID) -- References CLARITY_EMP table

);
