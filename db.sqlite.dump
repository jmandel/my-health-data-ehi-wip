CREATE TABLE ARPB_TX_VOID ( -- This table contains information on transactions that were either: * Transferred * Voided * Reversed * Retroadjudicated
  TX_ID NUMERIC, -- The unique identifier for the transaction record.
  OLD_ETR_ID NUMERIC, -- This item holds a pointer for a transaction that was reposted retroactively to the original transaction.
  REPOSTED_ETR_ID NUMERIC, -- This item stores a pointer to the old transaction in the case of charge correction or repost.
  REPOST_TYPE_C_NAME TEXT, -- This indicates the type of repost on the source transaction.  This is either set to repost or correction.
  DEL_REVERSE_DATE TEXT, -- This is the date that the transaction was voided or reversed.
  DEL_CHARGE_USER_ID TEXT, -- This item stores the user who voided this transaction.
  DEL_CHARGE_USER_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  DEL_CHARGE_INSTANT TEXT, -- This item stores the instant that a transaction was voided.
  PRIMARY KEY (TX_ID) -- Primary Key, FOREIGN KEY (TX_ID) REFERENCES ARPB_TRANSACTIONS (TX_ID) -- References ARPB_TRANSACTIONS table

