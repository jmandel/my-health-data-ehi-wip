CREATE TABLE ORDER_READ_ACK ( -- This table is used to store information from the overtime-related Orders items used for the result read/acknowledgment tracking feature. Namely, this is the Read or Acknowledge by User (I ORD 1910) and Read or Acknowledged On (I ORD 1915) items.-- Child table: ORD_RSLT_COMPON_ID (1 to 1 rows) joined on ORDER_ID, CONTACT_DATE_REAL, GROUP_LINE

  ORDER_ID NUMERIC, -- The unique identifier for the order record.
  CONTACT_DATE_REAL NUMERIC, -- A unique contact date in decimal format. The integer portion of the number indicates the date of contact. The digits after the decimal distinguish different contacts on the same date and are unique for each contact on that date. For example, .00 is the first/only contact, .01 is the second contact, etc.
  LINE INTEGER, -- The line number for when and who viewed the results associated with this contact. Multiple pieces of information can be associated with this contact.
  CONTACT_DATE TEXT, -- The date of this contact in calendar format.
  READ_ACK_ACTUAL_UTC_DTTM TEXT, -- This item stores when the result was actually viewed by an end user.
  WHO_READ_ACK_EMP_ID TEXT, -- This item stores which user viewed this result.
  WHO_READ_ACK_EMP_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  PRIMARY KEY (ORDER_ID, CONTACT_DATE_REAL, LINE) -- Primary Key, FOREIGN KEY (ORDER_ID) REFERENCES ORDER_MYC_INFO (ORDER_PROC_ID)
, FOREIGN KEY (ORDER_ID) REFERENCES ORDER_PARENT_INFO (ORDER_ID)
, FOREIGN KEY (ORDER_ID, CONTACT_DATE_REAL) REFERENCES ORDER_STATUS (ORDER_ID, ORD_DATE_REAL)
, FOREIGN KEY (ORDER_ID) REFERENCES ORDER_PROC (ORDER_PROC_ID)
, FOREIGN KEY (ORDER_ID) REFERENCES ORD_PRFLST_TRK (ORDER_ID)
, FOREIGN KEY (ORDER_ID) REFERENCES ORDER_MYC_INFO (ORDER_PROC_ID) -- References ORDER_MYC_INFO table
, FOREIGN KEY (ORDER_ID) REFERENCES ORDER_PARENT_INFO (ORDER_ID) -- References ORDER_PARENT_INFO table
, FOREIGN KEY (ORDER_ID) REFERENCES ORDER_PROC (ORDER_PROC_ID) -- References ORDER_PROC table
, FOREIGN KEY (ORDER_ID) REFERENCES ORD_PRFLST_TRK (ORDER_ID) -- References ORD_PRFLST_TRK table
, FOREIGN KEY (WHO_READ_ACK_EMP_ID) REFERENCES CLARITY_EMP (USER_ID) -- References CLARITY_EMP table

);
