  NOTE_ID TEXT, -- The unique identifier (.1 item) for the note record.
  LINE INTEGER, -- The line number for the information associated with this record. Multiple pieces of information can be associated with this record.
  ORDER_ID NUMERIC, -- The order ID associated to the note.
  ORDER_DAT TEXT, -- The order DAT associated to the note.
  PRIMARY KEY (NOTE_ID, LINE) -- Primary Key, FOREIGN KEY (NOTE_ID) REFERENCES HNO_INFO (NOTE_ID)
, FOREIGN KEY (NOTE_ID) REFERENCES HNO_INFO (NOTE_ID) -- References HNO_INFO table
, FOREIGN KEY (ORDER_ID) REFERENCES ORDER_MYC_INFO (ORDER_PROC_ID) -- References ORDER_MYC_INFO table
, FOREIGN KEY (ORDER_ID) REFERENCES ORDER_PARENT_INFO (ORDER_ID) -- References ORDER_PARENT_INFO table
, FOREIGN KEY (ORDER_ID) REFERENCES ORDER_PROC (ORDER_PROC_ID) -- References ORDER_PROC table
, FOREIGN KEY (ORDER_ID) REFERENCES ORD_PRFLST_TRK (ORDER_ID) -- References ORD_PRFLST_TRK table

);
