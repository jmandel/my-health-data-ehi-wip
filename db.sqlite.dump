CREATE TABLE IMMUNE_HISTORY ( -- The IMMUNE_HISTORY table contains the history data for immunizations ordered through the clinical system. It may also contain information on immunizations as reported by the patient, but not ordered/administered via clinical system. This table contains the history of changes made to the information that is found in the IMMUNE table. Fields in this table are no-add related group items in the LPL database.
  IMMUNE_ID NUMERIC, -- The unique ID of the immunization record in your system production system.
  LINE INTEGER, -- The Line Count for the line in the table which in combination with the IMMUNE_ID forms the primary key for this table.
  IMM_TYPE_HIST_ID NUMERIC, -- Stores the unique ID of the immunizations (LIM) master file which is associated with this immunization record.  Corresponds to the type of immunization given to this patient.
  IMM_TYPE_HIST_ID_NAME TEXT, -- The name of the immunization.
  IMM_HX_PRODUCT TEXT, -- Stores the product information associated with this immunization. Products are usually related to the lot number.
  IMM_HX_NDC_NUM_ID TEXT, -- The unique ID of the Medication National Drug Codes (NDC) master file that is associated with this immunization and stores the NDC numbers associated with the administration of this immunization.
  IMM_HX_NDC_NUM_ID_NDC_CODE TEXT, -- The external code for the National Drug Code (NDC). An NDC represents packages of medications.
  IMMNZTN_HX_DATE TEXT, -- The  date when this immunization was administered.
  IMMNZTN_HX_DOSE TEXT, -- The dosage information for this immunization administered.
  IMMNZTN_HX_ROUTE_C_NAME TEXT, -- The category number for the route of the immunization, such as oral, intramuscular, or intradermal.
  IMMNZTN_HX_SITE_C_NAME TEXT, -- The location of the injection, if appropriate.
  IMMNZTN_HX_MFG_C_NAME TEXT, -- The category number for the manufacturer of this vaccine.
  IMMNZTN_HX_LOT TEXT, -- The LOT number for the immunization administered.
  IMM_HX_EXP_DATE TEXT, -- The date on which the immunization administered expires.
  IMMNZTN_HX_GIVEN_ID TEXT, -- The unique ID of the user in the EMP master file that is listed in the clinical system as actually administering the  immunization to the patient.
  IMMNZTN_HX_GIVEN_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  IMMNZTN_HX_EXT_AD_C_NAME TEXT, -- The category number for the source of verification of external administration of immunization.
  IMM_HX_ANSWER_ID TEXT, -- The unique ID in the questionnaire answers (HQA) master file that is associated with the immunization administered.
  IMMNZTN_HX_VIS_DATE TEXT, -- The free text date field associated with the immunization where VIS (Vaccine Information Statements) date is stored.
  IMMNZTN_HX_COMMENT TEXT, -- The free text comments associated with the immunization administered.
  IMMNZTN_HX_ENTRY_ID TEXT, -- The unique ID of the user  in the EMP masterfile associated with the person who entered the immunization administration information into the clinical system.
  IMMNZTN_HX_ENTRY_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  IMM_HX_HIST_ADMI_YN TEXT, -- Indicates whether or not an immunization is historical or not.
  IMMNZTN_HX_ENT_DATE TEXT, -- The date on which the immunization was entered into the system.
  IMMNZTN_HX_STATUS_C_NAME TEXT, -- The category number for the immunization status. Examples are "Given" if the immunization has been administered, "Deleted" if the immunization has been deleted from the administration history, "Incomplete" if the item has been ordered but not administered and a status of "Deferred" if the immunization has been deferred.
  IMM_CHRG_REC_HX_ID TEXT, -- The unique ID in the Universal Charge Line (UCL) master file that is associated with the immunization administered.
  IMMNZTN_HX_ENC_CSN NUMERIC, -- This column stores the history information of immunization CSN whenever an edit was made on the Immunization.
  IMMNZTN_HX_DOSE_AMT NUMERIC, -- The history of dosage amount for the immunization administered.
  IMMNZTN_HX_DOSE_UNIT_C_NAME TEXT, -- The history of dosage unit for the immunization administered.
  IMMZTN_HX_ENTRY_DTTM TEXT, -- Contains the date and time that the data in the row was entered. If the exact time is not known, a date may be contained in IMMNZTN_HX_ENT_DATE instead.
  IMM_HX_LOT_NUM_ID NUMERIC, -- The history of lot(LOT) record Id for the immunization adminstered.
  PRIMARY KEY (IMMUNE_ID, LINE) -- Primary Key, FOREIGN KEY (IMMUNE_ID) REFERENCES IMMUNE (IMMUNE_ID)
, FOREIGN KEY (IMMUNE_ID) REFERENCES PROBLEM_LIST_ALL (PROBLEM_LIST_ID)
, FOREIGN KEY (IMMUNE_ID) REFERENCES IMMUNE (IMMUNE_ID) -- References IMMUNE table
, FOREIGN KEY (IMMUNE_ID) REFERENCES PROBLEM_LIST_ALL (PROBLEM_LIST_ID) -- References PROBLEM_LIST_ALL table
, FOREIGN KEY (IMM_TYPE_HIST_ID) REFERENCES CLARITY_IMMUNZATN (IMMUNZATN_ID) -- References CLARITY_IMMUNZATN table
, FOREIGN KEY (IMMNZTN_HX_ENTRY_ID) REFERENCES CLARITY_EMP (USER_ID) -- References CLARITY_EMP table

);
