  ADDR_HX_LINE1 TEXT, -- First line of patient's home address, current between dates recorded in columns EFF_START_DATE and EFF_END_DATE.
  CITY_HX TEXT, -- Patient's home city, current between dates recorded in columns EFF_START_DATE and EFF_END_DATE.
  COUNTY_HX_C_NAME TEXT, -- Patient's home county, current between dates recorded in columns EFF_START_DATE and EFF_END_DATE.
  ZIP_HX TEXT, -- Patient's home ZIP, current between dates recorded in columns EFF_START_DATE and EFF_END_DATE.
  ADDR_CHNG_SOURCE_C_NAME TEXT, -- Source of address changes.
  EFF_START_DATE TEXT, -- Effective start date of changed address (date when address was changed).
  EFF_END_DATE TEXT, -- Effective end date of changed address (date of the next address change or NULL if this is the last address change).
  SIGNIFICANT_CHANGE_YN TEXT, -- Audit trail item used to store whether the address change should be considered significant when a new address is entered or if the current primary address is edited.
  COUNTRY_C_NAME TEXT, -- Audit trail item used to store the previous country when a new country is entered or if the current primary address is edited.
  PRIMARY KEY (PAT_ID, LINE) -- Primary Key, FOREIGN KEY (PAT_ID) REFERENCES PATIENT (PAT_ID)
, FOREIGN KEY (PAT_ID) REFERENCES PATIENT (PAT_ID) -- References PATIENT table

);
