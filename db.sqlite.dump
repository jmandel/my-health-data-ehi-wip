CREATE TABLE CLM_VALUES ( -- All values associated with a claim are stored in the Claim External Value record. The CLM_VALUES table holds claim-level values set by the system during claims processing or by user edits.-- Child table: CLM_DX (1 to 4 rows) joined on RECORD_ID
  -- Child table: CLM_NOTE (1 to 1 rows) joined on RECORD_ID
  -- Child table: EXT_CAUSE_INJ_DX (1 to 1 rows) joined on RECORD_ID
  -- Child table: OCC_CD (2 to 2 rows) joined on RECORD_ID
  -- Child table: PAT_RSN_VISIT_DX (1 to 1 rows) joined on RECORD_ID
  -- Child table: REL_CAUSE_CD (1 to 1 rows) joined on RECORD_ID
  -- Child table: SVC_LN_INFO (1 to 3 rows) joined on RECORD_ID

  RECORD_ID NUMERIC, -- This column stores the unique identifier for the claim record.
  BIL_PROV_TYP_QUAL TEXT, -- The value indicating whether the billing provider on the claim is a person or a non-person.
  BIL_PROV_NAM_LAST TEXT, -- The billing provider's last name (if a person) or the organization name (if a non-person).
  BIL_PROV_NPI TEXT, -- The billing provider's National Provider Identifier (NPI).
  BIL_PROV_TAXONOMY TEXT, -- The billing provider's taxonomy code.
  BIL_PROV_TAXID_QUAL TEXT, -- The qualifier for the billing provider's tax ID defining the type of ID reported as the tax ID.
  BIL_PROV_TAXID TEXT, -- The billing provider's tax ID. For individuals, this ID could be the SSN or tax ID.
  BIL_PROV_ADDR_1 TEXT, -- The first line of the billing provider's street address.
  BIL_PROV_CITY TEXT, -- The billing provider's city.
  BIL_PROV_STATE TEXT, -- The billing provider's state.
  BIL_PROV_ZIP TEXT, -- The billing provider's ZIP Code.
  CLM_CVG_SEQ_CD TEXT, -- The code identifying the filing order for the claim (e.g., primary, secondary, tertiary).
  CLM_CVG_PYR_NAM TEXT, -- The payer name.
  CLM_CVG_GRP_NUM TEXT, -- The group number entered in the coverage record.
  CLM_CVG_GRP_NAM TEXT, -- The group name entered in the coverage record.
  CLM_CVG_FILING_IND TEXT, -- The indicator identifying the type of claim.
  CLM_CVG_PYR_ID_TYP TEXT, -- The qualifier that describes the type of ID used to identify the payer.
  CLM_CVG_PYR_ID TEXT, -- The primary ID for the payer.
  CLM_CVG_ACPT_ASGN TEXT, -- The indicator that the provider accepts assignment from the payer.
  CLM_CVG_AUTH_PMT TEXT, -- The indicator that the insured assigns benefits to the provider.
  CLM_CVG_REL_INFO TEXT, -- The indicator that the insured has authorized the release of information to the payer.
  PYR_ADDR_1 TEXT, -- The first line of the payer's street address.
  PYR_CITY TEXT, -- The payer's city.
  PYR_STATE TEXT, -- The payer's state.
  PYR_ZIP TEXT, -- The payer's ZIP Code.
  PAT_NAM_LAST TEXT, -- The patient's last name.
  PAT_NAM_FIRST TEXT, -- The patient's first name.
  PAT_NAM_MID TEXT, -- The patient's middle name.
  PAT_MRN TEXT, -- The patient's medical record number.
  PAT_REL_TO_INS TEXT, -- The patient's relationship to the coverage subscriber.
  PAT_BIRTH_DATE TEXT, -- The patient's birthdate.
  PAT_SEX TEXT, -- The patient's gender.
  PAT_SIG_ON_FILE TEXT, -- The indicator that the patient has signed the necessary release forms and the forms are on file at the provider.
  PAT_MAR_STAT TEXT, -- The patient's marital status.
  PAT_EMPY_STAT TEXT, -- The patient's employment status.
  PAT_PH TEXT, -- The patient's phone number.
  PAT_ADDR_1 TEXT, -- The first line of the patient's street address.
  PAT_CITY TEXT, -- The patient's city.
  PAT_STATE TEXT, -- The patient's state.
  PAT_ZIP TEXT, -- The patient's ZIP Code.
  INV_NUM TEXT, -- The invoice number that uniquely identifies the claim in the billing system.
  ICN TEXT, -- The payer's internal control number (ICN) that uniquely identifies the claim in the payer's system.
  TTL_CHG_AMT NUMERIC, -- The total charge amount for the claim.
  BILL_TYP_FAC_CD TEXT, -- The facility code portion of the bill type (first and second digits).
  BILL_TYP_FREQ_CD TEXT, -- The frequency code portion of the bill type (third digit).
  ADMSN_TYP TEXT, -- The admission type for the claim.
  ADMSN_SRC TEXT, -- The admission source for the claim.
  DISCHRG_DISP TEXT, -- The patient's discharge status, also referred to as the discharge disposition.
  CLIA_NUM TEXT, -- The Clinical Laboratory Improvement Amendment (CLIA) number when the claim contains lab services.
  OUTSIDE_LAB TEXT, -- The indicator that the claim includes purchased services rendered by an independent provider.
  OUTSIDE_LAB_CHG NUMERIC, -- The price of the purchased services.
  CLM_FROM_DT TEXT, -- The earliest date represented on the claim. This date could be the minimum service date for an outpatient or professional claim or the admission date for an inpatient claim.
  CLM_TO_DT TEXT, -- The latest date represented on the claim. This date could be the maximum service date for an outpatient or professional claim or the discharge date for an inpatient claim.
  ACDNT_DT TEXT, -- The date the automobile accident occurred.
  ATT_PROV_NAM_LAST TEXT, -- The attending provider's last name.
  ATT_PROV_NAM_FIRST TEXT, -- The attending provider's first name.
  ATT_PROV_NAM_MID TEXT, -- The attending provider's middle name.
  ATT_PROV_NPI TEXT, -- The attending provider's National Provider Identifier (NPI).
  ATT_PROV_TAXONOMY TEXT, -- The attending provider's taxonomy code.
  REND_PROV_TYP TEXT, -- The rendering provider on the claim is a person or a non-person.
  REND_PROV_NAM_LAST TEXT, -- The rendering provider's last name (if a person) or the organization name (if a non-person).
  REND_PROV_NAM_FIRST TEXT, -- The rendering provider's first name. It is only populated when the provider is a person.
  REND_PROV_NAM_MID TEXT, -- The rendering provider's middle name. It is only populated when the provider is a person.
  REND_PROV_NAM_SUF TEXT, -- The suffix to the rendering provider's name (e.g., Jr, III). It is only populated when the provider is a person.
  REND_PROV_NPI TEXT, -- The rendering provider's National Provider Identifier (NPI).
  REND_PROV_TAXONOMY TEXT, -- The rendering provider's taxonomy code.
  REF_PROV_NAM_LAST TEXT, -- The referring provider's last name.
  REF_PROV_NAM_FIRST TEXT, -- The referring provider's first name.
  REF_PROV_NAM_MID TEXT, -- The referring provider's middle name.
  REF_PROV_NPI TEXT, -- The referring provider's National Provider Identifier (NPI).
  REF_PROV_TAXONOMY TEXT, -- The referring provider's taxonomy code.
  SVC_FAC_NAM TEXT, -- The name of the external location where the services were performed.
  SVC_FAC_NPI TEXT, -- The NPI of the external location where the services were performed.
  SVC_FAC_CNCT_PH TEXT, -- The contact phone number for the external location.
  SVC_FAC_ADDR_1 TEXT, -- The first line of the external location street address.
  SVC_FAC_CITY TEXT, -- The external location's city.
  SVC_FAC_STATE TEXT, -- The external location's state.
  SVC_FAC_ZIP TEXT, -- The external location's ZIP code.
  CREATE_DT TEXT, -- The date the claim was created. It is used for paper institutional claims.
  CLM_CVG_AMT_PAID NUMERIC, -- The amount already paid by the payer of the current coverage.
  CLM_CVG_AMT_DUE NUMERIC, -- The amount due by the payer of the current coverage.
  CLM_CVG_REL_INFO_DT TEXT, -- The date on which the insured authorized the release of information to the payer.
  RECORD_CREATION_DT TEXT, -- The date the record was created.
  CLM_TYP_C_NAME TEXT, -- The type of claim values stored in the record. CMS is used for professional and dental claims. UB is used for institutional claims.
  FORM_TYP_C_NAME TEXT, -- The type of form used during processing.
  SERV_AREA_ID NUMERIC, -- The service area for the claim values record.
  PRIMARY KEY (RECORD_ID) -- Primary Key, FOREIGN KEY (SERV_AREA_ID) REFERENCES CLARITY_SA (SERV_AREA_ID) -- References CLARITY_SA table
, FOREIGN KEY (SERV_AREA_ID) REFERENCES CLARITY_LOC (LOC_ID) -- References CLARITY_LOC table

);
