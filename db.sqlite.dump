CREATE TABLE HSP_TX_LINE_INFO ( -- This table contains Line level transactions information from Hospital Permanent Transactions (HTR).
  TX_ID NUMERIC, -- This column stores the unique identifier for the hospital billing transaction.
  LINE INTEGER, -- The line number for the information associated with this record. Multiple pieces of information can be associated with this record.
  HSP_ACCOUNT_ID NUMERIC, -- This column stores the unique identifier for the hospital account.
  POST_DATE TEXT, -- The post date for the transaction on the hospital account.
  SERV_AREA_ID NUMERIC, -- This column stores the unique identifier for the service area associated with the hospital billing transaction on the hospital account.
  LL_REV_CODE_ID NUMERIC, -- This column stores the line-level revenue code for the hospital billing transaction on the hospital account.
  LL_REV_CODE_ID_REVENUE_CODE_NAME TEXT, -- The name of the revenue code.
  LL_CPT_CODE TEXT, -- This column stores the line-level CPT code for the hospital billing transaction on the hospital account.
  LL_MODIFIER TEXT, -- This column stores the line-level modifier info for the hospital billing transaction on the hospital account.
  LL_SERVICE_DATE TEXT, -- This column stores the line-level service date for the hospital billing transaction on the hospital account.
  LL_BILLED_AMT NUMERIC, -- This column stores the line-level billed amount for the hospital billing transaction on the hospital account.
  LL_ALLOWED_AMT NUMERIC, -- This column stores the line-level allowed amount for the hospital billing transaction on the hospital account.
  LL_NOT_ALLOWED_AMT NUMERIC, -- This column stores the line-level not-allowed amount for the hospital billing transaction on the hospital account.
  LL_DED_AMT NUMERIC, -- This column stores the line-level deductible amount for the hospital billing transaction on the hospital account.
  LL_POSTED_AMT NUMERIC, -- This column stores the line-level posted amount for the hospital billing transaction on the hospital account.
  LL_REASON_CODES TEXT, -- This column stores the line-level reason codes for the hospital billing transaction on the hospital account.
  LL_ACTIONS TEXT, -- This column stores the line-level action string for the hospital billing transaction on the hospital account.
  LL_QUANTITY NUMERIC, -- This column stores the line-level quantity from remittance payments.
  PRIMARY KEY (TX_ID, LINE) -- Primary Key, FOREIGN KEY (TX_ID) REFERENCES HSP_TRANSACTIONS (TX_ID)
, FOREIGN KEY (TX_ID) REFERENCES HSP_TRANSACTIONS (TX_ID) -- References HSP_TRANSACTIONS table
, FOREIGN KEY (HSP_ACCOUNT_ID) REFERENCES HAR_ALL (ACCT_ID) -- References HAR_ALL table
, FOREIGN KEY (HSP_ACCOUNT_ID) REFERENCES HSP_ACCT_CLAIM_HAR (ACCT_ID) -- References HSP_ACCT_CLAIM_HAR table
