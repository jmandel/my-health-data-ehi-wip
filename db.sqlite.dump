CREATE TABLE INV_CLM_ICN ( -- This table holds information of claim override internal control number (ICN) on A/R Invoice (INV) for Resolute Professional Billing.
  INVOICE_ID NUMERIC, -- The invoice record ID.
  LINE INTEGER, -- The line number for the information associated with this record. Multiple pieces of information can be associated with this record.
  CLM_OVRD_ICN_CVG_ID INTEGER, -- The override coverage ID for the internal control number for the claim.
  CLM_OVERRIDE_ICN TEXT, -- The override internal control number for the claim.
  CLM_OVRD_ICN_T_DTTM TEXT, -- The override edit time for the internal control number for the claim.
  CLM_OVRD_ICN_USR_ID TEXT, -- The override edit user for the internal control number for the claim.
  CLM_OVRD_ICN_USR_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  CLM_OVRD_ICN_IGN_YN TEXT, -- Whether or not the override for the internal control number for the claim was ignored.
  CLM_OVRD_ICN_CMNT TEXT, -- The override comment for the internal control number for the claim.
  PRIMARY KEY (INVOICE_ID, LINE) -- Primary Key, FOREIGN KEY (INVOICE_ID) REFERENCES INVOICE (INVOICE_ID)
, FOREIGN KEY (INVOICE_ID) REFERENCES INVOICE (INVOICE_ID) -- References INVOICE table
, FOREIGN KEY (CLM_OVRD_ICN_CVG_ID) REFERENCES COVERAGE (COVERAGE_ID) -- References COVERAGE table
, FOREIGN KEY (CLM_OVRD_ICN_USR_ID) REFERENCES CLARITY_EMP (USER_ID) -- References CLARITY_EMP table

);
