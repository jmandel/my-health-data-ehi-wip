CREATE TABLE PAT_ENC_LETTERS ( -- The patient encounter letters table contains information about letters associated with encounters.
  PAT_ENC_CSN_ID NUMERIC, -- The unique contact serial number for this contact. This number is unique across all patient encounters in your system. If you use IntraConnect, this is the Unique Contact Identifier (UCI).
  LINE INTEGER, -- The line count used to identify different letters associated with an encounter.
  CONTACT_DATE TEXT, -- The date of this contact in calendar format.
  LTR_STATUS_C_NAME TEXT, -- The category value associated with the status of each letter,  such as 1- Open, 2- Deleted, 3- Sent, 4- Voided, 5-Sent by batch.
  LETTER_CREAT_DT TEXT, -- The date that the letter was created.
  LETTER_HNO_ID TEXT, -- The note (HNO) record that hold the letter text.
  LETTER_FROM_ID TEXT, -- The user ID of the user entered as the "from" user in the letter activity.
  LETTER_FROM_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  LETTER_REASON_C_NAME TEXT, -- The category value associated with the reason for the letter.
  LTR_STATUS_CHG_TM TEXT, -- The instant (date and time) the status of the letter was created or changed.
  LTR_STAT_CH_USR_ID TEXT, -- The user that changed or created the letter status.
  LTR_STAT_CH_USR_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  LETTER_WORKINGDAYS INTEGER, -- The number of work days (excluding weekends and holidays) from the date of the letter's encounter to the date the letter was sent.
  PRIMARY KEY (PAT_ENC_CSN_ID, LINE) -- Primary Key, FOREIGN KEY (PAT_ENC_CSN_ID) REFERENCES PAT_ENC (PAT_ENC_CSN_ID)
, FOREIGN KEY (PAT_ENC_CSN_ID) REFERENCES PAT_ENC (PAT_ENC_CSN_ID) -- References PAT_ENC table
, FOREIGN KEY (LETTER_HNO_ID) REFERENCES HNO_INFO (NOTE_ID) -- References HNO_INFO table
, FOREIGN KEY (LETTER_FROM_ID) REFERENCES CLARITY_EMP (USER_ID) -- References CLARITY_EMP table
, FOREIGN KEY (LTR_STAT_CH_USR_ID) REFERENCES CLARITY_EMP (USER_ID) -- References CLARITY_EMP table

);
