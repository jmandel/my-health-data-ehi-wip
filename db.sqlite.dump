CREATE TABLE IMM_ADMIN_GROUPS ( -- This table extracts the related multiple response Vaccine Group (I DXR 4220) item.
  DOCUMENT_ID NUMERIC, -- The unique identifier (.1 item) for the document record.
  CONTACT_DATE_REAL NUMERIC, -- A unique contact date in decimal format. The integer portion of the number indicates the date of contact. The digits after the decimal distinguish different contacts on the same date and are unique for each contact on that date. For example, .00 is the first/only contact, .01 is the second contact, etc.
  GROUP_LINE INTEGER, -- The line number for the information associated with this contact.
  VALUE_LINE INTEGER, -- The line number of one of the multiple values associated with a specific group of data within this contact.
  CONTACT_DATE TEXT, -- The date of this contact in calendar format.
  IMM_GROUPS_ID NUMERIC, -- Stores the discrete value for a vaccine group for an immunization administration received from an external system. This can be either the component or the family of a vaccine. The corresponding free-text value received from registry is stored in DXR 4221.
  IMM_GROUPS_ID_NAME TEXT, -- The name of the immunization.
  PRIMARY KEY (DOCUMENT_ID, CONTACT_DATE_REAL, GROUP_LINE, VALUE_LINE) -- Primary Key, FOREIGN KEY (DOCUMENT_ID, CONTACT_DATE_REAL, GROUP_LINE) REFERENCES IMM_ADMIN (DOCUMENT_ID, CONTACT_DATE_REAL, LINE)
, FOREIGN KEY (DOCUMENT_ID, CONTACT_DATE_REAL, GROUP_LINE) REFERENCES IMM_DUE (DOCUMENT_ID, CONTACT_DATE_REAL, LINE)
