CREATE TABLE IP_FLOWSHEET_ROWS ( -- This table contains flowsheet row (FLO) data for an encounter. This table is a key table in tying LDA assessment row lines in flowsheet data records to the LDAs, and the necessary joins are:       IP_FLOWSHEET_ROWS.IP_LDA_ID with IP_LDA_NOADDSINGLE.IP_LDA_ID   IP_FLOWSHEET_ROWS.INPATIENT_DATA_ID with IP_FLWSHT_REC.INPATIENT_DATA_ID   IP_FLOWSHEET_ROWS.LINE with IP_FLWSHT_MEAS.OCCURANCE   IP_FLWSHT_REC.FSD_ID with IP_FLWSHT_MEAS.FSD_ID.
  INPATIENT_DATA_ID TEXT, -- The unique ID of the inpatient data store record.
  LINE INTEGER, -- The line count for the item.
  FLO_MEAS_ID TEXT, -- The unique ID of the flowsheet group/row.
  FLO_MEAS_ID_DISP_NAME TEXT, -- The display name given to the flowsheet group/row.
  IP_LDA_ID TEXT, -- Stores the Lines/Drains/Airways (LDA) ID for the flowsheet group.
  ROW_VARIANCE_C_NAME TEXT, -- The flowsheet row variance.
  PRIMARY KEY (INPATIENT_DATA_ID, LINE) -- Primary Key, FOREIGN KEY (INPATIENT_DATA_ID) REFERENCES IP_DATA_STORE (INPATIENT_DATA_ID)
, FOREIGN KEY (INPATIENT_DATA_ID) REFERENCES IP_DATA_STORE (INPATIENT_DATA_ID) -- References IP_DATA_STORE table
, FOREIGN KEY (FLO_MEAS_ID) REFERENCES IP_FLO_GP_DATA (FLO_MEAS_ID) -- References IP_FLO_GP_DATA table

);
