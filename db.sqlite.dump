CREATE TABLE HSP_CLP_CMS_LINE ( -- This table contains claim line information for claims associated with the hospital account/liability bucket. For uniform medical billing (UB) claims, this table contains pre-processing charge information, which is used in the creation of UB claim lines. Post-processing claim line data for UB claims is stored in the HSP_CLP_REV_CODE table. For CMS claims, this table contains the post-processing claim line data.-- Child table: HSP_CLP_CMS_TX_PIECES (1 to 1 rows) joined on CLAIM_PRINT_ID, LINE
  -- Child table: HSP_CLP_UB_TX_PIECES (1 to 1 rows) joined on CLAIM_PRINT_ID, LINE

  CLAIM_PRINT_ID NUMERIC, -- The ID of the claim record associated with a single hospital account or liability bucket.
  LINE INTEGER, -- The line number of one of the multiple values associated with a specific group of data within this record.
  FROM_SERV_DT TEXT, -- Stores the from date for a claim line. For services that do not span multiple days, the service date will be held here.
  POS_TYPE_PER_TX TEXT, -- Stores the place of service type per transaction.
  PROC_ID NUMERIC, -- Stores the internal procedure ID.
  PROC_DESC TEXT, -- Stores the procedure description.
  HCPCS_CODES TEXT, -- Stores the Healthcare Common Procedure Coding System code for the claim line.
  DX_MAP TEXT, -- Comma-delimited list of diagnosis pointers for the claim line.
  QUANTITY NUMERIC, -- Stores the quantity associated with the claim line.
  OVRD_REV_CODE_ID NUMERIC, -- Stores the override revenue code.
  OVRD_REV_CODE_ID_REVENUE_CODE_NAME TEXT, -- The name of the revenue code.
  CHARGE_AMT NUMERIC, -- Stores the charge amount for the claim line.
  PAYMENT_AMT NUMERIC, -- Stores the payment amount for the claim line.
  PRINT_DESCRIPTIO_YN TEXT, -- This controls procedure description printing for professional claims.
  REV_LOCATION_ID NUMERIC, -- Revenue location for the line.
  DEPT_ID NUMERIC, -- Department of service for the line.
  LINE_POS_ID NUMERIC, -- Place of service ID.
  CMS_CODE_TYPE_C_NAME TEXT, -- Stores the code type for the transaction level Healthcare Common Procedure Coding System code override. If a procedure has been assigned to the line without setting the override, this column will be left blank.
  INVOICE_GRP_LN TEXT, -- The group line number on the invoice record that corresponds to the data in INVOICE_CLM_LN.
  NON_GROUP_HTR_ID NUMERIC, -- Stores charges that are active on the current claim.
  PRIMARY KEY (CLAIM_PRINT_ID, LINE) -- Primary Key, FOREIGN KEY (CLAIM_PRINT_ID) REFERENCES HSP_CLAIM_DETAIL1 (CLAIM_PRINT_ID)
, FOREIGN KEY (CLAIM_PRINT_ID) REFERENCES HSP_CLAIM_DETAIL1 (CLAIM_PRINT_ID) -- References HSP_CLAIM_DETAIL1 table
, FOREIGN KEY (PROC_ID) REFERENCES CLARITY_EAP (PROC_ID) -- References CLARITY_EAP table
, FOREIGN KEY (OVRD_REV_CODE_ID) REFERENCES CL_UB_REV_CODE (UB_REV_CODE_ID) -- References CL_UB_REV_CODE table
, FOREIGN KEY (REV_LOCATION_ID) REFERENCES CLARITY_LOC (LOC_ID) -- References CLARITY_LOC table
, FOREIGN KEY (DEPT_ID) REFERENCES CLARITY_DEP (DEPARTMENT_ID) -- References CLARITY_DEP table
, FOREIGN KEY (LINE_POS_ID) REFERENCES CLARITY_LOC (LOC_ID) -- References CLARITY_LOC table
, FOREIGN KEY (NON_GROUP_HTR_ID) REFERENCES HSP_TRANSACTIONS (TX_ID) -- References HSP_TRANSACTIONS table

);
