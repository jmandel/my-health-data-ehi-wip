CREATE TABLE ARPB_TX_MATCH_HX ( -- Matching History Transaction Related Items.  A line is added to this related group whenever two transactions are matched together.-- Child table: SVC_PMT_HISTORY (1 to 1 rows) joined on TX_ID, GROUP_LINE

  TX_ID NUMERIC, -- The unique identifier for the transaction record.
  LINE INTEGER, -- The line number for the information associated with this record. Multiple pieces of information can be associated with this record.
  MTCH_TX_HX_DT TEXT, -- This item stores the date that a transaction was matched to this transaction.
  MTCH_TX_HX_ID NUMERIC, -- This item stores the transaction that this transaction was matched to.  If the MTCH_TX_HX_UN_DT is null, then the transaction is still currently matched to this transaction.
  MTCH_TX_HX_AMT NUMERIC, -- This item stores the (insurance+self-pay) amount for which this transaction is matched to the transaction in column MTCH_TX_HX_ID.
  MTCH_TX_HX_INS_AMT NUMERIC, -- This item stores the insurance amount for which this transaction is matched to the transaction in column MTCH_TX_HX_ID.
  MTCH_TX_HX_PAT_AMT NUMERIC, -- This item stores the self-pay amount for which this transaction is matched to the transaction in column MTCH_TX_HX_ID.
  MTCH_TX_HX_COMMENT TEXT, -- This item holds the comment for the matching of this transaction to the transaction in column MTCH_TX_HX_ID.  This item is typically only populated by the system and not user entered comments.
  MTCH_TX_HX_UN_DT TEXT, -- This item holds the date that the transaction was unmatched from the transaction in column MTCH_TX_HX_ID.
  MTCH_TX_HX_D_CVG_ID NUMERIC, -- This item stores the coverage ID at the time that the transaction was matched to the transaction in column MTCH_TX_HX_ID.
  MTCH_TX_HX_DSUSR_ID TEXT, -- This item stores the users who matched this transaction to the transaction from column MTCH_TX_HX_ID.
  MTCH_TX_HX_DSUSR_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  MTCH_TX_HX_UDUSR_ID TEXT, -- This item stores the user that unmatched this transaction from the transaction in the MTCH_TX_HX_ID column.
  MTCH_TX_HX_UDUSR_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  MTCH_TX_HX_INV_NUM TEXT, -- This item stores the invoice associated with the debit transaction in the matching group.
  MTCH_TX_HX_UN_CV_ID NUMERIC, -- This is the coverage of the debit transaction at the time of unmatch.
  MTCH_TX_HX_LINE INTEGER, -- This item stores the corresponding line from the matched transaction.
  MTCH_TX_HX_DTTM TEXT, -- The UTC date and time the transaction was matched.
  MTCH_TX_HX_UN_DTTM TEXT, -- The UTC date and time the transaction was unmatched.
  PRIMARY KEY (TX_ID, LINE) -- Primary Key, FOREIGN KEY (TX_ID) REFERENCES ARPB_TRANSACTIONS (TX_ID)
, FOREIGN KEY (TX_ID) REFERENCES ARPB_TRANSACTIONS (TX_ID) -- References ARPB_TRANSACTIONS table
, FOREIGN KEY (MTCH_TX_HX_ID) REFERENCES ARPB_TRANSACTIONS (TX_ID) -- References ARPB_TRANSACTIONS table
, FOREIGN KEY (MTCH_TX_HX_DSUSR_ID) REFERENCES CLARITY_EMP (USER_ID) -- References CLARITY_EMP table

);
