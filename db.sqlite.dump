CREATE TABLE MEDICAL_HX ( -- The MEDICAL_HX table contains data from medical history contacts entered in clinical system patient encounters. Since one patient encounter may contain multiple medical history contacts, each contact is uniquely identified by a patient encounter serial number and a line number.
  LINE INTEGER, -- The line number of the medical history contact within the encounter. Each line of history is stored in enterprise reporting as its own record; a given patient may have multiple records (identified by line number) that reflect multiple lines of history.
  DX_ID NUMERIC, -- The unique ID of the diagnosis record associated with the medical history contact. Note: This is NOT the ICD9 diagnosis code. It is an internal identifier that is typically not visible to a user.
  PAT_ENC_CSN_ID NUMERIC, -- A unique serial number for this encounter. This number is unique across all patients and encounters in the system.
  PRIMARY KEY (PAT_ENC_CSN_ID, LINE) -- Primary Key, FOREIGN KEY (PAT_ENC_CSN_ID) REFERENCES PAT_ENC (PAT_ENC_CSN_ID)
, FOREIGN KEY (PAT_ENC_CSN_ID) REFERENCES SOCIAL_HX (PAT_ENC_CSN_ID)
, FOREIGN KEY (DX_ID) REFERENCES CLARITY_EDG (DX_ID) -- References CLARITY_EDG table
, FOREIGN KEY (PAT_ENC_CSN_ID) REFERENCES PAT_ENC (PAT_ENC_CSN_ID) -- References PAT_ENC table
, FOREIGN KEY (PAT_ENC_CSN_ID) REFERENCES SOCIAL_HX (PAT_ENC_CSN_ID) -- References SOCIAL_HX table

);
