  MYC_QUESR_START_DT TEXT, -- Stores the start date of when a questionnaire can be shown for an upcoming appointment,
  PAT_APPT_QNR_STAT_C_NAME TEXT, -- The status of the patient-entered appointment questionnaire.
  PRIMARY KEY (PAT_ENC_CSN_ID, LINE) -- Primary Key, FOREIGN KEY (PAT_ENC_CSN_ID) REFERENCES PAT_ENC (PAT_ENC_CSN_ID)
, FOREIGN KEY (PAT_ENC_CSN_ID) REFERENCES PAT_ENC (PAT_ENC_CSN_ID) -- References PAT_ENC table
, FOREIGN KEY (MYC_APPT_QUESR_ID) REFERENCES CL_QFORM1 (FORM_ID) -- References CL_QFORM1 table

