CREATE TABLE INV_CLM_LN_ADDL ( -- This table holds additional line-level information about the invoice (INV) specific to a given invoice including any line-level overrides.-- Child table: INV_NUM_TX_PIECES (1 to 1 rows) joined on INV_ID, LINE
  -- Child table: INV_TX_PIECES (1 to 1 rows) joined on INV_ID, LINE

  INVOICE_ID NUMERIC, -- The invoice record ID.
  LINE INTEGER, -- The line number.
  INVOICE_NUM TEXT, -- The invoice number related to this claim line.
  CLM_LN INTEGER, -- The invoice claim line number.
  PROC_OR_REV_CODE TEXT, -- This is the procedure revenue code
  POS_CODE TEXT, -- The place of service type for this claim line
  CLAIM_STATUS_C_NAME TEXT, -- The claim line status.
  CLAIM_PAID_AMT NUMERIC, -- The claim line paid amount.
  EOB_ALLOWED_AMOUNT NUMERIC, -- The service line's explanation of benefits adjustment amount.
  EOB_NON_COVRD_AMT NUMERIC, -- The service line's explanation of benefits non-covered amount.
  EOB_COINSURANCE NUMERIC, -- The service line's explanation of benefits coinsurance amount.
  EOB_DEDUCTIBLE NUMERIC, -- The service line's explanation of benefits deductible.
  EOB_ICN TEXT, -- The explanation of benefits internal control number for the claim line.
  CLAIM_DENIED_CODE TEXT, -- Claim denied code for this claim line on this invoice.
  REMIT_CODE_ID TEXT, -- Remittance code for this claim line on this invoice.
  TRANSACTION_LIST TEXT, -- The charges associated with the invoice. May hold a comma delimited list of professional transactions if the charges were bundled.
  FROM_SVC_DATE TEXT, -- The date when the service was first performed.
  PROC_ID NUMERIC, -- The unique ID of the procedure associated with the invoice.
  MODIFIER_ONE TEXT, -- The first modifier associated with the invoice. This is the external modifier, as it was printed on the claim.
  QUANTITY NUMERIC, -- The number of units associated with the invoice.
  CHARGE_AMOUNT NUMERIC, -- The charge amount associated with the claim line.
  NONCVD_AMOUNT NUMERIC, -- The non-covered amount associated with the invoice.
  TYPE_OF_SERVICE_C_NAME TEXT, -- The type of service category value for the claim.
  DIAGNOSIS_MAP TEXT, -- Holds a comma-delimited list of pointers to the claim level diagnosis. The first number listed represents the primary diagnosis for the charge.
  REMITTANCE_RMC1_ID TEXT, -- First remittance code ID.
  REMITTANCE_RMC1_ID_REMIT_CODE_NAME TEXT, -- The name of each remittance code.
  REMITTANCE_RMC2_ID TEXT, -- Second remittance code ID.
  REMITTANCE_RMC2_ID_REMIT_CODE_NAME TEXT, -- The name of each remittance code.
  CLM_LN_CREAT_DATE TEXT, -- Stores the date the claim line is created.
  INV_NUM_GRP100LN INTEGER, -- The invoice line number.
  CLM_LN_PAID_DATE TEXT, -- Stores the most recent date the invoice line is paid.
  IS_CODE_ONLY TEXT, -- Identifies show only lines.
  PRIMARY KEY (INVOICE_ID, LINE) -- Primary Key, FOREIGN KEY (INVOICE_ID) REFERENCES INVOICE (INVOICE_ID)
, FOREIGN KEY (INVOICE_ID) REFERENCES INVOICE (INVOICE_ID) -- References INVOICE table
, FOREIGN KEY (PROC_ID) REFERENCES CLARITY_EAP (PROC_ID) -- References CLARITY_EAP table

