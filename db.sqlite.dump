CREATE TABLE CL_REMIT ( -- This table stores information for each Image Database (IMD) record. This can be check-level or claim-level, and is indicated in column IMD_TYPE_C.-- Child table: CL_RMT_CLM_DT_INFO (1 to 2 rows) joined on IMAGE_ID
  -- Child table: CL_RMT_CLM_ENTITY (2 to 3 rows) joined on IMAGE_ID
  -- Child table: CL_RMT_HC_RMK_CODE (1 to 1 rows) joined on IMAGE_ID
  -- Child table: CL_RMT_SVC_AMT_INF (1 to 3 rows) joined on IMAGE_ID
  -- Child table: CL_RMT_SVC_LVL_ADJ (1 to 5 rows) joined on IMAGE_ID
  -- Child table: CL_RMT_SVCE_LN_INF (1 to 3 rows) joined on IMAGE_ID

  IMAGE_ID TEXT, -- This is the ID for the remittance image record. A separate remittance image record is created for each invoice payment.
  CREATION_DATE TEXT, -- The date when the remittance image record was created (i.e., when the electronic file was loaded and created).
  SERVICE_AREA_ID NUMERIC, -- The service area of the invoice to which this remittance payment was posted.
  PAYMENT_METHOD_C_NAME TEXT, -- The posting method by which this remittance record was created (manual or through electronic remittance).
  PAYMENT_TYPE_C_NAME TEXT, -- The payment type for this remittance record (self-pay or insurance).
  REF_IMG_ID TEXT, -- Reference remittance record for general remittance file information.  A separate remittance image record is created for each invoice payment in the remittance file. The general check level information is stored in the first remittance image record and subsequent image records hold the image ID of that record in this item.
  PAT_ID TEXT, -- The patient from the invoice to which the payment in the remittance image record is posted.
  CLM_START_DATE TEXT, -- The starting date of the claim that was sent out to the payer.
  CLM_END_DATE TEXT, -- The ending date of the claim that was sent out to the payer.
  CLP_ID NUMERIC, -- Internal ID of the claim record (for Hospital Billing only).
  IMD_TYPE_C_NAME TEXT, -- Specifies the type of Remittance Image. 1 is Check Level and 2 is File Level.
  INV_NO TEXT, -- The invoice number for the remittance image.
  CLM_STAT_CD_C_NAME TEXT, -- This is the code identifying the status of an entire claim.
  CLAIM_CHRG_AMT NUMERIC, -- This is the amount for submitted charges on this claim.
  CLAIM_PAID_AMT NUMERIC, -- This is the amount paid on the claim.
  PAT_RESP_AMT NUMERIC, -- This is the patient responsibility amount for the claim.
  CLM_FILING_CODE_C_NAME TEXT, -- This is a code identifying the type of claim.
  ICN_NO TEXT, -- This is the payer's internal control number for the claim.
  FAC_CODE_VAL TEXT, -- This is the facility code used when the submitted code has been modified through adjudication.
  CLAIM_FREQ_C_NAME TEXT, -- This is the frequency code of the claim.
  DRG_WGT NUMERIC, -- The diagnosis related group weight.
  FILE_INV_NUM TEXT, -- Contains the actual invoice number that came in the file.
  PRIMARY KEY (IMAGE_ID) -- Primary Key, FOREIGN KEY (SERVICE_AREA_ID) REFERENCES CLARITY_SA (SERV_AREA_ID) -- References CLARITY_SA table
, FOREIGN KEY (SERVICE_AREA_ID) REFERENCES CLARITY_LOC (LOC_ID) -- References CLARITY_LOC table
, FOREIGN KEY (PAT_ID) REFERENCES PATIENT (PAT_ID) -- References PATIENT table

);
