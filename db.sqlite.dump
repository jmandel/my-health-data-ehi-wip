CREATE TABLE IMM_DUE ( -- The IMM_DUE table contains information about when an immunization is due. The rows included in this table are items from DXR (Document) masterfile which include information on type of immunization, due date, earliest date and next dose number for the due immunization.-- Child table: IMM_ADMIN_GROUPS_FT (1 to 1 rows) joined on DOCUMENT_ID, CONTACT_DATE_REAL, GROUP_LINE
  -- Child table: IMM_ADMIN_GROUPS (1 to 1 rows) joined on DOCUMENT_ID, CONTACT_DATE_REAL, GROUP_LINE

  DOCUMENT_ID NUMERIC, -- The unique identifier for the document record.
  CONTACT_DATE_REAL NUMERIC, -- A unique contact date in decimal format. The integer portion of the number indicates the date of contact. The digits after the decimal distinguish different contacts on the same date and are unique for each contact on that date. For example, .00 is the first/only contact, .01 is the second contact, etc.
  LINE INTEGER, -- The line number for the information associated with this contact. Multiple pieces of information can be associated with this contact.
  CONTACT_DATE TEXT, -- The date of this contact in calendar format.
  IMM_DUE_TYPE_ID NUMERIC, -- The type ID of immunization that is due. This column is frequently used to link to the table CLARITY_IMMUNZATN.
  IMM_DUE_TYPE_ID_NAME TEXT, -- The name of the immunization.
  IMM_DUE_TYPE_FT TEXT, -- The free text description of the type of immunization that is due.
  IMM_DUE_DUE_DATE TEXT, -- The due date for immunization that is due.
  IMM_DUE_EARLIEST_DT TEXT, -- The earliest date of immunization that is due.
  IMM_DUE_NEXT_DOSE TEXT, -- The next dose number of immunization that is due.
  IMM_DUE_SCHED_ID_FT TEXT, -- Immunization schedule ID used for the recommended vaccination.
  IMM_DUE_SCHED_NM_FT TEXT, -- Immunization schedule name used for the recommended vaccination.
  IMM_DUE_SCHED_CD_FT TEXT, -- Immunization schedule coding system used for the recommended vaccination.
  PRIMARY KEY (DOCUMENT_ID, CONTACT_DATE_REAL, LINE) -- Primary Key
);
