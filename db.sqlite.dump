, FOREIGN KEY (NDC_ID) REFERENCES RX_NDC (NDC_ID) -- References RX_NDC table
, FOREIGN KEY (LINE_PTR) REFERENCES CLARITY_SA (SERV_AREA_ID) -- References CLARITY_SA table
, FOREIGN KEY (LINE_PTR) REFERENCES CLARITY_DEP (DEPARTMENT_ID) -- References CLARITY_DEP table
, FOREIGN KEY (LINE_PTR) REFERENCES CLARITY_LOC (LOC_ID) -- References CLARITY_LOC table

);
