CREATE TABLE BDC_INFO ( -- This table contains Denial/Remark/Correspondence/Variance information from the Denial/Correspondence (BDC) master file. It includes information about the denial/remark code received or correspondence text as applicable. It also includes some general information about the current state of the record. It does not include the change history information and specific denial information from the explanation of benefits (EOB).-- Child table: BDC_ASSOC_REMARK_CODES (1 to 1 rows) joined on BDC_ID
  -- Child table: BDC_PB_CHGS (1 to 1 rows) joined on BDC_ID
  -- Child table: HSP_BDC_DENIAL_DATA (1 to 1 rows) joined on BDC_ID
  -- Child table: HSP_BDC_PAYOR (1 to 1 rows) joined on BDC_ID
  -- Child table: HSP_BDC_RECV_TX (1 to 1 rows) joined on BDC_ID

  BDC_ID NUMERIC, -- The unique identifier for the denial/correspondence record.
  BDC_NAME TEXT, -- BDC Record Name - Denial/Remark/Correspondence Record Name.
  RECORD_TYPE_C_NAME TEXT, -- The record type category ID for the Denial/Remark/Correspondence.
  RECORD_STATUS_C_NAME TEXT, -- The status category ID for the Denial/Remark/Correspondence.
  RECORD_SOURCE_C_NAME TEXT, -- The source category ID for the Denial/Remark/Correspondence.
  INVOICE_NUMBER TEXT, -- The invoice number associated with this denial/correspondence.
  GRP_CODE_C_NAME TEXT, -- This item holds the group code associated with the reason code in a denial correspondence record.
  REMIT_CODE_ID TEXT, -- The unique ID of the remittance code associated with the record.
  REMIT_CODE_ID_REMIT_CODE_NAME TEXT, -- The name of each remittance code.
  EXTERNAL_CODE TEXT, -- The external ID of the remittance code associated with this denial/correspondence.
  RESOLVE_REASON_C_NAME TEXT, -- Resolution Category for Denial/Remark/Correspondence if it has been resolved.
  BDC_RECEIVE_DATE TEXT, -- The date the Denial/Remark/Correspondence was received.
  BDC_COMPLETE_VOID_DATE TEXT, -- The date the Denial/Remark/Correspondence record was completed or voided.
  PB_INVOICE_ID NUMERIC, -- The professional invoice associated with the denial/correspondence record.
  GUARANTOR_ID NUMERIC, -- The unique ID of the guarantor associated with the denial/correspondence record.
  SOURCE_PMT_PB_TX_ID NUMERIC, -- Professional Billing payment transaction which led to denial/correspondence record creation.
  APPEAL_DEADLINE_DATE TEXT, -- This item stores the initial appeal deadline at the moment the BDC is created.
  PRIMARY KEY (BDC_ID) -- Primary Key, FOREIGN KEY (REMIT_CODE_ID) REFERENCES CLARITY_RMC (REMIT_CODE_ID) -- References CLARITY_RMC table
, FOREIGN KEY (PB_INVOICE_ID) REFERENCES INVOICE (INVOICE_ID) -- References INVOICE table
, FOREIGN KEY (GUARANTOR_ID) REFERENCES ACCOUNT (ACCOUNT_ID) -- References ACCOUNT table
, FOREIGN KEY (SOURCE_PMT_PB_TX_ID) REFERENCES ARPB_TRANSACTIONS (TX_ID) -- References ARPB_TRANSACTIONS table

);
