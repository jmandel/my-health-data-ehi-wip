CREATE TABLE FRONT_END_PMT_COLL_HX ( -- This table stores information about front-end collection actions taken through point of sale (POS) payment posting or refund workflows.
  PAT_ENC_CSN_ID NUMERIC, -- The unique contact serial number for this contact. This number is unique across all patient encounters in your system. If you use IntraConnect, this is the Unique Contact Identifier (UCI).
  LINE INTEGER, -- The line number for the information associated with this contact. Multiple pieces of information can be associated with this contact.
  CONTACT_DATE TEXT, -- The date of this contact in calendar format.
  COLL_INSTANT_UTC_DTTM TEXT, -- The instant that this event occurred.
  COLL_WORKFLOW_TYPE_C_NAME TEXT, -- The workflow in which this event took place.
  LOGIN_DEPARTMENT_ID NUMERIC, -- The unique ID of the department which a user was logged into at the time of this event.
  ENC_DEPARTMENT_ID NUMERIC, -- The unique ID of the encounter department associated with this event.
  RSN_NON_COLL_AMT_C_NAME TEXT, -- The non-collection reason category ID for this event.
  RSN_NON_COLL_AMT_COMMENT TEXT, -- The free text non-collection comment explaining why some portion of a due amount was not collected.
  GUARANTOR_ACCOUNT_ID NUMERIC, -- The unique ID of the guarantor who is associated with this event.
  EVENT_TYPE_C_NAME TEXT, -- The event type category ID that defines the type of payment event data that is stored in this row.
  PB_COPAY_COLL NUMERIC, -- The amount of professional billing copay that a user collected during this event.
  PB_COPAY_PAID NUMERIC, -- The amount of professional billing copay that had already been paid towards an encounter at the time of this event.
  PB_COPAY_DUE NUMERIC, -- The total amount of professional billing copay that is required for this visit at the time of this event.
  HB_COPAY_COLL NUMERIC, -- The amount of hospital billing copay that a user collected during this event.
  HB_COPAY_PAID NUMERIC, -- The amount of hospital billing copay that had already been paid towards an encounter at the time of this event.
  HB_COPAY_DUE NUMERIC, -- The total amount of hospital billing copay that is required for this visit at the time of this event.
  PB_PREPAY_COLL NUMERIC, -- The amount of professional billing prepayment that a user collected.
  PB_PREPAY_PAID NUMERIC, -- The amount of professional billing prepayment that had already been paid towards an encounter at the time of this event.
  PB_PREPAY_DUE NUMERIC, -- The total amount of professional billing prepayment that is required for this visit at the time of this event.
  HB_PREPAY_COLL NUMERIC, -- The amount of hospital billing prepayment that a user collected during this event.
  HB_PREPAY_PAID NUMERIC, -- The amount of hospital billing prepayment that had already been paid towards an encounter at the time of this event.
  HB_PREPAY_DUE NUMERIC, -- The total amount of hospital billing prepayment that is required for this visit at the time of this event.
  PB_PREV_BAL_COLL NUMERIC, -- The amount of professional billing previous balance that a user collected during this event.
  PB_PREV_BAL_PAID NUMERIC, -- The amount of professional billing previous balance that had already been paid towards this guarantor's outstanding balance during the day of this event.
  PB_PREV_BAL_DUE NUMERIC, -- The amount of self-pay professional billing outstanding balance that a guarantor owed at the time of this event.
  HB_PREV_BAL_COLL NUMERIC, -- The amount of hospital billing previous balance that a user collected during this event.
  HB_PREV_BAL_PAID NUMERIC, -- The amount of hospital billing previous balance that had already been paid towards this guarantor's outstanding balance during the day of this event.
  HB_PREV_BAL_DUE NUMERIC, -- The amount of self-pay hospital billing outstanding balance that a guarantor owed at the time of this event.
  PREPAY_DISCOUNT_OFFERED NUMERIC, -- The total amount of the prepay discount that was offered for this visit at the time of this event.
  PRIMARY KEY (PAT_ENC_CSN_ID, LINE) -- Primary Key, FOREIGN KEY (PAT_ENC_CSN_ID) REFERENCES PAT_ENC (PAT_ENC_CSN_ID)
, FOREIGN KEY (PAT_ENC_CSN_ID) REFERENCES PAT_ENC (PAT_ENC_CSN_ID) -- References PAT_ENC table
, FOREIGN KEY (LOGIN_DEPARTMENT_ID) REFERENCES CLARITY_DEP (DEPARTMENT_ID) -- References CLARITY_DEP table
, FOREIGN KEY (ENC_DEPARTMENT_ID) REFERENCES CLARITY_DEP (DEPARTMENT_ID) -- References CLARITY_DEP table
, FOREIGN KEY (GUARANTOR_ACCOUNT_ID) REFERENCES ACCOUNT (ACCOUNT_ID) -- References ACCOUNT table

);
