CREATE TABLE IP_LDA_NOADDSINGLE ( -- This table stores LDA information for a patient. A record is created in LDA for insertion of every line, drain, airway, or wound for a patient, as well as entering a trip into a patient's travel history. The no-add information for this LDA is stored in the table.-- Child table: IP_LDA_INPS_USED (1 to 1 rows) joined on IP_LDA_ID

  IP_LDA_ID TEXT, -- The internal ID of the Lines/Drains/Airways (LDA) record.
  PAT_ID TEXT, -- This item stores the ID of the patient to which this line record was added.
  PAT_ENC_CSN_ID NUMERIC, -- This item stores the contact serial number of the encounter in which the record was created.
  FLO_MEAS_ID TEXT, -- This item stores the Flowsheet ID that defines the structure of this record. It is the flowsheet group that is used to define the set of rows for the Line/Drain/Airway (LDA).
  FLO_MEAS_ID_DISP_NAME TEXT, -- The display name given to the flowsheet group/row.
  FSD_ID TEXT, -- This item stores the flowsheet data (FSD) ID of the record that has information about the properties of the line/drain/airway (LDA).
  DESCRIPTION TEXT, -- This item stores the name/description of the line/drain.
  PROPERTIES_DISPLAY TEXT, -- Stores the properties display string to be displayed in Doc Flowsheets and Reports.
  LDA_GROUP_CDR NUMERIC, -- This column stores the contact date real of the Line/Drain/Airway (LDA) Group contact that created this LDA.  This column is stored in Epic's datetime (DTE) format.
  TRIP_REGION_ID NUMERIC, -- Represents where the patient traveled for this trip
  TRIP_REGION_ID_GEO_REGION_NAME TEXT, -- The record name for the geographical region record.
  TRIP_BEGIN_DATE TEXT, -- Represents when a patient began their trip
  TRIP_END_DATE TEXT, -- Represents the end of this patient trip
  TRIP_DATE_APPROX_C_NAME TEXT, -- Whether the dates for this trip are exact or approximate
  RECORDED_DTTM TEXT, -- The recorded time used in IP_FLWSHT_MEAS for storing the property data for this LDA.
  PRIMARY KEY (IP_LDA_ID) -- Primary Key, FOREIGN KEY (PAT_ID) REFERENCES PATIENT (PAT_ID) -- References PATIENT table
, FOREIGN KEY (PAT_ENC_CSN_ID) REFERENCES PAT_ENC (PAT_ENC_CSN_ID) -- References PAT_ENC table
, FOREIGN KEY (FLO_MEAS_ID) REFERENCES IP_FLO_GP_DATA (FLO_MEAS_ID) -- References IP_FLO_GP_DATA table
, FOREIGN KEY (FSD_ID) REFERENCES IP_FLWSHT_REC (FSD_ID) -- References IP_FLWSHT_REC table
, FOREIGN KEY (TRIP_REGION_ID) REFERENCES CLARITY_HM_TOPIC (HM_TOPIC_ID) -- References CLARITY_HM_TOPIC table
, FOREIGN KEY (TRIP_REGION_ID) REFERENCES GEO_REGION (GEO_REGION_ID) -- References GEO_REGION table

);
