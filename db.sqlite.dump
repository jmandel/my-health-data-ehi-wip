CREATE TABLE EPISODE ( -- This table contains high-level information on the episodes recorded in the clinical system for your patients. When a provider sees a patient several times for an ongoing condition, such as prenatal care, these encounters can be linked to a single Episode of Care. It does not contain episodes linked to an inpatient encounter.-- Child table: ALL_EPISODE_CSN_LINKS (2 to 2 rows) joined on EPISODE_ID
  -- Child table: EPISODE_OT (1 to 1 rows) joined on SUMMARY_BLOCK_ID

  EPISODE_ID NUMERIC, -- The unique ID of the episode of care record.
  NAME TEXT, -- The name of the episode.
  SUM_BLK_TYPE_ID NUMERIC, -- The episode type.
  START_DATE TEXT, -- The date the episode was initiated.
  END_DATE TEXT, -- The date the episode was resolved in calendar format. This field is called "Resolved" on the clinical system screen.
  STATUS_C_NAME TEXT, -- The status category number for the episode.
  L_UPDATE_USER_ID TEXT, -- The ID of the last user that updated the episode of care record.
  L_UPDATE_USER_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  SERV_AREA_ID NUMERIC, -- The unique ID of the episode's service area. This column is used for DBC episodes, which are specific to a service area.
  EPISODE_NAME TEXT, -- The name of the episode.
  PAT_ID TEXT, -- The unique ID of the patient associated with the episode.
  PRIMARY KEY (EPISODE_ID) -- Primary Key, FOREIGN KEY (EPISODE_ID) REFERENCES CARE_PATH (SUMMARY_BLOCK_ID) -- References CARE_PATH table
, FOREIGN KEY (SUM_BLK_TYPE_ID) REFERENCES EPISODE_DEF (EPISODE_DEF_ID) -- References EPISODE_DEF table
, FOREIGN KEY (L_UPDATE_USER_ID) REFERENCES CLARITY_EMP (USER_ID) -- References CLARITY_EMP table
, FOREIGN KEY (SERV_AREA_ID) REFERENCES CLARITY_SA (SERV_AREA_ID) -- References CLARITY_SA table
, FOREIGN KEY (SERV_AREA_ID) REFERENCES CLARITY_LOC (LOC_ID) -- References CLARITY_LOC table
, FOREIGN KEY (PAT_ID) REFERENCES PATIENT (PAT_ID) -- References PATIENT table

);
