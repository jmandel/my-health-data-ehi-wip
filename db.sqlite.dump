  MESSAGE_ID TEXT, -- The unique ID used to identify a web based chart system message record.
  LINE INTEGER, -- The line number used to identify each row of read data associated with an individual web based chart system message record.
  QUESR_ANS_ID TEXT, -- This stores the IDs of the questionnaire answers associated with this message.
  PRIMARY KEY (MESSAGE_ID, LINE) -- Primary Key, FOREIGN KEY (MESSAGE_ID) REFERENCES MYC_MESG (MESSAGE_ID)
, FOREIGN KEY (MESSAGE_ID) REFERENCES MYC_MESG (MESSAGE_ID) -- References MYC_MESG table
, FOREIGN KEY (QUESR_ANS_ID) REFERENCES CL_QANSWER (ANSWER_ID) -- References CL_QANSWER table

);
