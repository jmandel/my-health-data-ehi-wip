CREATE TABLE MED_CVG_ESTIMATE_VALS ( -- This table holds information about values sent to the payer when generating a medication estimate.-- Child table: MED_CVG_ALTERNATIVES (1 to 1 rows) joined on MED_ESTIMATE_ID, CONTACT_DATE_REAL
