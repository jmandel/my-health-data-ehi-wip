CREATE TABLE ORDER_STATUS ( -- The ORDER_STATUS table contains overtime single response orders information.-- Child table: OBS_MTHD_ID (1 to 12 rows) joined on ORDER_ID, CONTACT_DATE_REAL
  -- Child table: ORDER_DOCUMENTS (1 to 1 rows) joined on ORDER_ID, CONTACT_DATE_REAL
  -- Child table: ORDER_IMPRESSION (2 to 2 rows) joined on ORDER_PROC_ID, ORD_DATE_REAL
  -- Child table: ORDER_NARRATIVE (10 to 15 rows) joined on ORDER_PROC_ID, ORD_DATE_REAL
  -- Child table: ORDER_READ_ACK (1 to 3 rows) joined on ORDER_ID, CONTACT_DATE_REAL
  -- Child table: ORDER_RES_COMMENT (2 to 8 rows) joined on ORDER_ID, CONTACT_DATE_REAL
  -- Child table: ORDER_RESULTS (1 to 12 rows) joined on ORDER_PROC_ID, ORD_DATE_REAL
  -- Child table: ORD_RSLT_COMPON_ID (1 to 1 rows) joined on ORDER_ID, CONTACT_DATE_REAL

  ORDER_ID NUMERIC, -- Unique ID for this order record
  ORD_DATE_REAL NUMERIC, -- This is a numeric representation of the date of this encounter in your system. The integer portion of the number specifies the date of the encounter. The digits after the decimal point indicate multiple visits on one day.
  CONTACT_DATE TEXT, -- The date of this contact in calendar format.
  CONTACT_NUMBER INTEGER, -- The contact number of the orders record.
  CONTACT_TYPE_C_NAME TEXT, -- The category value for the contact type, such as "1" for "Ordered" or "2" for "Resulted"
  ABNORMAL_YN TEXT, -- This Y/N flag reports Y if a result component in the order is reported as abnormal or N if the order is normal.
  ORDER_CREATOR_ID TEXT, -- The unique ID of the person creating the order.
  ORDER_CREATOR_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  RESULTING_LAB_ID NUMERIC, -- The unique ID of the lab running the test.
  RESULTING_LAB_ID_LLB_NAME TEXT, -- Interface laboratory name.
  INSTANT_OF_ENTRY TEXT, -- The instant the record was last entered.
  ROUTING_OUTCOME_C_NAME TEXT, -- The category value of the outcome of results routing for each resulting contact of order associated with this row.
  ROUTING_SCHEME_LINE TEXT, -- The line of the results routing scheme that was executed to determine recipients for this result. If no line was executed, the value of the column will be the string "DEFAULT".
  ROUTING_INST_TM TEXT, -- The date and time the order was resulted and routed.
  ROUTING_USER_ID TEXT, -- The unique ID of the user the result was routed to for this row.
  ROUTING_USER_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  ROUTING_CURSTATUS_C_NAME TEXT, -- This item stores the current routing status for a resulting contact.
  LAB_STATUS_C_NAME TEXT, -- Indicates the lab status value associated with each order contact.
  RSLT_CNCT_INSTANT_DTTM TEXT, -- The instant in which a result contact is modified/filed to the system. Not to be confused with Result Date/Time, which is when the result was actually generated.
  RSLT_CNCT_USER_ID TEXT, -- The user filing the result contact. For interfaces or Beaker Result Filing background job, this might be a generic user.
  RSLT_CNCT_USER_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  RSLT_CNCT_SOURCE_C_NAME TEXT, -- Stores which entry point modified the result contact.
  RESULT_DTTM TEXT, -- The date and time the technician ran the tests for each order in calendar format. NOTE: Concatenates the result date (ORD 26) and result time (ORD 28) into a datetime format. If the time value is null, the query will return 12:00 AM for a time.
  NARRATIVE_PERF_ORG_INFO INTEGER, -- This item stores the line number of the performing organization related group (ORD 1220) and acts as a pointer to the performing organization information of narrative of the result.
  IMPRESSION_PERF_ORG_INFO INTEGER, -- This item stores the line number of the preforming organization related group (ORD 1220) and acts as a pointer to the performing organization information of impression of the result.
  PRIMARY KEY (ORDER_ID, ORD_DATE_REAL) -- Primary Key, FOREIGN KEY (ORDER_ID) REFERENCES ORD_PRFLST_TRK (ORDER_ID)
, FOREIGN KEY (ORDER_ID) REFERENCES ORD_PRFLST_TRK (ORDER_ID) -- References ORD_PRFLST_TRK table
, FOREIGN KEY (ORDER_CREATOR_ID) REFERENCES CLARITY_EMP (USER_ID) -- References CLARITY_EMP table

);
