CREATE TABLE DOC_INFORMATION ( -- The DOC_INFORMATION table contains information about documents, including scanned and electronically signed documents.-- Child table: DOC_CSN_REFS (1 to 1 rows) joined on DOCUMENT_ID
  -- Child table: DOC_LINKED_PAT_CSNS (1 to 1 rows) joined on DOCUMENT_ID
  -- Child table: DOC_LINKED_PATS (1 to 1 rows) joined on DOCUMENT_ID

  DOC_INFO_ID TEXT, -- The unique ID of the document information record.
  DOC_INFO_TYPE_C_NAME TEXT, -- The type of document described by this document information.
  DOC_STAT_C_NAME TEXT, -- The current status of the document described by this document information.
  DOC_DESCR TEXT, -- A short free text description of the document described by this document information.
  DOC_RECV_TIME TEXT, -- The date and time the document described by this document information was received.
  RECV_BY_USER_ID TEXT, -- The employee who received the document described by this document information. This ID may be encrypted.
  RECV_BY_USER_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  IS_SCANNED_YN TEXT, -- Specifies whether there is a scanned image version of the document described by this document information.
  SCAN_TIME TEXT, -- The date and time the document described by this document information was scanned.
  SCAN_BY_USER_ID TEXT, -- The employee who scanned the document described by this document information. This ID may be encrypted.
  SCAN_BY_USER_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  SCAN_DEP_ID NUMERIC, -- The department where the document described by this document information was scanned.
  SCAN_FILE TEXT, -- The file name of the scanned image version of the document described by this document information.
  DOC_PT_ID TEXT, -- The unique ID of the patient associated with the document record.
  DOC_CSN NUMERIC, -- This stores the contact serial number of the encounter that this record is attached to, if applicable.
  DOC_HNO_ID TEXT, -- The unique ID of the note record associated with this document.
  SCAN_LWS_ID TEXT, -- This item stores the id of the workstation where the document was scanned.
  DOC_SRVC_DTTM TEXT, -- The service date and time of the document.
  SCAN_INST_DTTM TEXT, -- This item stores the most clinically relevant date for the document. From top to bottom, it looks to the service date/time (I DCS 246/247), the order prioritized instant (I ORD 24), the encounter date, and the import date/time (I DCS 310/315).
  DOC_STORAGE_LVL_C_NAME TEXT, -- The level at which the document is attached to the patient's record. For example, the document could be stored at the patient, encounter, or hospital account level.
  DOC_CREAT_DEPT_ID NUMERIC, -- The department where this document was created.
  DOC_SRVR_NAME_C_NAME TEXT, -- This item stores the desktop integration (FDI) record that was used for scanning the document.
  DOC_SOURCE_INFO_C_NAME TEXT, -- Determines the source of the document rather than the document's type.
  DOCUMENT_ID TEXT, -- The unique identifier (.1 item) for the document record.
  BLOB_CATEGORY_C_NAME TEXT, -- The blob category for the document.
  FILE_CREATION_TIME TEXT, -- Stores the timestamp in HL7 format of when the file was created on the blob or DMS server.
  FILE_LAST_UPD_TIME TEXT, -- Timestamp in HL7 format of when the image was last updated.
  FILE_TYPE TEXT, -- Mime type of the image/document.
  PRIMARY KEY (DOC_INFO_ID) -- Primary Key, FOREIGN KEY (RECV_BY_USER_ID) REFERENCES CLARITY_EMP (USER_ID) -- References CLARITY_EMP table
, FOREIGN KEY (DOC_PT_ID) REFERENCES PATIENT (PAT_ID) -- References PATIENT table

);
