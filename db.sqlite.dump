CREATE TABLE HSP_ACCOUNT ( -- This table contains hospital account information from the Hospital Account (HAR) and Claim (CLM) master files. It will exclude professional billing hospital accounts created by visit filing order in non-single billing office service areas.-- Child table: CODE_INT_COMB_LN (3 to 3 rows) joined on HSP_ACCOUNT_ID
  -- Child table: DOCS_FOR_HOSP_ACCT (1 to 1 rows) joined on ACCT_ID
  -- Child table: HSP_ACCT_ADJ_LIST (5 to 5 rows) joined on HSP_ACCOUNT_ID
  -- Child table: HSP_ACCT_ADMIT_DX (1 to 1 rows) joined on HSP_ACCOUNT_ID
  -- Child table: HSP_ACCT_ATND_PROV (1 to 1 rows) joined on HSP_ACCOUNT_ID
  -- Child table: HSP_ACCT_CHG_LIST (3 to 3 rows) joined on HSP_ACCOUNT_ID
  -- Child table: HSP_ACCT_CL_AG_HIS (2 to 2 rows) joined on HSP_ACCOUNT_ID
  -- Child table: HSP_ACCT_DX_LIST (2 to 2 rows) joined on HSP_ACCOUNT_ID
  -- Child table: HSP_ACCT_EARSTADDR (1 to 1 rows) joined on ACCT_ID
  -- Child table: HSP_ACCT_EXTINJ_CD (1 to 1 rows) joined on HSP_ACCOUNT_ID
  -- Child table: HSP_ACCT_OCUR_HAR (1 to 1 rows) joined on ACCT_ID
  -- Child table: HSP_ACCT_PYMT_LIST (2 to 2 rows) joined on HSP_ACCOUNT_ID

  HSP_ACCOUNT_ID NUMERIC, -- This column stores the unique identifier for the hospital account.
  HSP_ACCOUNT_NAME TEXT, -- The name of the patient associated with the hospital account.
  ACCT_CLASS_HA_C_NAME TEXT, -- The hospital account's account class.
  ACCT_FIN_CLASS_C_NAME TEXT, -- The hospital account's financial class.
  ACCT_SLFPYST_HA_C_NAME TEXT, -- The hospital account's self-pay status.
  ACCT_BILLSTS_HA_C_NAME TEXT, -- This column stores the status of the hospital account. If this is a professional billing default hospital account, this column always returns 4 (Billed).
  ACCT_ZERO_BAL_DT TEXT, -- This column stores the date the hospital account went to a zero balance. This may be empty for older accounts, as Zero Balance Date (I HAR 244) had not always been available.
  ADM_DATE_TIME TEXT, -- This column stores the admission date and time associated with the hospital account. The admission date and time (I HAR 400/405) are first pulled from the coding information on the hospital account. If this data is not stored on the hospital account yet, then this will pull the data from the primary patient encounter for the hospital account. Depending on a hospital billing system definition setting, this will pull either the admission date and time (I EPT 18850/18851) or the arrival date and time (I EPT 10820/10815). If the arrival date and time is not available than the admission date and time will be used.
  ADM_DEPARMENT_ID NUMERIC, -- The department of the account's admission event.
  ADM_LOC_ID NUMERIC, -- The location of the account's admission event.
  ATTENDING_PROV_ID TEXT, -- This column stores the unique identifier for the attending provider stored on the hospital account. See column PROV_ID in table HSP_ATND_PROV if coding has not been performed on this hospital account. Alternatively, see V_ARHB_HSP_ACCOUNT_ADDL_INFO for the calculated value for this column based on both hospital account and encounter data. This view should be used to maintain backwards compatibility with reports created before upgrading to the Summer 2009 release.
  COMPLETION_DT_TM TEXT, -- The date and time when abstracting was completed for the hospital account.
  CVG_LIST_SELECT_YN TEXT, -- Denotes whether coverages have been put on the hospital account.
  DISCH_DATE_TIME TEXT, -- This column stores the discharge date and time associated with the hospital account. The discharge date and time (I HAR 425/430) are first pulled from the coding information on the hospital account. If this data is not stored on the hospital account yet, then this will pull the discharge date and time (I EPT 18855/18856) that is stored on the primary patient encounter for the hospital account.
  DISCH_DEPT_ID NUMERIC, -- This column stores the unique identifier for the discharge department on the hospital account.
  DISCH_LOC_ID NUMERIC, -- The discharge location stored in the hospital account.
  GUAR_ADDR_1 TEXT, -- The first line of the street address of the guarantor for the hospital account at time of discharge.
  GUAR_NAME TEXT, -- The name of the guarantor for the hospital account at time of discharge.
  GUAR_ZIP TEXT, -- The ZIP Code of the guarantor for the hospital account at time of discharge.
  PAT_CITY TEXT, -- The city portion of the address of the patient for the hospital account at time of discharge.
  PAT_DOB TEXT, -- The date of birth of the patient for the hospital account at time of discharge.
  PAT_HOME_PHONE TEXT, -- The home phone number of the patient for the hospital account at time of discharge.
  PAT_SSN TEXT, -- The social security number of the patient for the hospital account at time of discharge.
  PAT_ZIP TEXT, -- The ZIP Code of the patient for the hospital account at time of discharge.
  PREBILL_BUCKET_ID NUMERIC, -- This column stores the unique identifier for the hospital account's prebilled bucket.
  SELF_PAY_BUCKET_ID NUMERIC, -- This column stores the unique identifier for the hospital account's self-pay bucket.
  SERV_AREA_ID NUMERIC, -- This column stores the unique identifier for the service area stored on the hospital account.
  TOT_ADJ NUMERIC, -- The total of all adjustments on the hospital account.
  TOT_CHGS NUMERIC, -- The total of all charges on the hospital account.
  UNDISTRB_BUCKET_ID NUMERIC, -- This column stores the unique identifier for a hospital account's undistributed bucket.
  PATIENT_STATUS_C_NAME TEXT, -- The patient status (discharge disposition) category ID for the hospital account.
  ADMISSION_SOURCE_C_NAME TEXT, -- The point of origin category ID (admission source) for the hospital account.
  ADMISSION_TYPE_C_NAME TEXT, -- The admission type category ID for the hospital account.
  PRIMARY_PAYOR_ID NUMERIC, -- This column stores the unique identifier for the primary payer associated with the hospital account.
  PRIMARY_PLAN_ID NUMERIC, -- This column stores the unique identifier for the primary benefit plan associated with the hospital account.
  NUM_OF_CHARGES INTEGER, -- The total number of charge transactions posted to the hospital account. This number may include inactive charges, such as reversals and reversed charges.
  CODING_STATUS_C_NAME TEXT, -- The coding status category ID for the hospital account.
  CODING_STS_USER_ID TEXT, -- This column stores the unique identifier for the user when the coding status for the hospital account last changed.
  CODING_STS_USER_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  CODING_DATETIME TEXT, -- The date and time that the coding status for the hospital account was last changed.
  ABSTRACT_USER_ID TEXT, -- This column stores the unique identifier for the user who last changed the abstracting status of the hospital account.
  ABSTRACT_USER_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  FIRST_BILLED_DATE TEXT, -- This column stores the date when the account moved to the billed state for the first time.
  BASE_INV_NUM TEXT, -- The base invoice number for this row.
  INV_NUM_SEQ_CTR INTEGER, -- The invoice number sequence counter for the hospital account.
  SBO_TOT_BALANCE NUMERIC, -- Contains the combined total balance of Hospital Billing (HB)�and Professional Billing (PB) transactions on the hospital account, including charges, payments and adjustments in all statuses (single billing office (SBO)�mode only).
  SBO_TOTAL_CHARGES NUMERIC, -- Combined Hospital Billing (HB)�and Professional Billing (PB) charges on the hospital account in all statuses (single billing office (SBO)�mode only).
  SBO_TOTAL_PAYMENTS NUMERIC, -- Combined Hospital Billing (HB)�and Professional Billing (PB) payments on the hospital account in all statuses (single billing office (SBO)�mode only).
  SBO_TOTAL_ADJ NUMERIC, -- Contains combined Hospital Billing (HB)�and Professional Billing (PB) adjustments on the hospital account in all statuses (single billing office (SBO)�mode only).
  SBO_PREBILL_BALANC NUMERIC, -- Contains combined Hospital Billing (HB)�and Professional Billing (PB) prebilled balances including charges, payments and adjustments (single billing office (SBO)�mode only).
  SBO_INS_BALANCE NUMERIC, -- Contains combined Hospital Billing (HB)�and Professional Billing (PB) insurance balances including charges, payments and adjustments (single billing office (SBO)�mode only).
  SBO_SP_BAL NUMERIC, -- Contains combined Hospital Billing (HB)�and Professional Billing (PB) self pay balances including charges, payments and adjustments (single billing office (SBO)�mode only).
  OPEN_DENIAL_BDC_YN TEXT, -- This column stores whether the hospital account has an open denial record.
  OPEN_RMK_BDC_YN TEXT, -- This column stores whether the hospital account has an open remark record.
  OPEN_COR_BDC_YN TEXT, -- This column stores whether the hospital account has an open correspondence record.
  REC_CREATE_USER_ID TEXT, -- This column stores the unique identifier for the user who created the hospital account.
  REC_CREATE_USER_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  CODING_USER TEXT, -- This column stores the unique identifier for the user who last changed the coding status of the hospital account. This is frequently used to join to the CLARITY_EMP table.
  CODING_USER_NAME TEXT, -- The name of the user record. This name may be hidden.
  FIRST_SELF_PAY_DT TEXT, -- This field contains the date of the first self-pay balance for this account.
  FIRST_FULL_SP_DT TEXT, -- This field contains the first date when the self-pay balance equaled the full account balance for this account.
  PRIMARY_CONTACT TEXT, -- This column stores the primary contact date (DAT) of the hospital account. It's used in many places, such as in determining the admission/discharge date of the account.
  ADMIT_TYPE_EPT_C_NAME TEXT, -- Admit type as stored on patient record.
  PAT_STS_EPT_C_NAME TEXT, -- Patient Status as stored on patient record.
  HAS_OPEN_OVRP_BD_YN TEXT, -- Indicates whether the hospital account has open overpayment records.
  SELF_PAY_YN TEXT, -- Indicates whether this hospital account is self-pay.
  ACTUAL_COPAY_AMT NUMERIC, -- For all insurance buckets of the last coverage in the filing order, the total copay specified by insurance on payments.
  ACTUAL_COINS_AMT NUMERIC, -- For all insurance buckets of the last coverage in the filing order, the total coinsurance specified by insurance on payments.
  ACTUAL_DED_AMT NUMERIC, -- For all insurance buckets of the last coverage in the filing order, the total deductible specified by insurance on payments.
  TOT_INS_PMT NUMERIC, -- The total amount of insurance payments posted to this account, not including refunds.
  TOT_INS_ADJ NUMERIC, -- The total amount of insurance adjustments posted to this account, not including refunds.
  TOT_SP_PMT NUMERIC, -- The total amount of self-pay payments posted to this account, not including refunds.
  TOT_AR_INS_PMT NUMERIC, -- The total amount of active AR insurance payments posted to this account, not including refunds.
  TOT_AR_INS_ALLOWANCES NUMERIC, -- The total amount of active AR insurance allowance adjustments posted to this account.
  TOT_AR_SP_PMT NUMERIC, -- The total amount of active AR self-pay payments posted to this account, not including refunds.
  TOT_INS_PMTS_AND_RFNDS NUMERIC, -- The total insurance payment amount posted to the account, less any refunded amount.
  TOT_SP_PMTS_AND_RFNDS NUMERIC, -- The total self-pay payment amount posted to the account, less any refunded amount.
  TOT_PMTS_AND_RFNDS NUMERIC, -- The total payment amount posted to the account, less any refunded amount.
  TOT_ADJ_EXCL_RFNDS NUMERIC, -- The total adjustment amount posted to the account, not including refund adjustments.
  SP_RESP_AFTER_INS NUMERIC, -- The balance transferred to self-pay after insurance is taken into account. This does not include any self-pay discount, financial assistance, or pre-payments.
  SP_RESP_LESS_DISCOUNT NUMERIC, -- The self-pay responsibility after any self-pay or financial assistance discounts are posted on the self-pay bucket.
  BAL_IN_FULL_SELF_PAY_YN TEXT, -- This column indicates whether the balances for this hospital account are entirely in self-pay. 'Y' indicates that all balances are in self-pay. 'N' or NULL indicates that there are some balances not in self-pay or the account has not been billed.
  FIRST_TX_POST_DATE TEXT, -- This column is the original post date for the very first transaction that filed to this hospital account, even if that transaction has been reposted, transferred, or reversed.
  PRIMARY KEY (HSP_ACCOUNT_ID) -- Primary Key, FOREIGN KEY (HSP_ACCOUNT_ID) REFERENCES HAR_ALL (ACCT_ID) -- References HAR_ALL table
, FOREIGN KEY (HSP_ACCOUNT_ID) REFERENCES HSP_ACCT_CLAIM_HAR (ACCT_ID) -- References HSP_ACCT_CLAIM_HAR table
, FOREIGN KEY (PREBILL_BUCKET_ID) REFERENCES HSP_BKT_ADDTL_REC (BUCKET_ID) -- References HSP_BKT_ADDTL_REC table
, FOREIGN KEY (SELF_PAY_BUCKET_ID) REFERENCES HSP_BKT_ADDTL_REC (BUCKET_ID) -- References HSP_BKT_ADDTL_REC table
, FOREIGN KEY (SERV_AREA_ID) REFERENCES CLARITY_SA (SERV_AREA_ID) -- References CLARITY_SA table
, FOREIGN KEY (SERV_AREA_ID) REFERENCES CLARITY_LOC (LOC_ID) -- References CLARITY_LOC table
, FOREIGN KEY (UNDISTRB_BUCKET_ID) REFERENCES HSP_BKT_ADDTL_REC (BUCKET_ID) -- References HSP_BKT_ADDTL_REC table
, FOREIGN KEY (REC_CREATE_USER_ID) REFERENCES CLARITY_EMP (USER_ID) -- References CLARITY_EMP table

);
