CREATE TABLE IMM_ADMIN ( -- The IMM_ADMIN table contains information about the immunization administered. The rows included in this table are items from DXR (Document) masterfile which include information on type of immunization, administration date, administered dose, administration route, administration site, immunization manufacturer, immunization lot number, administered by, visit date, deferral reason, administration notes, administration location, administration status, administered amount, administered unit, contact serial number of the DXR record that owns the immunization instance and a unique reference identifier to identify a specific instance of an immunization.-- Child table: IMM_ADMIN_GROUPS_FT (1 to 1 rows) joined on DOCUMENT_ID, CONTACT_DATE_REAL, GROUP_LINE
  -- Child table: IMM_ADMIN_GROUPS (1 to 1 rows) joined on DOCUMENT_ID, CONTACT_DATE_REAL, GROUP_LINE

  DOCUMENT_ID NUMERIC, -- The unique identifier for the document record.
  CONTACT_DATE_REAL NUMERIC, -- A unique contact date in decimal format. The integer portion of the number indicates the date of contact. The digits after the decimal distinguish different contacts on the same date and are unique for each contact on that date. For example, .00 is the first/only contact, .01 is the second contact, etc.
  LINE INTEGER, -- The line number for the information associated with this contact. Multiple pieces of information can be associated with this contact.
  CONTACT_DATE TEXT, -- The date of this contact in calendar format.
  IMM_TYPE_ID NUMERIC, -- External immunization type ID.
  IMM_TYPE_ID_NAME TEXT, -- The name of the immunization.
  IMM_TYPE_FREE_TEXT TEXT, -- The immunization type information for the administered immunization as free text.
  IMM_DATE TEXT, -- The immunization administration date.
  IMM_ROUTE_C_NAME TEXT, -- The immunization administration route category ID.
  IMM_SITE_C_NAME TEXT, -- The immunization administration site category ID.
  IMM_MANUFACTURER_C_NAME TEXT, -- The immunization administered manufacturer category ID.
  IMM_LOT_NUMBER TEXT, -- The immunization administered lot number.
  IMM_GIVEN_BY_FT TEXT, -- The immunization given by information for the administered immunization as free text.
  IMM_NOTES_RAW_DATA TEXT, -- Free text immunization notes from the immunization administration.
  IMM_NOTES TEXT, -- The immunization administration notes.
  IMM_LOCATION TEXT, -- The immunization administration location.
  IMM_STATUS_C_NAME TEXT, -- Status of the vaccination administration.
  IMM_REFERENCE_ID TEXT, -- This item stores a unique reference identifier to identify a specific instance of an immunization.
  PRIMARY KEY (DOCUMENT_ID, CONTACT_DATE_REAL, LINE) -- Primary Key, FOREIGN KEY (IMM_TYPE_ID) REFERENCES CLARITY_IMMUNZATN (IMMUNZATN_ID) -- References CLARITY_IMMUNZATN table

);
