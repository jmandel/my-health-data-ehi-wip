CREATE TABLE ARPB_VISITS ( -- This table contains Professional Billing visit information stored in the Hospital Accounts Receivable (HAR) master file. It doesn�t include HAR records created for Hospital Billing and Single Billing Office.-- Child table: HSP_ACCT_OTHR_PROV (1 to 2 rows) joined on HSP_ACCOUNT_ID

  PB_VISIT_ID NUMERIC, -- The unique identifier for the Professional Billing visit.
  PB_BILLING_STATUS_C_NAME TEXT, -- This column stores the Professional Billing status category ID for the visit.
  PB_FO_OVRRD_ST_C_NAME TEXT, -- This column indicates whether the Professional Billing filing order has been overridden by a user.
  PB_FO_MSPQ_STATE_C_NAME TEXT, -- This column indicates whether the filing order for the Professional Billing visit has been verified by Medicare Secondary Payer Questionnaire logic.
  PB_VISIT_NUM TEXT, -- This column stores the PB visit number.
  PRIM_ENC_CSN_ID NUMERIC, -- The contact serial number associated with the primary patient contact on the Professional Billing visit.
  GUARANTOR_ID NUMERIC, -- Stores the guarantor ID associated with the Professional Billing visit.
  COVERAGE_ID NUMERIC, -- The primary coverage on the Professional Billing visit.
  SELF_PAY_YN TEXT, -- Indicates whether the Professional Billing visit is self-pay.
  DO_NOT_BILL_INS_YN TEXT, -- Indicates�whether the Professional Billing visit has the Do Not Bill Insurance flag set.
  ACCT_FIN_CLASS_C_NAME TEXT, -- The financial class category ID�for the Professional Billing visit.
  SERV_AREA_ID NUMERIC, -- The service area of the Professional Billing visit.
  REVENUE_LOCATION_ID NUMERIC, -- The revenue location of the Professional Billing visit.
  DEPARTMENT_ID NUMERIC, -- The department of the Professional Billing visit.
  PB_TOTAL_BALANCE NUMERIC, -- Contains the combined total balance of transactions on the PB visit.
  PB_TOTAL_CHARGES NUMERIC, -- The total charges on the PB visit.
  PB_TOTAL_PAYMENTS NUMERIC, -- The total payments on the PB visit.
  PB_TOTAL_ADJ NUMERIC, -- Contains total adjustments on the PB visit.
  PB_INS_BALANCE NUMERIC, -- Contains insurance balance on the PB visit.
  PB_SELFPAY_BALANCE NUMERIC, -- Contains the self-pay balance on the Professional Billing visit.
  REC_CREATE_USER_ID TEXT, -- The user who created the Professional Billing visit record.
  REC_CREATE_USER_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  FIRST_PB_CHG_TX_ID NUMERIC, -- Contains the first valid Professional Billing charge on the Professional Billing visit.
  BAL_FULL_SELF_PAY_YN TEXT, -- This item shows whether the balances for this hospital account are in full self-pay.
  PRIMARY KEY (PB_VISIT_ID) -- Primary Key, FOREIGN KEY (PB_VISIT_ID) REFERENCES HAR_ALL (ACCT_ID) -- References HAR_ALL table
, FOREIGN KEY (PRIM_ENC_CSN_ID) REFERENCES PAT_ENC (PAT_ENC_CSN_ID) -- References PAT_ENC table
, FOREIGN KEY (SERV_AREA_ID) REFERENCES CLARITY_SA (SERV_AREA_ID) -- References CLARITY_SA table
, FOREIGN KEY (SERV_AREA_ID) REFERENCES CLARITY_LOC (LOC_ID) -- References CLARITY_LOC table
, FOREIGN KEY (REC_CREATE_USER_ID) REFERENCES CLARITY_EMP (USER_ID) -- References CLARITY_EMP table
, FOREIGN KEY (FIRST_PB_CHG_TX_ID) REFERENCES ARPB_TRANSACTIONS (TX_ID) -- References ARPB_TRANSACTIONS table

);
