CREATE TABLE RECONCILE_CLM ( -- This table contains basic information for claim reconciliation records.-- Child table: RECONCILE_CLAIM_STATUS (3 to 10 rows) joined on CLAIM_RECON_ID
  -- Child table: RECONCILE_CLM_OT (3 to 13 rows) joined on CLAIM_REC_ID

  CLAIM_REC_ID TEXT, -- The unique ID of the claim reconciliation record.
  CLAIM_INVOICE_NUM TEXT, -- The invoice number of the claim.
  CUR_EPIC_STATUS_C_NAME TEXT, -- This is the system status of the claim: opened or closed.
  PAYOR_ID NUMERIC, -- This is the payer ID for the payer on the claim.
  BENEFIT_PLAN_ID NUMERIC, -- This is the benefit plan ID for the benefit plan on the claim.
  LOC_ID NUMERIC, -- This is the location ID for the location of the claim.
  DEPARTMENT_ID NUMERIC, -- This is the department ID for the department of the claim.
  BUCKET_ID NUMERIC, -- The hospital liability bucket ID for the claim. This column is populated only for Hospital Billing claims.
  HSP_ACCOUNT_ID NUMERIC, -- The hospital account ID for the claim. This column is populated only for Hospital Billing claims.
  SERVICE_AREA_ID NUMERIC, -- This column stores the service area ID from the claim that created the reconciliation record.
  EPIC_CLM_STS_C_NAME TEXT, -- This column stores the status of the claim that created the reconciliation record. This column will indicate whether the claim has been accepted, processed, or rejected in Epic.
  CLAIM_RUN_ID TEXT, -- This column stores the claim run ID for the reconciliation record. This column will be populated for records created in the 2010 version and beyond.
  TOTAL_BILLED NUMERIC, -- This column stores the billed amount for the reconciliation record.
  EXT_STS_RCV_YN TEXT, -- This column indicates whether at least one external status update has been received for the reconciliation record.
  CROSSOVER_CLAIM_YN TEXT, -- Indicates whether the associated claim is a crossover claim.
  RECORD_TYPE_C_NAME TEXT, -- This item indicates whether the record is Interchange Control Header (ISA), Transaction Set Header (ST), or Claim data.
  POST_ACK_STATUS_YN TEXT, -- This item indicates whether at least one external status update has been received on or after either four days post claim acceptance or the predicted payment response date if earlier. Claim acknowledgements are generally posted before then.
  CLAIM_CLOSED_DATE TEXT, -- This item holds the contact date the CRD was marked closed.
  PRIMARY KEY (CLAIM_REC_ID) -- Primary Key, FOREIGN KEY (PAYOR_ID) REFERENCES CLARITY_EPM (PAYOR_ID) -- References CLARITY_EPM table
, FOREIGN KEY (BENEFIT_PLAN_ID) REFERENCES CLARITY_EPP (BENEFIT_PLAN_ID) -- References CLARITY_EPP table
, FOREIGN KEY (LOC_ID) REFERENCES CLARITY_LOC (LOC_ID) -- References CLARITY_LOC table
, FOREIGN KEY (SERVICE_AREA_ID) REFERENCES CLARITY_SA (SERV_AREA_ID) -- References CLARITY_SA table
, FOREIGN KEY (SERVICE_AREA_ID) REFERENCES CLARITY_LOC (LOC_ID) -- References CLARITY_LOC table

);
