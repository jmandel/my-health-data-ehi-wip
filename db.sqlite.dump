CREATE TABLE PAT_ADDR_CHNG_HX ( -- This table keeps track of changes in the patient's address.-- Child table: PAT_REL_ADDR (1 to 1 rows) joined on PAT_ID, GROUP_LINE

  PAT_ID TEXT, -- Patient ID for whom address is changed.
