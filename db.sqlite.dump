CREATE TABLE TX_NDC_INFORMATION ( -- This table contains Medical National Drug Code (NDC) information. Since one transaction may be associated with multiple NDC codes, each row in this table represents one NDC code and is identified by the transaction ID and line number. The data for this table is extracted using a KB_SQL query.
  TX_ID NUMERIC, -- The unique accounts receivable transaction record ID.
  LINE INTEGER, -- Line number to identify each row of�Medical National Drug Code (NDC) data associated with an individual transaction.
  NDC_CODES_ID TEXT, -- The ID of the Medical National Drug Code (NDC) associated with a transaction.
  NDC_CODES_ID_NDC_CODE TEXT, -- The external code for the National Drug Code (NDC). An NDC represents packages of medications.
  NDC_CODES_ADMIN_AMT NUMERIC, -- The NDC amount associated with this charge.
  NDC_CODES_UNIT_C_NAME TEXT, -- The Medical National Drug Code (NDC) unit associated with the charge.
  PRIMARY KEY (TX_ID, LINE) -- Primary Key, FOREIGN KEY (TX_ID) REFERENCES ARPB_TRANSACTIONS (TX_ID)
, FOREIGN KEY (TX_ID) REFERENCES ARPB_TRANSACTIONS (TX_ID) -- References ARPB_TRANSACTIONS table
, FOREIGN KEY (NDC_CODES_ID) REFERENCES RX_NDC (NDC_ID) -- References RX_NDC table

);
