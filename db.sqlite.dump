CREATE TABLE INV_BASIC_INFO ( -- This table contains basic invoice information. Each column in this table is from the INV 100 related group, and each line in the table corresponds to a claim that was sent for this invoice (INV) record.
  INV_ID NUMERIC, -- The invoice ID.
  LINE INTEGER, -- The line number for the invoice number associated with the invoice record. Multiple invoice numbers can be associated with a single invoice record.
  INV_NUM TEXT, -- The specific invoice number for the bill or claim. Subsequent invoice numbers may be secondary claims or primary claims that were resubmitted to the same payer.
  INV_STATUS_C_NAME TEXT, -- The status for the invoice number.
  CVG_ID NUMERIC, -- The coverage record ID.
  EPM_ID NUMERIC, -- The payer ID.
  EPP_ID NUMERIC, -- The benefit plan ID.
  FROM_SVC_DATE TEXT, -- The from (minimum) service date for the invoice number. This date is determined from the transaction on the invoice with the service date furthest in the past.
  TO_SVC_DATE TEXT, -- The to (maximum) service date for the invoice number. This date is determined from the transaction on the invoice with the most recent service date.
  INV_TYPE_C_NAME TEXT, -- The claim type for the invoice number. This column identifies whether the invoice number is a bill/statement or a claim.
  CROSS_OVER_YN TEXT, -- Indicates whether the invoice number is a crossover claim. This usually only applies to secondary claims and indicates that although the claim associated with the invoice was created, it was suppressed from a claim run.
  MAILING_NAME TEXT, -- The bill/statement or claim mailing name for the invoice number.
  MAILING_ADDR TEXT, -- The bill/statement or claim mailing street address for the invoice number.
  CITY_STATE_ZIP TEXT, -- The bill/statement or claim mailing city, state, and ZIP Code for the invoice number.
  CLM_ID NUMERIC, -- The claim information record ID.
  REF_ID TEXT, -- The referral ID.
  REF_ID_REFERRING_PROV_NAM TEXT, -- The name of the referral source.
  EAF_POS_ID NUMERIC, -- The place of service ID.
  TAX_ID_NUM TEXT, -- The tax ID/IRS number for the invoice number.
  TAX_ID_TYPE TEXT, -- The tax ID/IRS number type for the invoice number.
  REPLACED_INV TEXT, -- Contains a list of all the replaced invoice numbers associated with the invoice record.
  CLM_CHANGE_RSN_COD TEXT, -- Contains a list of all the claim change reason codes associated with the invoice record.
  CLM_CHANGE_COMMENT TEXT, -- Contains a list of all the claim change comments associated with the invoice record.
  MAIL_PHONE TEXT, -- The mailing phone number for the invoice number.
  ALTPAYR_INV_YN TEXT, -- Identifies if the invoice is for an alternate payer.
  LATE_REPLACEMENT_C_NAME TEXT, -- Flag to indicate the late replacement claim status of the invoice.
  CRD_ID TEXT, -- The claim reconciliation record ID.
  CLM_EXT_VAL_ID NUMERIC, -- The unique ID associated with the claim external value record for this row. Values derived from the claim print record or edited by the user will be stored in the claim external value. Form output will be based on the claim external value.
  CLM_ACCEPT_DT TEXT, -- The invoice accept date.
  FILING_ORDER_C_NAME TEXT, -- This column holds the filing order position of the claim coverage at the time claims were processed.
  CLAIM_RUN_NUM TEXT, -- The claim run number.
  DEMAND_CLAIM_YN TEXT, -- This column indicates whether the invoice was created in a demand claim run.
  PREDETERMINATION_YN TEXT, -- Stores a Yes/No indicator that the associated record represents a request for a predetermination of benefits.
  PREDICTED_PAY_DATE TEXT, -- The predicted payment response date for a claim based on historical trends for the payer.
  SUGGESTED_FOL_UP_DATE TEXT, -- The suggested initial follow-up date for a claim based on historical trends for the payer.
  FINAL_FOL_UP_DATE TEXT, -- This item shows the final date all the follow-up records were completed and is based on the last Completed Date (I FOL 122). It will only have a value if all of the follow-up records are currently completed. Should one reopen, this value will also be cleared.
  CLM_CLOSED_TIMELY_YN TEXT, -- Denotes if the claim closed prior to its Suggested Initial Follow-up Date, whereby it was no longer outstanding to insurance. The claim closed date is based on the CRD item of the same name (I CRD 86) if set, else the Final Follow-up Completed Date (I INV 133).
  PRIMARY KEY (INV_ID, LINE) -- Primary Key, FOREIGN KEY (INV_ID) REFERENCES INVOICE (INVOICE_ID)
, FOREIGN KEY (INV_ID) REFERENCES INVOICE (INVOICE_ID) -- References INVOICE table
, FOREIGN KEY (CVG_ID) REFERENCES COVERAGE (COVERAGE_ID) -- References COVERAGE table
, FOREIGN KEY (EPM_ID) REFERENCES CLARITY_EPM (PAYOR_ID) -- References CLARITY_EPM table
, FOREIGN KEY (EPP_ID) REFERENCES CLARITY_EPP (BENEFIT_PLAN_ID) -- References CLARITY_EPP table
, FOREIGN KEY (REF_ID) REFERENCES CLARITY_SER (PROV_ID) -- References CLARITY_SER table
, FOREIGN KEY (REF_ID) REFERENCES REFERRAL_SOURCE (REFERRING_PROV_ID) -- References REFERRAL_SOURCE table
, FOREIGN KEY (EAF_POS_ID) REFERENCES CLARITY_LOC (LOC_ID) -- References CLARITY_LOC table
, FOREIGN KEY (CRD_ID) REFERENCES RECONCILE_CLM (CLAIM_REC_ID) -- References RECONCILE_CLM table
, FOREIGN KEY (CLM_EXT_VAL_ID) REFERENCES CLM_VALUES (RECORD_ID) -- References CLM_VALUES table

);
