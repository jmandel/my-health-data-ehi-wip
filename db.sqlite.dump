CREATE TABLE REFERRAL_APT ( -- This table contains information about referral appointments.
  REFERRAL_ID NUMERIC, -- The referral ID for the referral record.
  LINE_COUNT INTEGER, -- A line number that is used to group information about contacts that have counted towards the referral.
  SERVICE_DATE TEXT, -- The date of the service (date of the appointment, claim, charge, or admission date) that is associated with the referral
  SERVICE_TYPE_C_NAME TEXT, -- The type of service that has been counted as a contact toward the total of completed contacts for this referral.
  SERIAL_NUMBER NUMERIC, -- The ID number of the contact, if the source is either "Visit" or "Admission"
  TABLE_COUNT NUMERIC, -- The number of completed contacts that have been counted for this source.
  PRIMARY KEY (REFERRAL_ID, LINE_COUNT) -- Primary Key, FOREIGN KEY (REFERRAL_ID) REFERENCES REFERRAL (REFERRAL_ID)
, FOREIGN KEY (REFERRAL_ID) REFERENCES REFERRAL (REFERRAL_ID) -- References REFERRAL table

);
