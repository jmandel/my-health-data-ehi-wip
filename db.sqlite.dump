CREATE TABLE HSP_PMT_LINE_REMIT ( -- This is a type of summary level of the remittance actions associated with a payment transaction. This table will summarize remittance information from Electronic Remittance and manual payment posting as stored on the transaction. This table is meant to hold a high level summary of line-level reason code information. Data will not be populated if a payment was posted at the invoice-level.
  TX_ID NUMERIC, -- The unique identifier for the transaction record.
  LINE INTEGER, -- The line number for the information associated with this record. Multiple pieces of information can be associated with this record.
  LINE_SVCLINE INTEGER, -- Stores the service line number for line level payments.
  LINE_GRP_CODE_C_NAME TEXT, -- Stores the group code corresponding to the reason code at the line level.
  LINE_REMIT_CODE_ID TEXT, -- Stores the reason code or remark code from the service line.
  LINE_REMIT_CODE_ID_REMIT_CODE_NAME TEXT, -- The name of each remittance code.
  LINE_RSN_CODE_EXTL TEXT, -- Stores the external claim level reason code or remark code from the service line.
  LINE_RMT_AMT NUMERIC, -- Stores the amount associated with the reason code at the line level.
  PRIMARY KEY (TX_ID, LINE) -- Primary Key, FOREIGN KEY (TX_ID) REFERENCES HSP_TRANSACTIONS (TX_ID)
, FOREIGN KEY (TX_ID) REFERENCES HSP_TRANSACTIONS (TX_ID) -- References HSP_TRANSACTIONS table
, FOREIGN KEY (LINE_REMIT_CODE_ID) REFERENCES CLARITY_RMC (REMIT_CODE_ID) -- References CLARITY_RMC table

);
