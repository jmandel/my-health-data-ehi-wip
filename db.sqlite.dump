  MOST_RECENT_VALUE TEXT, -- Contains the most recent compliance value for the goal
  RECENT_VALUE_I_DTTM TEXT, -- The instant that the most recent compliance value was recorded
  REC_VALUE_CHEC_DTTM TEXT, -- The instant that this goal was checked to determine its most recent compliance value
  AMB_GOAL_TYPE_C_NAME TEXT, -- Ambulatory goal type category (for example, Blood Pressure, Diet, etc.).
