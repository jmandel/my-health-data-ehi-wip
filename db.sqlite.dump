CREATE TABLE IP_FLWSHT_EDITED ( -- This table contains the previous values from edited flowsheet records.
  FSD_ID TEXT, -- The unique ID for the flowsheet data record.
  LINE INTEGER, -- The line count for the item.
  LINE_NUM INTEGER, -- The line number of the reading that replaced this flowsheet reading.
  RECORDED_TIME TEXT, -- The instant the reading was taken.
  ENTRY_TIME TEXT, -- The instant the reading was entered.
  TAKEN_USER_ID TEXT, -- The unique ID of the user taking the flowsheet readings.
  TAKEN_USER_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  ENTRY_USER_ID TEXT, -- The unique ID of the user entering the readings.
  ENTRY_USER_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  ACCEPTED_YN TEXT, -- Determines if this flowsheet record has been accepted.
  AUDIT_NEED_COSIG_YN TEXT, -- Indicates whether a value needing a co-sign is stored for the audit record for the flowsheet data record. Yes indicates that a value needing a co-sign gets over-written with a value not needing a co-sign.
  EDITED_FLT_ID TEXT, -- The unique ID of the flowsheet template (FLT) which was used to enter this data into the cell.
  EDITED_FLT_ID_DISPLAY_NAME TEXT, -- The display name associated with this template.
  EDT_FLO_DAT_USED NUMERIC, -- If the contact date (DAT) used to define data for a flowsheet group or row (see column FLO_DAT_USED) is changed, this column stores the prior value.
  FLO_CNCT_DATE_REAL NUMERIC, -- This column converts the contact date (DAT) of the flowsheet group or row to DTE, based on the value in column FLO_DAT_USED.
  PRIMARY KEY (FSD_ID, LINE) -- Primary Key, FOREIGN KEY (FSD_ID) REFERENCES IP_FLWSHT_REC (FSD_ID)
, FOREIGN KEY (FSD_ID) REFERENCES IP_FLWSHT_REC (FSD_ID) -- References IP_FLWSHT_REC table
, FOREIGN KEY (TAKEN_USER_ID) REFERENCES CLARITY_EMP (USER_ID) -- References CLARITY_EMP table
, FOREIGN KEY (ENTRY_USER_ID) REFERENCES CLARITY_EMP (USER_ID) -- References CLARITY_EMP table
, FOREIGN KEY (EDITED_FLT_ID) REFERENCES IP_FLT_DATA (TEMPLATE_ID) -- References IP_FLT_DATA table

);
