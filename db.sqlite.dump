CREATE TABLE ECHKIN_STEP_INFO ( -- This table contains eCheck-In information for a specific appointment.
  PAT_ENC_CSN_ID NUMERIC, -- The unique contact serial number for this contact. This number is unique across all patient encounters in your system. If you use IntraConnect, this is the Unique Contact Identifier (UCI).
  LINE INTEGER, -- The line number for the information associated with this contact. Multiple pieces of information can be associated with this contact.
  PAT_ENC_DATE_REAL NUMERIC, -- A unique contact date in decimal format. The integer portion of the number indicates the date of contact. The digits after the decimal distinguish different contacts on the same date and are unique for each contact on that date. For example, .00 is the first/only contact, .01 is the second contact, etc.
  CONTACT_DATE TEXT, -- The date of this contact in calendar format.
  INCLUDED_STEP_C_NAME TEXT, -- The step of the eCheck-In workflow.
  ECHKIN_STEP_STAT_C_NAME TEXT, -- The status of the specific step mentioned in the eCheck-In workflow.
  STEP_COMPLETED_UTC_DTTM TEXT, -- Stores the instant (in UTC) the step was completed in the eCheck-In workflow
  MYPT_ID TEXT, -- Stores the MyChart user ID that did the eCheck-In step.
  STEP_ACTION_C_NAME TEXT, -- The category ID for the action taken on an eCheck-In step.
  PRIMARY KEY (PAT_ENC_CSN_ID, LINE) -- Primary Key, FOREIGN KEY (PAT_ENC_CSN_ID) REFERENCES PAT_ENC (PAT_ENC_CSN_ID)
, FOREIGN KEY (PAT_ENC_CSN_ID) REFERENCES PAT_ENC (PAT_ENC_CSN_ID) -- References PAT_ENC table

);
