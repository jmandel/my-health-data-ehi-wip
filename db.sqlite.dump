CREATE TABLE DOCS_RCVD_ALGS ( -- This table stores discrete allergies information received from outside sources.-- Child table: DOCS_RCVD_ALGS_CMT (1 to 1 rows) joined on DOCUMENT_ID, CONTACT_DATE_REAL, GROUP_LINE

  DOCUMENT_ID NUMERIC, -- This item stores the Received Document record ID.
  CONTACT_DATE_REAL NUMERIC, -- This is a numeric representation of the date of this encounter in your system. The integer portion of the number specifies the date of the encounter. The digits after the decimal point indicate multiple visits on one day.
  LINE INTEGER, -- The line number for the information associated with this contact. Multiple pieces of information can be associated with this contact.
  ALG_REF_ID TEXT, -- This item stores a unique reference identifier to identify a specific instance of an allergy.
  ALG_SEVERITY_C_NAME TEXT, -- This item stores the overall allergy severity.
  ALG_DATE_NOTED_DT TEXT, -- This item stores the date when the allergy was noted by the external system.
  ALG_SRC_LPL_ID NUMERIC, -- This item stores the source Problem List record identifier.
  ALG_TYPE_OF_CHNG_C_NAME TEXT, -- This item stores the type of change being performed on the allergy.
  ALGN_NAME TEXT, -- This item stores the display name of the allergen as sent by the external source.
  ALGN_ID NUMERIC, -- This item stores the Allergen record identifier that maps to the allergen from the external source.
  ALGN_ID_ALLERGEN_NAME TEXT, -- The name of the allergen record.
  ALG_LAST_UPD_DTTM TEXT, -- This item stores the date and time when the allergy was last updated by the external system.
  ALG_PT_SRC_APP_C_NAME TEXT, -- If this allergy is a patient-entered allergy (i.e. DXR type = 25), this item stores the application which was used to edit the allergy for the contact (e.g. MyChart or Welcome). If blank, this is assumed to be MyChart.
  ALGRX_TYPE TEXT, -- This item stores the text value of the allergy reaction type sent by the external source.
  ALG_STATE_C_NAME TEXT, -- This item stores the value from the statusCode node in a received CDA document. This item itself is not the status of the allergy.
  ALG_STATUS_ENTRY_C_NAME TEXT, -- This item stores the value from the status entryRelationship node in a received CDA document. This item itself is not the status of the allergy.
  ALG_SRC_WPR_ID TEXT, -- Stores the Patient Access Accounts ID of the MyChart user who edited the allergy for the contact.
  ALG_CRITICALITY_C_NAME TEXT, -- This item stores the category value for overall allergy criticality.
  ALG_CRITICALITY_TXT TEXT, -- This item stores the free text allergy criticality.
  PRIMARY KEY (DOCUMENT_ID, CONTACT_DATE_REAL, LINE) -- Primary Key
);
