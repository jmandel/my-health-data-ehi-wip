CREATE TABLE RECONCILE_CLAIM_STATUS ( -- This table contains claim-level status code information for claims reconciliation.
  CLAIM_RECON_ID TEXT, -- The unique identifier for the claim reconciliation record.
  CONTACT_DATE_REAL NUMERIC, -- A unique contact date in decimal format. The integer portion of the number indicates the date of contact. The digits after the decimal distinguish different contacts on the same date and are unique for each contact on that date. For example, .00 is the first/only contact, .01 is the second contact, etc.
  LINE INTEGER, -- The line number for the information associated with this contact. Multiple pieces of information can be associated with this contact.
  CONTACT_DATE TEXT, -- The date of this contact in calendar format.
  CLM_STAT_CODE_C_NAME TEXT, -- Claim level status code received in a reconciliation file if value matches codes in the current claim status code category.
  CLM_CAT_CODE_C_NAME TEXT, -- Claim level category code received in a reconciliation file if value matches codes in the claim status category code category.
  CLM_MAPPED_ACT_C_NAME TEXT, -- Mapped action for the status code.
  CLM_ACT_CODE_C_NAME TEXT, -- Claim level action code received in a reconciliation file if value matches codes in the claim action category.
  CLM_STATUS_MSG TEXT, -- Claim level status message received in a reconciliation file.
  PRIMARY KEY (CLAIM_RECON_ID, CONTACT_DATE_REAL, LINE) -- Primary Key, FOREIGN KEY (CLAIM_RECON_ID) REFERENCES RECONCILE_CLM (CLAIM_REC_ID)
, FOREIGN KEY (CLAIM_RECON_ID) REFERENCES RECONCILE_CLM (CLAIM_REC_ID) -- References RECONCILE_CLM table

);
