CREATE TABLE IMMUNE ( -- The IMMUNE table contains data for immunizations ordered through clinical system. May also contain information on immunizations as reported by patient, but not ordered/administered via clinical system;  Fields in this table are noadd- single items in database. If an immunization record is edited/changed, that record will be re-extracted and reflect the updated values.-- Child table: IMMUNE_HISTORY (1 to 2 rows) joined on IMMUNE_ID

  IMMUNE_ID NUMERIC, -- The unique ID of the immunization record in your system production system.
  IMMUNZATN_ID NUMERIC, -- The ID of the immunization record that corresponds to the type of immunization given to this patient.
  IMMUNZATN_ID_NAME TEXT, -- The name of the immunization.
  IMMUNE_DATE TEXT, -- The date the immunization was administered in calendar format.
  DOSE TEXT, -- The immunization dosage.
  ROUTE_C_NAME TEXT, -- The category value associated with the route of the immunization, such as oral, intramuscular, or intradermal.
  SITE_C_NAME TEXT, -- The category value associated with the location of the injection, if appropriate. For example, left gluteus or right deltoid.
  MFG_C_NAME TEXT, -- The category value associated with the manufacturer of this vaccine.
  LOT TEXT, -- The lot number of the vaccine.
  GIVEN_BY_USER_ID TEXT, -- The unique ID of the system user who administered the immunization. This ID may be encrypted.
  GIVEN_BY_USER_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  ENTRY_USER_ID TEXT, -- The unique ID of the system user who ordered the immunization. This ID may be encrypted.  NOTE: If an immunization record is edited/updated, this will show the most recent change user ID.
  ENTRY_USER_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  ENTRY_DATE TEXT, -- The date the immunization was recorded in the patient�s chart in calendar format. NOTE: If an immunization record is edited/updated, this will show the most recent change date.
  EXPIRATION_DATE TEXT, -- Date upon which this immunization expires
  EXTERNAL_ADMIN_C_NAME TEXT, -- Category value indicating the source of verification of external administration of immunization, e.g. patient reported, WIR reported, etc.
  VIS_DATE_TEXT TEXT, -- The date on the vaccine information statement. Note that this is a free text field in the application, so data will not be in standard datetime format.
  MED_ADMIN_COMMENT TEXT, -- Free text comment regarding the administration of this immunization
  IMM_PRODUCT TEXT, -- Item which stores the product of the immunization. Products are usually related to the lot number.
  NDC_NUM_ID TEXT, -- Store the NDC number ID associated with the administration
  NDC_NUM_ID_NDC_CODE TEXT, -- The external code for the National Drug Code (NDC). An NDC represents packages of medications.
  ORDER_ID NUMERIC, -- Order ID for immunization ordered.
  IMM_ANSWER_ID TEXT, -- Stores answers for immunization questions.
  IMMNZTN_STATUS_C_NAME TEXT, -- The category value associated with "Given" if the immunization has been administered, "Deleted" if the immunization has been deleted from the administration history, "Incomplete" if the item has been ordered but not administered and a status of "Deferred" if the immunization has been deferred.
  IMM_CHARGE_REC_ID TEXT, -- This column contains the UCL (Universal Charge Line) record ID for the immunization charge.
  IMM_CSN NUMERIC, -- This column contains the CSN (contact serial number) for the immunization.
  IMM_HISTORIC_ADM_YN TEXT, -- Indicates whether the immunization administration is a historical administration.
  IMMNZTN_DOSE_AMOUNT NUMERIC, -- Immunization dose amount.
  IMMNZTN_DOSE_UNIT_C_NAME TEXT, -- Immunization dose unit.
  ENTRY_DTTM TEXT, -- Contains the date and time that the immunization administration data was last updated. If the exact time is not known, a date may be contained in ENTRY_DATE instead.
  IMM_LOT_NUM_ID NUMERIC, -- This item stores the record ID of the lot(LOT) used for immunization administration.
  PRIMARY KEY (IMMUNE_ID) -- Primary Key, FOREIGN KEY (IMMUNE_ID) REFERENCES PROBLEM_LIST_ALL (PROBLEM_LIST_ID) -- References PROBLEM_LIST_ALL table
, FOREIGN KEY (IMMUNZATN_ID) REFERENCES CLARITY_IMMUNZATN (IMMUNZATN_ID) -- References CLARITY_IMMUNZATN table
, FOREIGN KEY (ENTRY_USER_ID) REFERENCES CLARITY_EMP (USER_ID) -- References CLARITY_EMP table

);
