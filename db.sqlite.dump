CREATE TABLE IP_FLOW_DATERNG ( -- Tracks the dates during which a flowsheet template was used on a given patient stay.
  INPATIENT_DATA_ID TEXT, -- The unique ID of the inpatient data store record.
  LINE INTEGER, -- The line number for the information associated with this record. Multiple pieces of information can be associated with this record.
  FLOWSHEET_DATE_ID TEXT, -- Stores the flowsheet template (FLT) ID related to the first and last instants documented.
  FLOWSHEET_DATE_ID_DISPLAY_NAME TEXT, -- The display name associated with this template.
  PRIMARY KEY (INPATIENT_DATA_ID, LINE) -- Primary Key, FOREIGN KEY (INPATIENT_DATA_ID) REFERENCES IP_DATA_STORE (INPATIENT_DATA_ID)
, FOREIGN KEY (INPATIENT_DATA_ID) REFERENCES IP_DATA_STORE (INPATIENT_DATA_ID) -- References IP_DATA_STORE table
, FOREIGN KEY (FLOWSHEET_DATE_ID) REFERENCES IP_FLT_DATA (TEMPLATE_ID) -- References IP_FLT_DATA table

);
