CREATE TABLE HSP_CLAIM_DETAIL1 ( -- This table contains claim print record information for claims associated with a given hospital account or liability bucket.-- Child table: CLP_OCCUR_DATA (2 to 3 rows) joined on CLAIM_PRINT_ID
  -- Child table: CLP_VALUE_DATA (1 to 1 rows) joined on CLAIM_PRINT_ID
  -- Child table: HSP_CLAIM_PRINT (1 to 1 rows) joined on CLAIM_PRINT_ID
  -- Child table: HSP_CLP_CMS_TX_PIECES (2 to 3 rows) joined on CLAIM_PRINT_ID
  -- Child table: HSP_CLP_DIAGNOSIS (1 to 2 rows) joined on CLAIM_PRINT_ID
  -- Child table: HSP_CLP_REV_CODE (23 to 23 rows) joined on CLAIM_PRINT_ID
  -- Child table: HSP_CLP_UB_TX_PIECES (2 to 3 rows) joined on CLAIM_PRINT_ID
  -- Child table: HSP_CLP_CMS_LINE (2 to 3 rows) joined on CLAIM_PRINT_ID

  CLAIM_PRINT_ID NUMERIC, -- Stores the claim record ID associated with a single hospital account.
  MAIL_NAME TEXT, -- The mailing name for this claim.
  MAIL_CITY_STATE_ZIP TEXT, -- The mailing city, state, and ZIP code for this claim.
  MAIL_PHONE TEXT, -- The mailing phone number for this claim.
  SRC_OF_ADDR_C_NAME TEXT, -- The source of the mailing address for this claim.
  LINE_SOURCE_CLP_ID TEXT, -- The source claim record for resubmit and demand claims.
  PARTIAL_CLAIM_YN TEXT, -- Indicates whether the claim is a partial resubmit.
  EXPECTED_PYMT NUMERIC, -- Claim level expected reimbursement.
  CLAIM_BILLED_AMOUNT NUMERIC, -- Billed amount determined from reimbursement information for Diagnosis Related Group priced claims.
  CLM_CONTRACTUAL NUMERIC, -- Contractual amount determined from reimbursement information for Diagnosis Related Group priced claims.
  CLM_EXPECTED_PRICE NUMERIC, -- Expected amount determined from reimbursement information for Diagnosis Related Group priced claims.
  CLAIM_INS_PORTION NUMERIC, -- Insurance portion of the expected amount. Determined from reimbursement information for Diagnosis Related Group priced claims.
  CLM_PATIENT_PORTION NUMERIC, -- Portion of the expected amount the patient is responsible for. Determined from reimbursement information for Diagnosis Related Group priced claims.
  CLAIM_MTHD_DESC TEXT, -- A text description of the method used to price the claim.  Determined from reimbursement information for Diagnosis Related Group priced claims.
  CONTRACT_DATE_REAL NUMERIC, -- A numerical representation of the contact date for the contract used in this claim. Used to help link to the VEN_NET_CONT_SVC table.
  MAIL_ADDR1 TEXT, -- First line of the mailing address for a given claim record.
  REIMB_COST_THRESH NUMERIC, -- The cost threshold of this claim's outlier data. Determined from reimbursement information for Diagnosis Related Group priced claims.
  REIMB_COST_OUT NUMERIC, -- The cost outlier of this claim's outlier data. Determined from reimbursement information for Diagnosis Related Group priced claims.
  SA_ID NUMERIC, -- This column holds the service area for the claim.
  INACTV_CLP_YN TEXT, -- This column has a value of yes when the claim is inactive.
  CLAIM_ACCEPT_DTTM TEXT, -- This column holds the instant the claim was accepted.
  SG_PAYOR_ID NUMERIC, -- The payer ID for this claim.
  SG_PLAN_ID NUMERIC, -- The plan ID for this claim.
  SG_CVG_ID NUMERIC, -- The coverage ID for this claim.
  INVOICE_NUM TEXT, -- The invoice number for this claim.
  SG_GR_ACCT_ID NUMERIC, -- The guarantor account ID for this claim.
  HOSPITAL_ACCT_ID NUMERIC, -- The hospital account ID for this claim.
  HLB_ID NUMERIC, -- The liability bucket ID for this claim.
  SG_PROV_ID TEXT, -- The billing provider ID for this claim.
  SG_LOC_ID NUMERIC, -- The location for this claim.
  SG_POS_ID NUMERIC, -- The place of service ID for this claim.
  CLAIM_CLASS_C_NAME TEXT, -- The account class used to evaluate this claim.
  CLAIM_BASE_CLASS_C_NAME TEXT, -- The base account class used to evaluate this claim.
  MIN_SERVICE_DT TEXT, -- The minimum service date for this claim.
  MAX_SERVICE_DT TEXT, -- The maximum service date for this claim.
  UB_FROM_DT TEXT, -- The uniform billing claim from date.
  UB_THROUGH_DT TEXT, -- The uniform billing claim through date.
  CLAIM_TYPE_C_NAME TEXT, -- The claim type.
  CLAIM_FRM_TYPE_C_NAME TEXT, -- The form type. This is either paper or electronic.
  TTL_CHRGS_AMT NUMERIC, -- Total charges amount.
  TTL_DUE_AMT TEXT, -- Total due amount.
  TTL_NONCVD_AMT NUMERIC, -- Total non-covered amount.
  TTL_PMT_AMT NUMERIC, -- Total payment amount.
  UB_BILL_TYPE TEXT, -- Type of bill.
  UB_CVD_DAYS INTEGER, -- Covered days.
  UB_COINS_DAYS INTEGER, -- Coinsurance days.
  UB_NON_CVD_DAYS INTEGER, -- Non-covered days.
  UB_PRINC_DX_ID NUMERIC, -- Principal diagnosis.
  SG_ALTPYR_CLM_YN TEXT, -- Flag used to indicate that claim is for alternate payer.
  FILING_ORDER_C_NAME TEXT, -- This column holds the filing order position of the claim coverage at the time claims were processed.
  CLM_EXT_VAL_ID NUMERIC, -- The ID of the claim record.
