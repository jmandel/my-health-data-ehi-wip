, FOREIGN KEY (ORDER_PROC_ID) REFERENCES ORDER_PROC (ORDER_PROC_ID) -- References ORDER_PROC table
, FOREIGN KEY (ORDER_PROC_ID) REFERENCES ORD_PRFLST_TRK (ORDER_ID) -- References ORD_PRFLST_TRK table

