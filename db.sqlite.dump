CREATE TABLE SURGICAL_HX ( -- The SURGICAL_HX table contains data from medical history contacts entered in clinical system patient encounters. Since one patient encounter may contain multiple medical history contacts, each contact is uniquely identified by a combination of the patient encounter serial number and a line number.
  PAT_ENC_DATE_REAL NUMERIC, -- This is a numeric representation of the date of this encounter in your system. The integer portion of the number specifies the date of the encounter. The digits after the decimal point indicate multiple visits on one day.
  LINE INTEGER, -- The line number of the surgical history contact within the encounter.
  PROC_ID NUMERIC, -- The unique ID of the procedure record associated with the surgical history contact. Note: This is NOT the CPT(R) code. It is an internal identifier that is typically not visible to a user.
  CONTACT_DATE TEXT, -- The contact date of the encounter associated with this surgical history contact. Note: There may be multiple encounters on the same calendar date.
  PAT_ENC_CSN_ID NUMERIC, -- A unique serial number for this encounter. This number is unique across all patients and encounters in the system.
  SURGICAL_HX_SRC_C_NAME TEXT, -- This category number represents the source of entry, the person providing the information, for the surgical history data.
  HX_LNK_ENC_CSN NUMERIC, -- The Contact Serial Number of the encounter in which the history was created/edited. If the history was created/edited outside of the context of an encounter, then this column will be blank.
  PRIMARY KEY (PAT_ENC_CSN_ID, LINE) -- Primary Key, FOREIGN KEY (PAT_ENC_CSN_ID) REFERENCES PAT_ENC (PAT_ENC_CSN_ID)
, FOREIGN KEY (PAT_ENC_CSN_ID) REFERENCES SOCIAL_HX (PAT_ENC_CSN_ID)
, FOREIGN KEY (PROC_ID) REFERENCES CLARITY_EAP (PROC_ID) -- References CLARITY_EAP table
, FOREIGN KEY (PAT_ENC_CSN_ID) REFERENCES PAT_ENC (PAT_ENC_CSN_ID) -- References PAT_ENC table
, FOREIGN KEY (PAT_ENC_CSN_ID) REFERENCES SOCIAL_HX (PAT_ENC_CSN_ID) -- References SOCIAL_HX table

);
