CREATE TABLE SERVICE_BENEFITS ( -- The SERVICE_BENEFITS table contains service-level benefit information about the coverages for the encounter or estimate to which this benefit record is attached.
  RECORD_ID NUMERIC, -- The unique identifier for the benefit record.
  LINE INTEGER, -- The line number for the information associated with this record. Multiple pieces of information can be associated with this record.
