  EXTERNAL_ID_NUM TEXT, -- The external identification number used on the referral.
  PAT_ID TEXT, -- The ID of the patient associated with the referral.
  PCP_PROV_ID TEXT, -- The unique ID of the patient's primary care provider at the time the referral was created.
  ENTRY_DATE TEXT, -- The date the referral was entered.
  RFL_STATUS_C_NAME TEXT, -- The category value representing the status of the referral (I.e. authorized, open, pending, etc.).
  REFERRING_PROV_ID TEXT, -- The unique ID of the referral source (REF) record of the provider who made the referral. This column is frequently used to link to the REFERRAL_SOURCE table. The actual provider (SER) ID can be found in column REF_PROVIDER_ID of table REFERRAL_SOURCE.
  REFERRING_PROV_ID_REFERRING_PROV_NAM TEXT, -- The name of the referral source.
  PROV_SPEC_C_NAME TEXT, -- The category value indicating the provider specialty being referred to.
  RFL_TYPE_C_NAME TEXT, -- The category value indicating the type of referral.
  RSN_FOR_RFL_C_NAME TEXT, -- The category value indicating the main (first) reason for the referral.   Since multiple reasons can be listed, use table REFERRAL_REASONS to view all of them.
  RFL_CLASS_C_NAME TEXT, -- The category value indicating the class of the referral.
  AUTH_NUM_OF_VISITS NUMERIC, -- The number of visits authorized for this referral.
  START_DATE TEXT, -- The start date of the referral.
  EXP_DATE TEXT, -- The expiration date of the referral.
  SERV_AREA_ID NUMERIC, -- The ID number of the service area associated with the referral.
  COVERAGE_ID NUMERIC, -- The unique ID of the coverage associated with the referral.
  NUM_PROC NUMERIC, -- The number of procedures associated with the referral.
  SVC_DATE_REAL NUMERIC, -- If available, this column is populated by the authorized start date (I RFL 85). If not, it is populated by the expiration date on the referral (I RFL 90). If neither of these are available, the column will be empty. The date in this column is based on days since December 31, 1840.
  PAYOR_ID NUMERIC, -- The ID number of the payor associated with the referral.
  PLAN_ID NUMERIC, -- The ID number of the plan associated with the referral.
  SERV_DATE TEXT, -- This column is populated by the authorized start date (I RFL 85) if available. If not, it is populated by the expiration date on the referral (I RFL 90). If neither of these are available, the column will be empty. The date in this column is in MM/DD/YYYY format.
  RETRO_FLAG_YN TEXT, -- The category value used to mark a referral as being "Retro" entered.
  AUTO_APPROVED_DATE TEXT, -- The date on which the referral was approved automatically by the system.
  AUTH_RSN_C_NAME TEXT, -- The category value indicating the authorization reason associated with the referral.
  REFD_BY_LOC_POS_ID NUMERIC, -- The ID number of the place of service the referral was referred from.
  REFD_TO_LOC_POS_ID NUMERIC, -- The ID number of the place of service the referral was referred to.
  REFD_TO_DEPT_ID NUMERIC, -- The ID number of the department the referral was referred to.
  REFD_TO_SPEC_C_NAME TEXT, -- The category value indicating the specialty the department was referred to.
  PRIORITY_C_NAME TEXT, -- The category value indicating the priority of the referral.
  IBNR_PAY_UNTIL_DT TEXT, -- The date up to which your facility will pay claims for the procedures approved on this referral.
  PRIM_LOC_ID NUMERIC, -- The unique ID of the member's primary location at the time the referral was entered.
  ACTUAL_NUM_VISITS NUMERIC, -- The actual number of completed visits for this referral.
  SCHED_NUM_VISITS NUMERIC, -- The number of visits scheduled for this referral.
  REQUEST_NUM_VISITS NUMERIC, -- The number of visits requested for this referral.
  DISP_VAL_C_NAME TEXT, -- Whether the referral was accepted ("appointed") or refused ("denied") by the referred-to provider, department or facility.
  REFD_BY_DEPT_ID NUMERIC, -- The ID number of the department the referral was referred by.
  CLOSE_RSN_C_NAME TEXT, -- For closed referrals, the category value indicating the reason for closing.
  SCHED_STATUS_C_NAME TEXT, -- Scheduling status of the referral to keep track of internally schedulable referrals.  For category list use ZC_SCHED_STATUS.
  SCHED_BY_DATE TEXT, -- Indicates deadline to schedule a referral.
  PREAUTH_REQ_C_NAME TEXT, -- For referrals created from an order, indicates if a preauthorization number must be collected before scheduling.
  PREAUTH_CHG_EMP_ID TEXT, -- The unique ID of the user who last changed the preauthorization data.
  PREAUTH_CHG_EMP_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  PREAUTH_CHNGD_DTTM TEXT, -- Date/time stamp for last time the preauthorization data was changed.
  EXT_REF_DATE TEXT, -- This is the external referring date.
  DECISION_DATE TEXT, -- Date on which the referral's current status was assigned.
  RFL_STATCHG_RSN_C_NAME TEXT, -- The reason category number describing why the referral status was changed.
  CALC_SVC_LVL_CNTS INTEGER, -- The calculated number of service level authorizations counts, based on the service level authorizations collected.
  RFL_DIRECTION_C_NAME TEXT, -- The referral direction, as calculated by logic for transitions of care
  REFD_TO_LINK_PROV_YN TEXT, -- Is the referred to provider an EpicCare Link provider
  REFD_TO_LINK_LOC_YN TEXT, -- Is the referred to location an EpicCare Link location
  IS_LEAKED_YN TEXT, -- Holds whether the referral is considered leaked
  RECENT_AUTH_RSN_C_NAME TEXT, -- This item holds the most recent non-empty authorization reason for the referral. If this item is null, it means the referral was never authorized. If value is added/updated to the authorization code (I RFL 73), same value is copied into this item. But if authorization code (I RFL 73) is cleared, then this item is not updated.
  INITIAL_REQUEST_TYPE_C_NAME TEXT, -- The initial request type for the referral.
  REF_BY_CNTRCT_AT_AUTH_YN TEXT, -- The contracted status of referred-by entities on the referral as of the last decision or the current contracted status if the referral is pending.
  REF_TO_CNTRCT_AT_AUTH_YN TEXT, -- The contracted status of referred-to entities on the referral as of the last decision or the current contracted status if the referral is pending.
  REF_BY_SER_CNTRCT_AT_AUTH_YN TEXT, -- The contracted status of referred-by provider on the referral as of the last decision or the current contracted status if the referral is pending.
  REF_BY_EAF_CNTRCT_AT_AUTH_YN TEXT, -- The contracted status of referred-by location on the referral as of the last decision or the current contracted status if the referral is pending.
  REF_TO_EAF_CNTRCT_AT_AUTH_YN TEXT, -- The contracted status of referred-to location on the referral as of the last decision or the current contracted status if the referral is pending.
  REF_BY_CNTRCT_AT_DEC_YN TEXT, -- The contracted status of referred-by entities on the referral as of the last decision.
  REF_TO_CNTRCT_AT_DEC_YN TEXT, -- The contracted status of referred-to entities on the referral as of the last decision.
  REF_BY_SER_CNTRCT_AT_DEC_YN TEXT, -- The contracted status of referred-by provider on the referral as of the last decision.
  REF_BY_LOC_CNTRCT_AT_DEC_YN TEXT, -- The contracted status of referred-by location on the referral as of the last decision.
  REF_TO_LOC_CNTRCT_AT_DEC_YN TEXT, -- The contracted status of referred-to location on the referral as of the last decision.
  PRIMARY KEY (REFERRAL_ID) -- Primary Key, FOREIGN KEY (PAT_ID) REFERENCES PATIENT (PAT_ID) -- References PATIENT table
, FOREIGN KEY (PCP_PROV_ID) REFERENCES CLARITY_SER (PROV_ID) -- References CLARITY_SER table
, FOREIGN KEY (PCP_PROV_ID) REFERENCES REFERRAL_SOURCE (REFERRING_PROV_ID) -- References REFERRAL_SOURCE table
, FOREIGN KEY (REFERRING_PROV_ID) REFERENCES CLARITY_SER (PROV_ID) -- References CLARITY_SER table
, FOREIGN KEY (REFERRING_PROV_ID) REFERENCES REFERRAL_SOURCE (REFERRING_PROV_ID) -- References REFERRAL_SOURCE table
, FOREIGN KEY (SERV_AREA_ID) REFERENCES CLARITY_SA (SERV_AREA_ID) -- References CLARITY_SA table
, FOREIGN KEY (SERV_AREA_ID) REFERENCES CLARITY_LOC (LOC_ID) -- References CLARITY_LOC table
, FOREIGN KEY (COVERAGE_ID) REFERENCES COVERAGE (COVERAGE_ID) -- References COVERAGE table
, FOREIGN KEY (PAYOR_ID) REFERENCES CLARITY_EPM (PAYOR_ID) -- References CLARITY_EPM table
, FOREIGN KEY (PLAN_ID) REFERENCES CLARITY_EPP (BENEFIT_PLAN_ID) -- References CLARITY_EPP table
, FOREIGN KEY (REFD_BY_LOC_POS_ID) REFERENCES CLARITY_LOC (LOC_ID) -- References CLARITY_LOC table
, FOREIGN KEY (PRIM_LOC_ID) REFERENCES CLARITY_LOC (LOC_ID) -- References CLARITY_LOC table
, FOREIGN KEY (REFD_BY_DEPT_ID) REFERENCES CLARITY_DEP (DEPARTMENT_ID) -- References CLARITY_DEP table

);
