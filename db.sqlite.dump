CREATE TABLE INVOICE ( -- This table includes the basic data for the invoice (INV) record. It contains one row for each used INV record (excluding unused pre-allocated invoices). Note that a row in this table can correspond to multiple claims sent.-- Child table: INV_BASIC_INFO (1 to 2 rows) joined on INV_ID
  -- Child table: INV_CLM_ICN (1 to 1 rows) joined on INVOICE_ID
  -- Child table: INV_CLM_LN_ADDL (1 to 3 rows) joined on INVOICE_ID
  -- Child table: INV_DX_INFO (1 to 6 rows) joined on INVOICE_ID
  -- Child table: INV_NDC_INFO (1 to 1 rows) joined on INVOICE_ID
  -- Child table: INV_NUM_TX_PIECES (1 to 3 rows) joined on INV_ID
  -- Child table: INV_PMT_RECOUP (1 to 5 rows) joined on INVOICE_ID
  -- Child table: INV_TX_PIECES (1 to 3 rows) joined on INV_ID

  INVOICE_ID NUMERIC, -- The Invoice ID.
  PAT_ID TEXT, -- The patient ID associated with this invoice.
  ACCOUNT_ID NUMERIC, -- The account ID that is associated with this invoice.
  SERV_AREA_ID NUMERIC, -- The ID of the service area associated with this invoice.
  LOC_ID NUMERIC, -- The ID of the revenue location associated with this invoice.
  POS_ID NUMERIC, -- The ID of the place of service associated with this invoice.
  DEPARTMENT_ID NUMERIC, -- The ID of the department associated with this invoice.
  PROV_ID TEXT, -- Stores the ID of the billing provider associated with the invoice.
  TAX_ID TEXT, -- Stores the tax ID associated with the invoice.
  TAX_ID_TYPE TEXT, -- Stores the type of tax ID associated with the invoice.
  INSURANCE_AMT NUMERIC, -- Stores the insurance amount for the invoice.
  SELF_PAY_AMT NUMERIC, -- Stores the initial self-pay amount for the invoice.
  INIT_INSURANCE_BAL NUMERIC, -- Stores the initial insurance amount for the invoice.
  INIT_SELF_PAY_BAL NUMERIC, -- Stores the initial self-pay amount for the invoice.
  BILL_AREA_ID NUMERIC, -- Stores the bill area associated with the invoice.
  BILL_AREA_ID_BILL_AREA_NAME TEXT, -- The record name of this bill area, financial subdivision, or financial division.
  PB_HOSP_ACT_ID NUMERIC, -- The Professional Billing Hospital Account ID.
  PRIMARY KEY (INVOICE_ID) -- Primary Key, FOREIGN KEY (PAT_ID) REFERENCES PATIENT (PAT_ID) -- References PATIENT table
, FOREIGN KEY (ACCOUNT_ID) REFERENCES ACCOUNT (ACCOUNT_ID) -- References ACCOUNT table
, FOREIGN KEY (SERV_AREA_ID) REFERENCES CLARITY_SA (SERV_AREA_ID) -- References CLARITY_SA table
, FOREIGN KEY (SERV_AREA_ID) REFERENCES CLARITY_LOC (LOC_ID) -- References CLARITY_LOC table
, FOREIGN KEY (LOC_ID) REFERENCES CLARITY_LOC (LOC_ID) -- References CLARITY_LOC table
, FOREIGN KEY (POS_ID) REFERENCES CLARITY_LOC (LOC_ID) -- References CLARITY_LOC table
, FOREIGN KEY (DEPARTMENT_ID) REFERENCES CLARITY_DEP (DEPARTMENT_ID) -- References CLARITY_DEP table
, FOREIGN KEY (PROV_ID) REFERENCES CLARITY_SER (PROV_ID) -- References CLARITY_SER table
, FOREIGN KEY (PROV_ID) REFERENCES REFERRAL_SOURCE (REFERRING_PROV_ID) -- References REFERRAL_SOURCE table
, FOREIGN KEY (BILL_AREA_ID) REFERENCES CLARITY_HM_TOPIC (HM_TOPIC_ID) -- References CLARITY_HM_TOPIC table
, FOREIGN KEY (BILL_AREA_ID) REFERENCES CLARITY_IMMUNZATN (IMMUNZATN_ID) -- References CLARITY_IMMUNZATN table
, FOREIGN KEY (BILL_AREA_ID) REFERENCES V_BIL_ALL (BILL_AREA_ID) -- References V_BIL_ALL table
, FOREIGN KEY (PB_HOSP_ACT_ID) REFERENCES ARPB_VISITS (PB_VISIT_ID) -- References ARPB_VISITS table
, FOREIGN KEY (PB_HOSP_ACT_ID) REFERENCES HAR_ALL (ACCT_ID) -- References HAR_ALL table

);
