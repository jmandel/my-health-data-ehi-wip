CREATE TABLE NOTES_ACCT ( -- This table contains summary information for billing system account notepad notes attached to accounts.
  NOTE_ID TEXT, -- The unique ID of the Account Notepad note record.
  ACCOUNT_ID NUMERIC, -- This column stores the unique identifier for the guarantor associated with the note. It is only populated for guarantor-level notes.
  ACTIVE_STATUS TEXT, -- The status of the note: active or inactive.
  ENTRY_USER_ID TEXT, -- The ID of the user who manually created the note. If the note was automatically created by billing system, this is the person who executed the activity that triggered the note creation. This ID may be encrypted
  ENTRY_USER_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  INVOICE_NUMBER TEXT, -- The invoice number associated with this note.
  NOTE_ENTRY_DTTM TEXT, -- The date and time the account note was created.
  PRIMARY KEY (NOTE_ID) -- Primary Key, FOREIGN KEY (NOTE_ID) REFERENCES HNO_INFO (NOTE_ID) -- References HNO_INFO table
, FOREIGN KEY (ACCOUNT_ID) REFERENCES ACCOUNT (ACCOUNT_ID) -- References ACCOUNT table
