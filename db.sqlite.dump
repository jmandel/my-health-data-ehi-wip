CREATE TABLE PROBLEM_LIST_HX ( -- This table contains data relating to the history of problems from patients' problem lists in the clinical system.
  PROBLEM_LIST_ID NUMERIC, -- The unique ID of this Problem List entry.
  LINE INTEGER, -- Used to identify the particular problem within the historical problems
  HX_PROBLEM_ID NUMERIC, -- ID of the diagnosis associated with this historical problem
  HX_DATE_NOTED TEXT, -- Represents the historical value of the first possible date that a problem could have been noted/onset on.    A problem's noted date is documented as a fuzzy date, meaning that it can capture approximate date data ("2012", "1/2012") or exact data ("3/5/2012"). This column captures the earliest date of the effective range. See HX_NOTED_END_DATE for the latest counterpart. For example, if 2012 is documented in hyperspace, then HX_NOTED_DATE will be 1/1/2012 and HX_NOTED_END_DATE will be 12/31/2012.
  HX_DATE_OF_ENTRY TEXT, -- The date that the problem was added to or updated on the patient's Problem List in calendar format.
  HX_ENTRY_USER_ID TEXT, -- The ID of the user who edited this problem on the patient's Problem List. This ID may be encrypted.
  HX_ENTRY_USER_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  HX_MYCHART_YN TEXT, -- Indicates this problem was visible on web based chart system when this historical action was recorded.
  HX_CHRONIC_YN TEXT, -- This column indicates whether or not this problem was flagged as chronic as of this historical event.
  HX_PRINCIPAL_YN TEXT, -- This column indicates whether or not this problem was flagged as the principal problem as of this historical event.
  HX_IS_HOSP_YN TEXT, -- This column indicates whether or not this problem was flagged as a hospital problem as of this historical event.
  HX_PROBLEM_EPT_CSN NUMERIC, -- Contact Serial Number (CSN) of the patient encounter where this historical problem list was documented.
  HX_STATUS_C_NAME TEXT, -- The category value associated with the problem's state: Active, Resolved, or Deleted as of this historical event.
  HX_ENTRY_INST TEXT, -- The date and time when the problem was updated on the patient's problem list.
  HX_NOTED_END_DATE TEXT, -- Represents the historical value of the last possible date that a problem could have been noted/onset on.    A problem's noted date is documented as a fuzzy date, meaning that it can capture approximate date data ("2012", "1/2012") or exact data ("3/5/2012"). This column captures the latest date of the effective range. See HX_NOTED_DATE for the latest counterpart. For example, if 2012 is documented in hyperspace, then HX_NOTED_DATE will be 1/1/2012 and HX_NOTED_END_DATE will be 12/31/2012.  Note that the value may be empty, even if HX_NOTED_DATE is populated
  PRIMARY KEY (PROBLEM_LIST_ID, LINE) -- Primary Key, FOREIGN KEY (PROBLEM_LIST_ID) REFERENCES PROBLEM_LIST_ALL (PROBLEM_LIST_ID)
, FOREIGN KEY (PROBLEM_LIST_ID) REFERENCES PROBLEM_LIST (PROBLEM_LIST_ID)
, FOREIGN KEY (PROBLEM_LIST_ID) REFERENCES PROBLEM_LIST_ALL (PROBLEM_LIST_ID) -- References PROBLEM_LIST_ALL table
, FOREIGN KEY (PROBLEM_LIST_ID) REFERENCES PROBLEM_LIST (PROBLEM_LIST_ID) -- References PROBLEM_LIST table
, FOREIGN KEY (HX_PROBLEM_ID) REFERENCES CLARITY_EDG (DX_ID) -- References CLARITY_EDG table
, FOREIGN KEY (HX_ENTRY_USER_ID) REFERENCES CLARITY_EMP (USER_ID) -- References CLARITY_EMP table

);
