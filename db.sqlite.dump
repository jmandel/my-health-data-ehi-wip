CREATE TABLE DOCS_RCVD_PROC ( -- This table stores procedure information received from outside sources.
  DOCUMENT_ID NUMERIC, -- The unique identifier for the document record.
  CONTACT_DATE_REAL NUMERIC, -- A unique contact date in decimal format. The integer portion of the number indicates the date of contact. The digits after the decimal distinguish different contacts on the same date and are unique for each contact on that date. For example, .00 is the first/only contact, .01 is the second contact, etc.
  LINE INTEGER, -- The line number for the information associated with this contact. Multiple pieces of information can be associated with this contact.
  CONTACT_DATE TEXT, -- The date of this contact in calendar format.
  PROC_REF_ID TEXT, -- This item stores the unique reference ID.
  PROC_TYPE_TXT TEXT, -- This item stores the free text name for the external procedure.
  PROC_START_DATE TEXT, -- This item stores the start date of the external procedure.
  PROC_END_DATE TEXT, -- This item stores the end date of the external procedure.
  PROC_LST_UPD_INST_DTTM TEXT, -- Stores the last update instant of the procedure in Coordinated Universal Time (UTC).
  PRIMARY KEY (DOCUMENT_ID, CONTACT_DATE_REAL, LINE) -- Primary Key
);
