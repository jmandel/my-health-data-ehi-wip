CREATE TABLE ARPB_TX_ACTIONS ( -- This table contains information about actions performed on professional billing transactions.
  TX_ID NUMERIC, -- The unique key or identification number for a given transaction.
  LINE INTEGER, -- This column contains the line count for the information in this table. Each action associated with this transaction is stored on a separate line, one line for each entry.
  ACTION_TYPE_C_NAME TEXT, -- The action type category ID taken on the transaction.
  ACTION_DATE TEXT, -- The date in which this action is performed.
  ACTION_AMOUNT NUMERIC, -- The amount associated with this action.
  PAYOR_ID NUMERIC, -- The Payor associated with this action.
  DENIAL_CODE TEXT, -- The denial code associated with this action.
  DENIAL_CODE_REMIT_CODE_NAME TEXT, -- The name of each remittance code.
  POST_DATE TEXT, -- The date this transaction was posted in calendar format.
  STMT_DATE TEXT, -- The statement date of this transaction.
  OUT_AMOUNT_BEFORE NUMERIC, -- Outstanding amount of associated transaction before the action is performed.
  OUT_AMOUNT_AFTER NUMERIC, -- Outstanding amount of the associated transaction after the action is performed.
  INS_AMOUNT_BEFORE NUMERIC, -- Insurance amount of the associated transaction before the action is performed.
  INS_AMOUNT_AFTER NUMERIC, -- Insurance amount of the associated transaction after the action is performed.
  BEFORE_PAYOR_ID NUMERIC, -- The Payor of the associated transaction before the action is performed.
  AFTER_PAYOR_ID NUMERIC, -- The Payor of the associated transaction after the action is performed.
  BEFORE_CVG_ID NUMERIC, -- The coverage of the associated transaction before the action is performed.
  AFTER_CVG_ID NUMERIC, -- The coverage of the associated transaction after the action is performed.
  ACTION_USER_ID TEXT, -- The unique ID of the user who performed this action.
  ACTION_USER_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  ADJ_CODE_ID NUMERIC, -- If an adjustment is associated with this action, this column contains the adjustment code of that adjustment.
  RMC_ID TEXT, -- The first reason code ID associated with this action.
  RMC_ID_REMIT_CODE_NAME TEXT, -- The name of each remittance code.
  PMT_PAYOR_ID NUMERIC, -- The Payor of the payment if this action is associated with a payment.
  POS_ID NUMERIC, -- Place of Service ID of the transaction.
  DEPARTMENT_ID NUMERIC, -- Department ID of this transaction.
  PROC_ID NUMERIC, -- The procedure ID for the transaction record.
  LOCATION_ID NUMERIC, -- Location Id for this transaction
  SERVICE_AREA_ID NUMERIC, -- Service Area ID for this transaction
  ACCOUNT_ID NUMERIC, -- The internal ID of the record that maintains the patient's transactions. A patient may use more than one account and an account may contain more than one patient.
  PRIMARY_DX_ID NUMERIC, -- Primary Diagnosis code for this charge.
  MODIFIER_ONE TEXT, -- The first procedure modifier of the associated transaction. This is the external modifier, as it would be printed on the claim.
  ASSIGNMENT_BEF_YN TEXT, -- This item is a Yes/No flag to determine if the transaction was assigned to insurance before the action on this line for this transaction.
  ASSIGNMENT_AFTER_YN TEXT, -- This item is a Yes/No flag to determine if the transaction was assigned to insurance after the action on this line for this transaction.
  ACTION_REMIT_CODES TEXT, -- This field stores a comma delimited list of external remittance codes for this transaction.
  ACTION_DATETIME TEXT, -- The UTC date and time the action was performed.
  PRIMARY KEY (TX_ID, LINE) -- Primary Key, FOREIGN KEY (TX_ID) REFERENCES ARPB_TRANSACTIONS (TX_ID)
, FOREIGN KEY (TX_ID) REFERENCES ARPB_TRANSACTIONS (TX_ID) -- References ARPB_TRANSACTIONS table
, FOREIGN KEY (POS_ID) REFERENCES CLARITY_LOC (LOC_ID) -- References CLARITY_LOC table
, FOREIGN KEY (DEPARTMENT_ID) REFERENCES CLARITY_DEP (DEPARTMENT_ID) -- References CLARITY_DEP table
, FOREIGN KEY (PROC_ID) REFERENCES CLARITY_EAP (PROC_ID) -- References CLARITY_EAP table
, FOREIGN KEY (LOCATION_ID) REFERENCES CLARITY_LOC (LOC_ID) -- References CLARITY_LOC table
, FOREIGN KEY (SERVICE_AREA_ID) REFERENCES CLARITY_SA (SERV_AREA_ID) -- References CLARITY_SA table
, FOREIGN KEY (SERVICE_AREA_ID) REFERENCES CLARITY_LOC (LOC_ID) -- References CLARITY_LOC table
, FOREIGN KEY (ACCOUNT_ID) REFERENCES ACCOUNT (ACCOUNT_ID) -- References ACCOUNT table
, FOREIGN KEY (PRIMARY_DX_ID) REFERENCES CLARITY_EDG (DX_ID) -- References CLARITY_EDG table

);
