CREATE TABLE ARPB_TX_STMCLAIMHX ( -- This table contains information about the statement and claim history for professional billing transactions.
  TX_ID NUMERIC, -- The unique ID of the transaction record
  LINE INTEGER, -- The line number for the information associated with this contact. Multiple pieces of information can be associated with this contact.
  BC_HX_TYPE_C_NAME TEXT, -- This item stores whether the current line is a bill or claim entry.  A category value of 1 means claim and 2 means bill.
  BC_HX_DATE TEXT, -- The date the statement or claim was processed. This can be NULL when an insurance payment posts and it can�t find a matching claim with that payer in the history.
  BC_HX_COVERAGE_ID NUMERIC, -- The unique ID of the coverage that is associated with the bill or claim run
  BC_HX_ASSIGNED_YN TEXT, -- Indicates whether or not the coverage is assigned to insurance for this transaction. Y indicates the coverage is assigned.
  BC_HX_AMOUNT NUMERIC, -- The amount of the transaction on the bill or claim.
  BC_HX_INVOICE_NUM TEXT, -- The invoice number for the bill or claim.
  BC_HX_PAYMENT_AMT NUMERIC, -- Payment amount for payment associated with this invoice.  This field applies to claims only.
  BC_HX_PAYMENT_DATE TEXT, -- Payment date for payment associated with this invoice.  This field applies to claims only.
  BC_HX_PAYOR_ID NUMERIC, -- The unique ID of the payor for this claim. This field applies to claims only.
  BC_HX_RESUBMIT_DATE TEXT, -- Resubmit date for this claim.  This field applies to claims only.
  BC_HX_CLM_DB_ID NUMERIC, -- The unique ID of the claim information record for this claim. This field applies to claims only.
  BC_HX_BO_PROC_ID NUMERIC, -- The internal record ID for the procedure billed out on the claim. This item only applies to claims.
  BC_HX_ACCEPT_DATE TEXT, -- Accept date for bill or claim
  BC_HX_FIRST_CLM_FLG TEXT, -- This item is set to 0 for accepted claims where all previous lines in the statement-claim history are claims. This item is set to 1 for accepted claims where there is at least one previous line in the statement-claim history that was a statement.  This item is only populated for claims.
  BC_HX_ACCEPT_DTTM TEXT, -- The UTC date and time the statement or claim was accepted.
  PRIMARY KEY (TX_ID, LINE) -- Primary Key, FOREIGN KEY (TX_ID) REFERENCES ARPB_TRANSACTIONS (TX_ID)
, FOREIGN KEY (TX_ID) REFERENCES ARPB_TRANSACTIONS (TX_ID) -- References ARPB_TRANSACTIONS table
, FOREIGN KEY (BC_HX_BO_PROC_ID) REFERENCES CLARITY_EAP (PROC_ID) -- References CLARITY_EAP table

);
