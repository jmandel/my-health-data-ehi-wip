CREATE TABLE CODE_INT_COMB_LN ( -- This table holds the combined service lines created by code integration.
  HSP_ACCOUNT_ID NUMERIC, -- The unique identifier (.1 item) for the hosp acct record.
  LINE INTEGER, -- The line number for the information associated with this record. Multiple pieces of information can be associated with this record.
  CODE_INT_REV_CODE_ID NUMERIC, -- This column stores the unique identifier for the revenue code for the combined service line.
  CODE_INT_REV_CODE_ID_REVENUE_CODE_NAME TEXT, -- The name of the revenue code.
  CODE_INT_CPT TEXT, -- This item holds the CPT(R)/HCPCS code for the combined service line.
  CODE_INT_MOD_1_ID TEXT, -- This column stores the unique identifier for the first modifier for the combined service line.
  CODE_INT_MOD_1_ID_MODIFIER_NAME TEXT, -- The name of the modifier record.
  CODE_INT_DATE TEXT, -- This item holds the service date for the combined service line.
  CODE_INT_QTY INTEGER, -- This item holds the quantity (number of units) for the combined service line.
  CODE_INT_AMT NUMERIC, -- This item holds the full charge amount for the combined service line. The value includes any non-covered amount for the line.
  CODE_INT_NONCVRD NUMERIC, -- This item holds the non-covered amount for the combined service line.
  CODE_INT_LN_SRC_C_NAME TEXT, -- This item holds the source for the CPT(R)/HCPCS code and modifiers for the combined service line.
  CODE_INT_UNUSED_YN TEXT, -- This item identifies lines that are not true service lines but represent coded CPT(R)/HCPCS codes that cannot be used to create actual service lines.
  CODE_INT_CHRG_CNT INTEGER, -- This item holds the number of charges associated with the combined service line.
  PRIMARY KEY (HSP_ACCOUNT_ID, LINE) -- Primary Key, FOREIGN KEY (HSP_ACCOUNT_ID) REFERENCES HAR_ALL (ACCT_ID)
, FOREIGN KEY (HSP_ACCOUNT_ID) REFERENCES HSP_ACCT_CLAIM_HAR (ACCT_ID)
, FOREIGN KEY (HSP_ACCOUNT_ID) REFERENCES HSP_ACCOUNT (HSP_ACCOUNT_ID)
, FOREIGN KEY (HSP_ACCOUNT_ID) REFERENCES HAR_ALL (ACCT_ID) -- References HAR_ALL table
, FOREIGN KEY (HSP_ACCOUNT_ID) REFERENCES HSP_ACCT_CLAIM_HAR (ACCT_ID) -- References HSP_ACCT_CLAIM_HAR table
, FOREIGN KEY (HSP_ACCOUNT_ID) REFERENCES HSP_ACCOUNT (HSP_ACCOUNT_ID) -- References HSP_ACCOUNT table
, FOREIGN KEY (CODE_INT_REV_CODE_ID) REFERENCES CL_UB_REV_CODE (UB_REV_CODE_ID) -- References CL_UB_REV_CODE table
, FOREIGN KEY (CODE_INT_MOD_1_ID) REFERENCES CLARITY_MOD (MODIFIER_ID) -- References CLARITY_MOD table

);
