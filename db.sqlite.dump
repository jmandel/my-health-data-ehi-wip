CREATE TABLE HSP_TX_DIAG ( -- This table contains hospital account transaction diagnoses from the Hospital Permanent Transactions (HTR) master file.
  TX_ID NUMERIC, -- This column stores the unique identifier for the hospital billing transaction.
  LINE INTEGER, -- The line number in the results of a query. Because multiple diagnosis codes can be associated with one charge, each diagnosis will have a unique line number.
  HSP_ACCOUNT_ID NUMERIC, -- This column stores the unique identifier for the hospital account associated with the hospital billing transaction.
  POST_DATE TEXT, -- The post date of a transaction.
  SERV_AREA_ID NUMERIC, -- The service area with which a transaction is associated.
  DX_ID NUMERIC, -- This column stores the unique identifier for the diagnosis code.
  DX_QUAL_HA_C_NAME TEXT, -- A qualifier entered with a diagnosis code in charge entry.
  PRIMARY KEY (TX_ID, LINE) -- Primary Key, FOREIGN KEY (TX_ID) REFERENCES HSP_TRANSACTIONS (TX_ID)
, FOREIGN KEY (TX_ID) REFERENCES HSP_TRANSACTIONS (TX_ID) -- References HSP_TRANSACTIONS table
, FOREIGN KEY (HSP_ACCOUNT_ID) REFERENCES HAR_ALL (ACCT_ID) -- References HAR_ALL table
, FOREIGN KEY (HSP_ACCOUNT_ID) REFERENCES HSP_ACCT_CLAIM_HAR (ACCT_ID) -- References HSP_ACCT_CLAIM_HAR table
, FOREIGN KEY (HSP_ACCOUNT_ID) REFERENCES HSP_ACCOUNT (HSP_ACCOUNT_ID) -- References HSP_ACCOUNT table
, FOREIGN KEY (SERV_AREA_ID) REFERENCES CLARITY_SA (SERV_AREA_ID) -- References CLARITY_SA table
, FOREIGN KEY (SERV_AREA_ID) REFERENCES CLARITY_LOC (LOC_ID) -- References CLARITY_LOC table
, FOREIGN KEY (DX_ID) REFERENCES CLARITY_EDG (DX_ID) -- References CLARITY_EDG table

);
