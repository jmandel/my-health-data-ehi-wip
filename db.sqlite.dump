CREATE TABLE ORDER_RPTD_SIG_HX ( -- This table contains a history of sig-related data for prescriptions, both what the provider initially prescribed and what the patient later reported taking. Most commonly, the first row for any prescription represents the sig as the prescription was written, and subsequent rows will represent changes in what the patient reports taking.-- Child table: ORDER_RPTD_SIG_INSTR (1 to 1 rows) joined on ORDER_ID, GROUP_LINE
  -- Child table: ORDER_RPTD_SIG_TEXT (1 to 1 rows) joined on ORDER_ID, GROUP_LINE

  ORDER_ID NUMERIC, -- The unique identifier for the order record.
  LINE INTEGER, -- The line number for the information associated with this record. Multiple pieces of information can be associated with this record.
  PAT_ENC_CSN_ID NUMERIC, -- The unique contact serial number for this contact. This number is unique across all patient encounters in your system. If you use IntraConnect, this is the Unique Contact Identifier (UCI).
  ENTRY_USER_ID TEXT, -- The unique identifier of the user who entered the medication instructions.
  ENTRY_USER_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  ENTRY_DTTM TEXT, -- The date and time when these medication instructions were entered.
  ACTION_C_NAME TEXT, -- The action represented by this sig - e.g. the initial prescription; the pharmacist has edited the sig; or the patient reports taking this medication differently from how it was prescribed.
  SOURCE_C_NAME TEXT, -- The location from which this sig was entered.
  DOSE_MIN NUMERIC, -- The lower bound of the ranged dose for this medication's instructions.
  DOSE_UNIT_C_NAME TEXT, -- The med & dose unit category ID for the dose of this medication's instructions.
  FREQUENCY_ID TEXT, -- The unique identifier of the frequency record used in the medication instructions.
  FREQUENCY_ID_FREQ_NAME TEXT, -- The name of the frequency record.
  ROUTE_C_NAME TEXT, -- The route for this medication instructions.
  PRIMARY KEY (ORDER_ID, LINE) -- Primary Key, FOREIGN KEY (ORDER_ID) REFERENCES ORDER_MED (ORDER_MED_ID)
, FOREIGN KEY (ORDER_ID) REFERENCES ORD_PRFLST_TRK (ORDER_ID)
, FOREIGN KEY (ORDER_ID) REFERENCES ORDER_MED (ORDER_MED_ID) -- References ORDER_MED table
, FOREIGN KEY (ORDER_ID) REFERENCES ORD_PRFLST_TRK (ORDER_ID) -- References ORD_PRFLST_TRK table
, FOREIGN KEY (PAT_ENC_CSN_ID) REFERENCES PAT_ENC (PAT_ENC_CSN_ID) -- References PAT_ENC table
, FOREIGN KEY (ENTRY_USER_ID) REFERENCES CLARITY_EMP (USER_ID) -- References CLARITY_EMP table

);
