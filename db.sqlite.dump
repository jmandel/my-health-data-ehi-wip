CREATE TABLE ABN_FOLLOW_UP ( -- This table stores the data related to the follow up done on an Advanced Beneficiary Notice (ABN).-- Child table: HNO_PLAIN_TEXT (1 to 24 rows) joined on NOTE_CSN_ID
  -- Child table: NOTE_CONTENT_INFO (1 to 1 rows) joined on NOTE_CSN_ID
  -- Child table: NOTES_TRANS_AUTH (1 to 1 rows) joined on NOTE_CSN_ID

  NOTE_CSN_ID NUMERIC, -- The contact serial number (CSN) of the contact.
  NOTE_ID TEXT, -- The unique ID of the note (HNO) record that contains the Advance Beneficiary Notice (ABN) information.
  CONTACT_DATE_REAL NUMERIC, -- A unique contact date in decimal format. The integer portion of the number indicates the date of contact. The digits after the decimal distinguish different contacts on the same date and are unique for each contact on that date. For example, .00 is the first/only contact, .01 is the second contact, etc.
  CONTACT_DATE TEXT, -- The date of this contact in calendar format.
  CONTACT_NUM TEXT, -- The number of this contact.
  CONTACT_SERIAL_NUM NUMERIC, -- The contact serial number (CSN) of the contact.
  COSIGN_REQUIRED_C_NAME TEXT, -- The cosign requirement for the current note contact.
  AUTH_LNKED_PROV_ID TEXT, -- The author's linked provider record.
  ENTRY_INSTANT_DTTM TEXT, -- UTC formatted instant of entry for a note.
  UPD_AUTHOR_INS_DTTM TEXT, -- UTC instant of update by a specific user.
  SPEC_NOTE_TIME_DTTM TEXT, -- The note's specified date paired with the specified time.
  NOTE_FILE_TIME_DTTM TEXT, -- UTC formatted instant of when a note is filed.
  AUTHOR_PRVD_TYPE_C_NAME TEXT, -- Author's provider type on a specific contact.
  NOTE_STATUS_C_NAME TEXT, -- The status of the note.
  UPDATE_USER_ID TEXT, -- The id of the user who updated this contact of the note.
  UPDATE_USER_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  TRN_DOC_AVAIL_STA_C_NAME TEXT, -- The availability status of the transcription.
  TRN_DOC_TYPE_C_NAME TEXT, -- The document type of the transcription.
  SENSITIVE_STAT_C_NAME TEXT, -- Sensitive status of a note.
  AUTHOR_USER_ID TEXT, -- The unique ID associated with the user who is the author of the note.
  AUTHOR_USER_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  NOTE_FORMAT_C_NAME TEXT, -- The format of the note text like Plain Text, Rich Text, HTML etc.
  UPD_BY_AUTH_DTTM TEXT, -- The instant when the note is updated by the author.
  AUTH_STAT_C_NAME TEXT, -- The authentication status category number for this activity if this note is a transcription. This is also known as the completion status.
  UPD_AUT_LOCAL_DTTM TEXT, -- Update by author instant in local format.
  ENT_INST_LOCAL_DTTM TEXT, -- Note entry instant in local format.
  SPEC_TIME_LOC_DTTM TEXT, -- Note specified instant in local format.
  NOT_FILETM_LOC_DTTM TEXT, -- Note file time in local format.
  DOCUMENT_NAME TEXT, -- Contains the name of the multi-part document.
  PRE_UCN_NOTE_TYPE_C_NAME TEXT, -- This virtual item is populated with a category value from the note type (I HNO 50) according to the following logic:   * if the note type (I HNO 50) is populated, use the value directly * if the note type (I HNO 50) is null and the note is not ambulatory, return null   * if the note type (I HNO 50) is null and the note has an ambulatory encounter context, obtain a category from the UCN note type (I HNO 34033) and map that value to an equivalent category from the note type (I HNO 50), if possible
  NOTE_SHARED_W_PAT_HX_YN TEXT, -- Was this note contact marked as eligible for sharing with the patient when it was last saved?  Notes will only be displayed in MyChart if their most recent contact is marked for sharing. If you want to determine if a note is currently shared, use the NOTE_SHARED_W_PAT_YN column in the HNO_INFO table instead of this one.
  NOTE_TYPE_C_NAME TEXT, -- Identifies what type of note this record is.
  IS_PRECHARTED_YN TEXT, -- This indicates whether or not the note is currently a pre-charted note (in appointment encounter).
  PRIMARY KEY (NOTE_CSN_ID) -- Primary Key, FOREIGN KEY (NOTE_ID) REFERENCES HNO_INFO (NOTE_ID) -- References HNO_INFO table
, FOREIGN KEY (AUTH_LNKED_PROV_ID) REFERENCES CLARITY_SER (PROV_ID) -- References CLARITY_SER table
, FOREIGN KEY (UPDATE_USER_ID) REFERENCES CLARITY_EMP (USER_ID) -- References CLARITY_EMP table
, FOREIGN KEY (AUTHOR_USER_ID) REFERENCES CLARITY_EMP (USER_ID) -- References CLARITY_EMP table

);
