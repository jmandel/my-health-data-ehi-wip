CREATE TABLE REFERRAL_DX ( -- The REFERRAL_DX table contains diagnosis information stored with referrals.
  REFERRAL_ID NUMERIC, -- The referral ID for the referral record.
  LINE INTEGER, -- The line number of the diagnosis associated with the referral. For example, if a referral has two associated diagnoses, the first diagnosis will have a line value of 1, while the second diagnosis will have a line value of 2.
  DX_ID NUMERIC, -- The ID number of the diagnosis associated with the referral. This is not the diagnosis code.  NOTE: Link to CLARITY_EDG to get the diagnosis code.
  PRIMARY KEY (REFERRAL_ID, LINE) -- Primary Key, FOREIGN KEY (REFERRAL_ID) REFERENCES REFERRAL (REFERRAL_ID)
, FOREIGN KEY (REFERRAL_ID) REFERENCES REFERRAL (REFERRAL_ID) -- References REFERRAL table
, FOREIGN KEY (DX_ID) REFERENCES CLARITY_EDG (DX_ID) -- References CLARITY_EDG table

