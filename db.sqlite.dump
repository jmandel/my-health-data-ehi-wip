CREATE TABLE SDD_ENTRIES ( -- This table stores basic info about Social Determinant entries. Each row represents one documentation of a need or risk for the patient in a given domain. This data includes the score that defines the severity of this need or risk.
  SDOH_DATA_ID NUMERIC, -- The unique identifier (.1 item) for the social determinant data record.
  CONTACT_DATE_REAL NUMERIC, -- A unique contact date in decimal format. The integer portion of the number indicates the date of contact. The digits after the decimal distinguish different contacts on the same date and are unique for each contact on that date. For example, .00 is the first/only contact, .01 is the second contact, etc.
  LINE INTEGER, -- The line number for the information associated with this contact. Multiple pieces of information can be associated with this contact.
  CONTACT_DATE TEXT, -- The date of this contact in calendar format.
  ENTRY_DOM_CONFIG_ID NUMERIC, -- Stores the source SDC that this entry contains data for.
  ENTRY_EFFECTIVE_UTC_DTTM TEXT, -- Stores the instant at which an entry in SDD was considered active.
  ENTRY_CONCERN_LVL_C_NAME TEXT, -- Stores the level of concern for this entry on a scale of low, medium, or high (or unknown).
  PRIMARY KEY (SDOH_DATA_ID, CONTACT_DATE_REAL, LINE) -- Primary Key, FOREIGN KEY (ENTRY_DOM_CONFIG_ID) REFERENCES SDOH_DOM_CONFIG_INFO (DOM_CONFIG_ID) -- References SDOH_DOM_CONFIG_INFO table

);
