CREATE TABLE IP_FLWSHT_MEAS ( -- This table contains the patient-specific measurements from flowsheets.
  FSD_ID TEXT, -- The unique ID for the flowsheet data record.
  LINE INTEGER, -- The line count for the item.
  OCCURANCE INTEGER, -- If the flowsheet group/row appears multiple times, this will distinguish the occurrence.
  RECORDED_TIME TEXT, -- The instant the reading was taken.
  ENTRY_TIME TEXT, -- The instant the reading was entered.
  TAKEN_USER_ID TEXT, -- The unique ID of the user taking the flowsheet readings.
  TAKEN_USER_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  ENTRY_USER_ID TEXT, -- The unique ID of the user entering the readings.
  ENTRY_USER_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  EDITED_LINE INTEGER, -- The line number of the previous value of an edited record.
  ISACCEPTED_YN TEXT, -- Determines if this flowsheet record has been accepted.
  NEEDS_COSIGN_C_NAME TEXT, -- If this item is blank or 0 (No), then this flowsheet data does not need a cosign.  If this item is 1 (Required Yes), then a cosign is required for this data and can only be pended.  If this item is 2 (Chosen Yes), then a cosign has been requested for this data but it is not required.  If this item is 3 (Required Yes Can File), then a cosign is required for the data and the data can be filed.
  FLT_ID TEXT, -- The unique ID of the flowsheet template (FLT) which was used to enter the data in this cell.
  FLT_ID_DISPLAY_NAME TEXT, -- The display name associated with this template.
  FLO_DAT_USED NUMERIC, -- This column stores the contact date (DAT) of the flowsheet row or group that is used to define the data.
  FLO_CNCT_DATE_REAL NUMERIC, -- This column converts the contact date (DAT) of the flowsheet group or row to DTE, based on the value in column FLO_DAT_USED.
  USER_PENDED_BY_ID TEXT, -- User ID of the user who pended this flowsheet value.
  USER_PENDED_BY_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  INSTANT_PENDED_DTTM TEXT, -- Date/time at which a flowsheet value is pended.
  ABNORMAL_C_NAME TEXT, -- Stores whether or not the value is abnormal
  PAT_REPORTED_STATUS_C_NAME TEXT, -- Indicates if the data was directly entered by the patient or a patient proxy and whether the data has been validated by a clinician
  MYPT_ID TEXT, -- The MyChart account from which the data was entered.
  ABNORMAL_TYPE_C_NAME TEXT, -- This column stores metadata for abnormal flowsheet values. It is only populated for data where ABNORMAL_C - Abnormal is set to 1-Yes. It is set to 1-Low for data that is below the minimum warning level and it is set to 2-High for data that is above the maximum warning level for flowsheet data of types: numeric, blood pressure, temperature, height, patient height, weight and patient weight. It is set to 0-Unspecified for data that is abnormal for other reasons. This is the only value that can be set for flowsheet data of type: custom list.
  FLO_NETWORKED_INI TEXT, -- The INI to which the value for this row is associated.
  PRIMARY KEY (FSD_ID, LINE) -- Primary Key, FOREIGN KEY (FSD_ID) REFERENCES IP_FLWSHT_REC (FSD_ID)
, FOREIGN KEY (FSD_ID) REFERENCES IP_FLWSHT_REC (FSD_ID) -- References IP_FLWSHT_REC table
, FOREIGN KEY (TAKEN_USER_ID) REFERENCES CLARITY_EMP (USER_ID) -- References CLARITY_EMP table
, FOREIGN KEY (ENTRY_USER_ID) REFERENCES CLARITY_EMP (USER_ID) -- References CLARITY_EMP table
, FOREIGN KEY (FLT_ID) REFERENCES IP_FLT_DATA (TEMPLATE_ID) -- References IP_FLT_DATA table

);
