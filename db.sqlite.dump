CREATE TABLE COVERAGE_BENEFITS ( -- The COVERAGE_BENEFITS table contains coverage-level benefit information for the encounter or estimate to which this benefit record is attached.
  RECORD_ID NUMERIC, -- The unique identifier (.1 item) for the benefit record.
  LINE INTEGER, -- The line number for the information associated with this record. Multiple pieces of information can be associated with this record.
  CVG_ID NUMERIC, -- Each line in this related group contains benefit information for a coverage that is or was in use for this visit.
  CVG_BEN_VERIF_ID NUMERIC, -- The verification record for visit-specific coverage benefits.
  DEDUCTIBLE_AMOUNT NUMERIC, -- Deductible for this coverage.
  DEDUCT_REMAIN_AMT NUMERIC, -- The amount of the deductible for this coverage that has yet to be paid.
  OUT_OF_POCKET_MAX NUMERIC, -- The maximum out-of-pocket payment for this coverage.
  OUT_OF_PCKT_REMAIN NUMERIC, -- The remaining out-of-pocket payment for this coverage.
  CVG_UPDATE_DTTM TEXT, -- This item contains the last instant that data related to this coverage was written to the record. This includes changes to general benefits, changes to service type specific benefits, and copying from another benefits record.
  CVG_UPDATE_USER_ID TEXT, -- This item contains the ID of the user who last saved data related to this coverage to the record, or whose actions caused the system to save that data. Updates when general benefits or service type specific benefits are saved or when benefits are copied from one benefit record to this one.
  CVG_UPDATE_USER_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  CVG_UPDATE_SRC_C_NAME TEXT, -- The method that was last used to make changes to the general benefits values. This reflects the original entry of the benefits, even if the entry was made on a different benefits record and the benefits were later copied to the current one. This only tracks changes to the general benefits, not to service type benefits.
  FAMILY_TIER_C_NAME TEXT, -- Family tier for this line's coverage.
  NET_LVL_CVG_C_NAME TEXT, -- Network level for this line's coverage.
  CALC_NET_LVL_CVG_C_NAME TEXT, -- Stores the most recently calculated network level for this coverage.
  CALC_NET_SRC_CVG_C_NAME TEXT, -- Stores the source of the most recently calculated network level for this coverage.
  RTE_DEDUCT_AMOUNT NUMERIC, -- Stores the last deductible amount filed by RTE at the coverage level
  RTE_DEDUCT_REMAIN NUMERIC, -- Stores the last deductible remaining amount filed by RTE at the coverage level
  RTE_OOP_MAX NUMERIC, -- Stores the last OOP max amount filed by RTE at the coverage level
  RTE_OOP_REMAIN NUMERIC, -- Stores the last OOP remaining amount filed by RTE at the coverage level
  PRIMARY KEY (RECORD_ID, LINE) -- Primary Key, FOREIGN KEY (RECORD_ID) REFERENCES BENEFITS (RECORD_ID)
, FOREIGN KEY (RECORD_ID) REFERENCES BENEFITS (RECORD_ID) -- References BENEFITS table
, FOREIGN KEY (CVG_ID) REFERENCES COVERAGE (COVERAGE_ID) -- References COVERAGE table

);
