CREATE TABLE CL_RMT_SVCE_LN_INF ( -- This table contains service line information from the remittance image.
  IMAGE_ID TEXT, -- This is the ID for the remittance image record with remittance service line information.
  LINE INTEGER, -- The line number in the results of a query.  Each instance of service line information will have its own line.
  SERVICE_LINE TEXT, -- Service line information for claim remittance.
  PROC_IDENTIFIER TEXT, -- The composite medical procedure identifier to identify a medical procedure by its standardized codes for service line information.
  LINE_ITEM_CHG_AMT NUMERIC, -- Monetary amount for submitted service line item charge.
  PROV_PAYMENT_AMT NUMERIC, -- Monetary amount for the service line item provider payment amount.
  NUBC_REV_CD TEXT, -- National uniform billing committee revenue code for service line information.
  UNITS_PAID_CNT NUMERIC, -- Count of the Units of Service Paid for service line information.
  ORIG_UNITS_CNT NUMERIC, -- Original units of service count for service line information.
  SVC_LINE_CHG_PB_ID NUMERIC, -- ID for professional billing service line charge.
  SVC_DATE_QUAL_C_NAME TEXT, -- This is the Code specifying type of service date.
  SERVICE_DATE TEXT, -- This is the service date.
  SERVICE_LN TEXT, -- This is the service line for which service date is specified
  REF_SVC_LN TEXT, -- This is the service line for reference segment.
  SVC_REF_ID_QUAL_C_NAME TEXT, -- This is the service line level reference segment ID qualifier.
  SVC_REF_IDENTIFIER TEXT, -- This is the service line level reference segment Identifier.
  PRIMARY KEY (IMAGE_ID, LINE) -- Primary Key, FOREIGN KEY (IMAGE_ID) REFERENCES CL_REMIT (IMAGE_ID)
, FOREIGN KEY (IMAGE_ID) REFERENCES CL_REMIT (IMAGE_ID) -- References CL_REMIT table
, FOREIGN KEY (SVC_LINE_CHG_PB_ID) REFERENCES ARPB_TRANSACTIONS (TX_ID) -- References ARPB_TRANSACTIONS table

);
