  PRIMARY KEY (PAT_ENC_CSN_ID, GROUP_LINE, VALUE_LINE) -- Primary Key, FOREIGN KEY (PAT_ENC_CSN_ID, GROUP_LINE) REFERENCES PAT_HX_REVIEW (PAT_ENC_CSN_ID, LINE_COUNT)
, FOREIGN KEY (PAT_ENC_CSN_ID, GROUP_LINE) REFERENCES SOCIAL_ADL_HX (PAT_ENC_CSN_ID, LINE)
, FOREIGN KEY (PAT_ENC_CSN_ID) REFERENCES PAT_ENC (PAT_ENC_CSN_ID)
, FOREIGN KEY (PAT_ENC_CSN_ID) REFERENCES SOCIAL_HX (PAT_ENC_CSN_ID)
, FOREIGN KEY (PAT_ENC_CSN_ID) REFERENCES PAT_ENC (PAT_ENC_CSN_ID) -- References PAT_ENC table
, FOREIGN KEY (PAT_ENC_CSN_ID) REFERENCES SOCIAL_HX (PAT_ENC_CSN_ID) -- References SOCIAL_HX table

);
