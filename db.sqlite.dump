CREATE TABLE ORDER_PROC ( -- The ORDER_PROC table enables you to report on the procedures ordered in the clinical system. We have also included patient and contact identification information for each record.-- Child table: FINALIZE_PHYSICIAN (1 to 1 rows) joined on ORDER_ID
  -- Child table: OBS_MTHD_ID (1 to 12 rows) joined on ORDER_ID
  -- Child table: ORD_CLIN_IND (1 to 2 rows) joined on ORDER_ID
  -- Child table: ORDER_ANATOMICAL_REGION (3 to 3 rows) joined on ORDER_ID
  -- Child table: ORDER_COMMENT (1 to 2 rows) joined on ORDER_ID
  -- Child table: ORDER_DOCUMENTS (1 to 1 rows) joined on ORDER_ID
  -- Child table: ORDER_DX_PROC (1 to 2 rows) joined on ORDER_PROC_ID
  -- Child table: ORDER_IMPRESSION (2 to 2 rows) joined on ORDER_PROC_ID
  -- Child table: ORDER_INSTANTIATED (1 to 1 rows) joined on ORDER_ID
  -- Child table: ORDER_MODALITY_TYPE (1 to 1 rows) joined on ORDER_ID
  -- Child table: ORDER_MYC_RELEASE (1 to 1 rows) joined on ORDER_PROC_ID
  -- Child table: ORDER_NARRATIVE (10 to 15 rows) joined on ORDER_PROC_ID
  -- Child table: ORDER_RAD_ACC_NUM (1 to 2 rows) joined on ORDER_PROC_ID
  -- Child table: ORDER_RAD_READING (1 to 1 rows) joined on ORDER_PROC_ID
  -- Child table: ORDER_READ_ACK (1 to 3 rows) joined on ORDER_ID
  -- Child table: ORDER_RES_COMMENT (2 to 8 rows) joined on ORDER_ID
  -- Child table: ORDER_RESULTS (1 to 12 rows) joined on ORDER_PROC_ID
  -- Child table: ORDER_SIGNED_PROC (1 to 1 rows) joined on ORDER_PROC_ID
  -- Child table: ORD_INDICATIONS (1 to 1 rows) joined on ORDER_ID
  -- Child table: ORD_PROC_INSTR (9 to 9 rows) joined on ORDER_ID
  -- Child table: ORD_RSLT_COMPON_ID (1 to 1 rows) joined on ORDER_ID
  -- Child table: ORD_SPEC_QUEST (1 to 2 rows) joined on ORDER_ID
  -- Child table: PERFORMING_ORG_INFO (1 to 1 rows) joined on ORDER_ID
  -- Child table: RIS_SGND_INFO (1 to 1 rows) joined on ORDER_PROC_ID
  -- Child table: SPEC_SOURCE_SNOMED (1 to 1 rows) joined on ORDER_ID
  -- Child table: SPEC_TYPE_SNOMED (1 to 1 rows) joined on ORDER_ID
  -- Child table: ORDER_REVIEW (1 to 3 rows) joined on ORDER_ID

  ORDER_PROC_ID NUMERIC, -- The unique ID of the order record associated with this procedure order.
  PAT_ID TEXT, -- The unique ID of the patient record for this order. This column is frequently used to link to the PATIENT table.
  PAT_ENC_DATE_REAL NUMERIC, -- A unique, internal contact date in decimal format. The integer portion of the number indicates the date of the contact. The digits after the decimal distinguish different contacts on the same date and are unique for each contact on that date. For example, .00 is the first/only contact, .01 is the second contact, etc.
  PAT_ENC_CSN_ID NUMERIC, -- The unique contact serial number for this contact. This number is unique across patients and encounters in your system. If you use IntraConnect this is the Unique Contact Identifier (UCI).
  RESULT_LAB_ID NUMERIC, -- The unique ID of the lab or other resulting agency, such as radiology, that provided the order results.
  RESULT_LAB_ID_LLB_NAME TEXT, -- Interface laboratory name.
  ORDERING_DATE TEXT, -- The date when the procedure order was placed.
  ORDER_TYPE_C_NAME TEXT, -- The order type category number for the procedure order.
  PROC_ID NUMERIC, -- The unique ID of the  procedure record corresponding to this order. This can be used to link to CLARITY_EAP.
  DESCRIPTION TEXT, -- A brief summary of the procedure order.
  ORDER_CLASS_C_NAME TEXT, -- The order class category number of the procedure order.
  AUTHRZING_PROV_ID TEXT, -- The unique ID of the provider prescribing or authorizing the order.
  ABNORMAL_YN TEXT, -- Indicates whether or not this order contains abnormal results. This column will contain a Y if there are abnormal results and an N or null if it does not. For orders with lab component results, if any one component of this order has an abnormal result value then this will hold a Y.
  BILLING_PROV_ID TEXT, -- The unique ID of the provider under whose name this order should be billed. This might be the same ID as the AUTHRZING_PROV_ID.
  ORD_CREATR_USER_ID TEXT, -- The unique identifier of the user who signed the order, or the last person who performed a sign and hold or release action for a signed and held order.
  ORD_CREATR_USER_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  LAB_STATUS_C_NAME TEXT, -- The category number for the status of results for an order, as of the date and time the record was extracted.
  ORDER_STATUS_C_NAME TEXT, -- The order status category number of the procedure order.
  QUANTITY INTEGER, -- The number of procedures authorized for this order.
  REASON_FOR_CANC_C_NAME TEXT, -- The reason for cancellation category number for the procedure order.
  FUTURE_OR_STAND TEXT, -- This column indicates whether an order is a future (F) or standing (S) order.
  STANDING_EXP_DATE TEXT, -- The date when a recurring procedure order expires.
  FUT_EXPECT_COMP_DT TEXT, -- The date by which each future procedure order should be completed. Displayed in calendar format.
  STAND_ORIG_OCCUR INTEGER, -- The total number of occurrences that a recurring order was authorized for.
  REFERRING_PROV_ID TEXT, -- The unique ID of the provider who has referred this order, i.e. the referring provider.
  REFERRING_PROV_ID_REFERRING_PROV_NAM TEXT, -- The name of the referral source.
  REFD_TO_LOC_ID NUMERIC, -- The unique ID of the location record to which this patient was referred.
  REQUESTED_SPEC_C_NAME TEXT, -- The medical specialty category number of the provider to which the patient was referred for the procedure order.
  RFL_CLASS_C_NAME TEXT, -- The referral class category number for the procedure order.
  RFL_TYPE_C_NAME TEXT, -- The referral type category number for the procedure order.
  RSN_FOR_RFL_C_NAME TEXT, -- The reason for referral category number for the procedure order.
  RFL_NUM_VIS INTEGER, -- The number of visits this referral order is authorized for.
  RADIOLOGY_STATUS_C_NAME TEXT, -- The category ID for the imaging study status (e.g. technician ended the exam, reading physician finalized the exam) of the procedure order.
  PROC_BGN_TIME TEXT, -- The date and time when the procedure order (exam) is to begin.
  ORDER_INST TEXT, -- The instant when the order was created.
  DISPLAY_NAME TEXT, -- The name of the order as it appears in the patient's record.
  HV_HOSPITALIST_YN TEXT, -- Indicates whether or not this order was placed by a hospitalist. �Y�  indicates that this order was placed by a hospitalist. �N� or NULL indicate that this order was not placed by a hospitalist.
  ORDER_PRIORITY_C_NAME TEXT, -- The overall priority category number for the procedure order.
  INSTANTIATED_TIME TEXT, -- The date and time of instantiation when a child order is generated from a standing or future order.
  INSTNTOR_USER_ID TEXT, -- The unique ID of the user who instantiated the order.
  INSTNTOR_USER_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  DEPT_REF_PROV_ID NUMERIC, -- The unique ID of the department to which this order is referred.
  SPECIALTY_DEP_C_NAME TEXT, -- The category value for the requested medical specialty of the department to which the patient is referred.
  SPECIMEN_TYPE_C_NAME TEXT, -- The specimen type category number for the procedure order.
  ORDER_TIME TEXT, -- The date and time when the procedure order was placed.
  RESULT_TIME TEXT, -- The most recent date and time when the procedure order was resulted.
  IS_PENDING_ORD_YN TEXT, -- Indicates whether or not the order has a pending status. 'Y' indicates that the order has a pending status. 'N' or NULL indicates that the order does not have a pending status.
  PROC_START_TIME TEXT, -- The date and time when the procedure order is to start.
  PROBLEM_LIST_ID NUMERIC, -- The unique ID of the problem list record that is associated with this order. This column is mainly used for immunization orders.
  PROC_ENDING_TIME TEXT, -- The date and time when the procedure order is to end.
  SESSION_KEY TEXT, -- The unique key associated with the order at the time of signing.  Other orders will share this key if they were signed at the same time.
  STUDY_INSTANCE TEXT, -- This column stores the unique identifier for an instance of a DICOM imaging study.
  PAT_LOC_ID NUMERIC, -- The unique identifier of the department where the patient is located at the time this order was signed,
  ACT_ORDER_C_NAME TEXT, -- The active order category ID, which indicates information about the order status.
  ORDER_SOURCE_C_NAME TEXT, -- The order source category ID of the order, indicating where in the EHR the order was entered.
  SPECIMN_TAKEN_DATE TEXT, -- The date the specimen was taken.
  SPECIMN_TAKEN_TIME TEXT, -- The time the specimen was taken.
  SPECIMEN_COMMENTS TEXT, -- This free text item is used to store comments about the specimen source.
  SPECIMEN_RECV_DATE TEXT, -- This item stores the date that the specimen was received. It is in the same logical group as the SPECIMEN_RECV_TIME column.
  SPECIMEN_RECV_TIME TEXT, -- This item stores the time the specimen was received. It is in the same logical group as the SPECIMEN_RECV_DATE column.
  COLLECTOR_IDN TEXT, -- The name of the user who collected the specimen for the order.
  FUTURE_APPROX_DT_YN TEXT, -- Indicates whether there is an approximate future expected date for this order. 'Y' indicates there is an approximate future expected date. 'N' or NULL indicates that there is not an approximate future expected date.
  LAST_STAND_PERF_DT TEXT, -- The date on which a standing order was last performed.
  LAST_STAND_PERF_TM TEXT, -- The time at which a standing order was last performed.
  EXTERNAL_ORD_ID TEXT, -- Stores the external order ID for the order.
  REMARKS_HNO_ID TEXT, -- Stores the link to the general notes record containing the result remarks. It is networked to General Use Notes record.
  SER_ADDRESSID TEXT, -- Stores the referring provider address ID for referral orders. The format is provider external ID - Address line number. For example, if provider external ID = 123 and Address line = 4, the value would be 123-4. If the referring provider has no address, this will store the provider external ID only.  Other columns in the same logical group are: REFG_FACILITY_ID (ORD 3010), REFD_TO_PROV_ID(ORD 3100) and REFERRAL_ID (ORD 3300)
  REFG_FACILITY_ID NUMERIC, -- Stores the referring facility for referral orders. This is networked to the facility record. Other columns in the same logical group are: SER_ADDRESSID (ORD 3001), REFD_TO_PROV_ID(ORD 3100) and REFERRAL_ID (ORD 3300)
  REFERRAL_ID NUMERIC, -- Stores the associated referral ID for referral orders. Other columns in the same logical group are: SER_ADDRESSID (ORD 3001), REFG_FACILITY_ID (ORD 3010) and REFD_TO_PROV_ID(ORD 3100)
  LAST_RESULT_UPD_TM TEXT, -- The date and time of the last result date. This item is set based on a time stamp received in the interface. It is also set when results are manually entered or when an order is cancelled.
  LOGIN_DEP_ID NUMERIC, -- The unique identifier for the login department used by the user signing the order.
  PROTCL_STAT_DT TEXT, -- The date an unscheduled order was placed, or the appointment date for a scheduled order which requires a protocol.
  PROTCL_STATE_C_NAME TEXT, -- The scheduling status for an order which requires a protocol.  This column links to the ZC_PROTCL_STATE table.
  INTERFACE_YN TEXT, -- Indicates whether this order was resulted through an interface or not. Y indicates that the result came through an interface, N indicates it was not resulted through an interface but rather some other result mechanism or not resulted at all.
  SOURCE_OF_PRI_INS_C_NAME TEXT, -- The source of prioritized date category ID for the order record. Prioritized date is determined by the following hierarchy: collection date and time, result date and time, release date and time (for future and standing orders), order date and time, and encounter date.
  PRIORITIZED_INST_TM TEXT, -- The time and date that is used as the prioritized date.
  ORDER_QUESN_LIST TEXT, -- The list of question records associated with this order.
  ORDER_ID NUMERIC, -- The unique identifier for the order record.
  ORDERING_MODE_C_NAME TEXT, -- This indicates whether an order is an inpatient or outpatient order. Note that Outpatient orders can be placed from an Inpatient encounter as discharge orders. This column might be blank for Outpatient orders placed prior to the creation of the IP module. This column is used to link to the ZC_ORDERING_MODE table.
  PROV_STATUS_C_NAME TEXT, -- The provider status category number for the order at the time of the extract. This item reflects the providers' viewed status of the order result message.  The amount, frequency and type of data stored in this item depends on the programming point records entered into the results message type definition in use at each facility. This column is used to link to the ZC_PROV_STATUS table.
  RESULT_TYPE_C_NAME TEXT, -- The result type category number for the order, if noted. A null value indicates that it is normal order results. This column is used to link to the ZC_ORD_RESULT_TYPE table.
  RFL_PRIORITY_C_NAME TEXT, -- The priority level category number of a referral order, which is used to specify whether a referral order is routine, urgent, emergency or elective. This column is used to link to the ZC_RFL_PRIORITY table.
  SIGN_ACTION_PEND_C_NAME TEXT, -- Sign action for pended order.
  STAND_EOW_ID TEXT, -- Holds the ID number of the Standing Status In Basket message associated with this Order.  The In Basket message informs the user that a standing order exists.
  INPAT_AUTO_RLSE_YN TEXT, -- This item indicates whether child instances of a standing order should be automatically released for inpatient orders.
  UNITS_REQUESTED INTEGER, -- The number of units requested for the service.
  UNITS_APPROVED INTEGER, -- The number of units approved for the service.
  ORD_CONDITION_FLAG INTEGER, -- This column contains the a Condition Flag if this is an order created from certain condition.
  NOCHRG_EXT_RSLT_YN TEXT, -- This column returns whether the order is an external result that should not drop charges. A value of 1 returns Y. A value of 0 returns N. A null value will return null but is treated the same as 0 when dropping charges.
  PROTOCOL_STATUS_C_NAME TEXT, -- Contains the current status of the order's protocols. Will be used to determine how to populate the protocol work list.
  PROC_LNC_ID NUMERIC, -- LOINC ID associated with the procedure.
  ABNORMAL_NOADD_YN TEXT, -- Indicates whether the most recent result is abnormal. 'Y' indicates that the most recent update of the result was marked abnormal. 'N' indicates that the abnormal flag is not set, or marked as normal. This is set to 'N' when a result is in progress, and will be updated as result updates are filed.
  SCREENING_FORM_ID NUMERIC, -- The unique ID of the screening form linked to the order.
  SCHED_TOL_NO_RESTR_BEF_YN TEXT, -- Indicates if the No restriction checkbox is checked for scheduling tolerance before the expected treatment date for a schedulable procedure. 'Y' indicates there are no restrictions.
  SCHED_TOL_NO_RESTR_AFTR_YN TEXT, -- Indicates if the No restriction checkbox is checked for scheduling tolerance after the expected treatment date for a schedulable procedure. 'Y' indicates there are no restrictions.
  FUTURE_RELATIVE_EXPECTED_DT_C_NAME TEXT, -- Holds a category value for the expected completion date. This may be subtly different from the expected date (ORDER_PROC.FUT_EXPECT_COMP_DT) for things like "in 3 months", which could be Start Date+90 days (S+90), Start date+91 days (S+91), or Start date + 92 days (S+92) depending on the current date.
  ORDER_INST_UTC_DTTM TEXT, -- The instant when the order was created in UTC.
  IMG_PUBLIC_RSLT_DTTM TEXT, -- The instant in local time at which the imaging result was made public, as defined by the order's study status (e.g. physician finalized the exam) as configured by the imaging analyst team (I RDF 192).
  ACTIVE_PROC_TYPE_C_NAME TEXT, -- This item holds the category type of the active procedure order. Only active procedure type orders will be contained in this item. Medications are excluded. The categories separate order type in parent/child/normal and IP/OP order type.
  RAD_EXAM_END_UTC_DTTM TEXT, -- The date and time an order's exam is ended in the Universal Time Coordinated (UTC) format.
  PAT_AGE_AT_EXAM INTEGER, -- The age of the patient (in years) as of the date of the exam. If the exam has ended, this will be the age as of end exam. If not, this will be the age as of the scheduled appointment date. If an appointment has not been scheduled for this exam, this value will be null.
  PRIORITIZED_UTC_DTTM TEXT, -- Stores the prioritized instant for the result in UTC
  RESULT_UPDATE_UTC_DTTM TEXT, -- Stores the last update instant for a result in UTC
  PERFORMED_IN_ISO_YN TEXT, -- Stores whether the imaging exam was performed in isolation.
  HAS_LAB_SPEC_YN TEXT, -- Indicates whether the order or any of the linked performable orders have a lab specimen. 'Y' indicates that the order or one of the linked performable orders has a lab specimen. 'N' indicates that the order does not have a linked lab specimen and no linked performable order has a lab specimen.
  HAS_RSLT_CNCT_YN TEXT, -- Indicates whether the order has a resulted contact.  'Y' indicates that the order has at least one contact of type 2-Resulted.  'N' indicates that the order does not have any contacts of type 2-Resulted.
  HAS_CORR_YN TEXT, -- Indicates whether the order has a correction. 'Y' indicates that the order has at least one contact with procedure result status equal to 4-Edited or 5-Edited Result - FINAL.  'N' indicates that the order does not have any contacts with procedure result status equal to 4-Edited or 5-Edited Result - FINAL.
  LAST_RSLT_LAB_ID NUMERIC, -- The unique ID of the resulting lab from the last contact where the procedure result status is not null.
  LAST_RSLT_LAB_ID_LLB_NAME TEXT, -- Interface laboratory name.
  FIRST_CHART_USER_ID TEXT, -- The unique ID of the user who first made results available on the chart.
  FIRST_CHART_USER_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  FIRST_CHART_UTC_DTTM TEXT, -- The date and time results were first made available on the chart.
  LAST_CHART_USER_ID TEXT, -- The unique ID of the user who last made results available on the chart.
  LAST_CHART_USER_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  LAST_CHART_UTC_DTTM TEXT, -- The date and time results were last made available on the chart.
  FIRST_FINAL_USER_ID TEXT, -- The unique ID of the user who first made final results available on the chart.
  FIRST_FINAL_USER_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  FIRST_FINAL_UTC_DTTM TEXT, -- The date and time final results were first made available on the chart.
  LAST_FINAL_USER_ID TEXT, -- The unique ID of the user who last made final results available on the chart.
  LAST_FINAL_USER_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  LAST_FINAL_UTC_DTTM TEXT, -- The date and time final results were last made available on the chart.
  PRIORITIZED_INST_DTTM TEXT, -- This item stores the prioritized instant (date and time) for an order in local time zone. It represents the most relevant date and time an action was taken on an order.
  PRIORITIZED_INST_UTC_DTTM TEXT, -- This item stores the prioritized instant (date and time) for an order in UTC time zone. It represents the most relevant date and time an action was taken on an order.
  RSLT_UPD_UTC_DTTM TEXT, -- Stores the last update instant for a result in UTC
  RECEIVED_EXT_AUTH_PROV TEXT, -- This item holds the free-text for the received authorizing provider's name. It is free text and not linked to a provider record because we do not want to generate provider records for all received external result orders.
  STUDY_READ_DATE TEXT, -- Stores the last time an imaging order had a status change of "SAVED" or higher.
  IMG_ADDEND_IN_EPIC_DATE TEXT, -- This column stores when the addendum is updated in Epic (addendum sign date if using an Epic workflow, otherwise whenever received over the interface)
  PERFORM_BY_DATE TEXT, -- Unbuffered perform-by date used in automatic order cancellation batch job.
  DISCON_LOC_DTTM TEXT, -- The instant the order was discontinued or canceled in the local timezone.
  FIRST_FINAL_LOC_DTTM TEXT, -- The instant final results were first made available on the chart. This is the result contact instant (ORD-1970) from the first contact where the procedure result status (ORD-115) is 3-Final result.
  TRANSPORT_C_NAME TEXT, -- Determines how the patient associated with this order is to be transported.
  ORD_PROV_ID TEXT, -- The provider ID of the ordering provider.
  INST_OF_UPDATE_TM TEXT, -- The day and time the order record was last updated.
  PRIMARY KEY (ORDER_PROC_ID) -- Primary Key, FOREIGN KEY (ORDER_PROC_ID) REFERENCES ORDER_PARENT_INFO (ORDER_ID) -- References ORDER_PARENT_INFO table
, FOREIGN KEY (ORDER_PROC_ID) REFERENCES ORD_PRFLST_TRK (ORDER_ID) -- References ORD_PRFLST_TRK table
, FOREIGN KEY (PAT_ID) REFERENCES PATIENT (PAT_ID) -- References PATIENT table
, FOREIGN KEY (PAT_ENC_CSN_ID) REFERENCES PAT_ENC (PAT_ENC_CSN_ID) -- References PAT_ENC table
, FOREIGN KEY (PROC_ID) REFERENCES CLARITY_EAP (PROC_ID) -- References CLARITY_EAP table
, FOREIGN KEY (AUTHRZING_PROV_ID) REFERENCES CLARITY_SER (PROV_ID) -- References CLARITY_SER table
, FOREIGN KEY (AUTHRZING_PROV_ID) REFERENCES REFERRAL_SOURCE (REFERRING_PROV_ID) -- References REFERRAL_SOURCE table
, FOREIGN KEY (ORD_CREATR_USER_ID) REFERENCES CLARITY_EMP (USER_ID) -- References CLARITY_EMP table
, FOREIGN KEY (PAT_LOC_ID) REFERENCES CLARITY_DEP (DEPARTMENT_ID) -- References CLARITY_DEP table
, FOREIGN KEY (REFG_FACILITY_ID) REFERENCES CLARITY_LOC (LOC_ID) -- References CLARITY_LOC table
, FOREIGN KEY (ORDER_ID) REFERENCES ORDER_PARENT_INFO (ORDER_ID) -- References ORDER_PARENT_INFO table
, FOREIGN KEY (ORDER_ID) REFERENCES ORD_PRFLST_TRK (ORDER_ID) -- References ORD_PRFLST_TRK table
, FOREIGN KEY (ORD_PROV_ID) REFERENCES CLARITY_SER (PROV_ID) -- References CLARITY_SER table
, FOREIGN KEY (ORD_PROV_ID) REFERENCES REFERRAL_SOURCE (REFERRING_PROV_ID) -- References REFERRAL_SOURCE table

);
