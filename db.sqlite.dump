CREATE TABLE ORD_PRFLST_TRK ( -- Tracking info for orders coming from a preference list or order template.  For Beacon, the column ORDER_TEMPLATE_ID is used, which is the unique ID of the order template in the patient's treatment plan used to create the order.-- Child table: FINALIZE_PHYSICIAN (1 to 1 rows) joined on ORDER_ID
  -- Child table: MEDICATION_COST_ESTIMATES (1 to 1 rows) joined on ORDER_ID
  -- Child table: OBS_MTHD_ID (1 to 12 rows) joined on ORDER_ID
  -- Child table: ORD_CLIN_IND (1 to 2 rows) joined on ORDER_ID
  -- Child table: ORDER_ANATOMICAL_REGION (3 to 3 rows) joined on ORDER_ID
  -- Child table: ORDER_COMMENT (1 to 2 rows) joined on ORDER_ID
  -- Child table: ORDER_DOCUMENTS (1 to 1 rows) joined on ORDER_ID
  -- Child table: ORDER_DX_MED (1 to 1 rows) joined on ORDER_MED_ID
  -- Child table: ORDER_DX_PROC (1 to 2 rows) joined on ORDER_PROC_ID
  -- Child table: ORDER_IMAGE_AVAIL_INFO (1 to 1 rows) joined on ORDER_ID
  -- Child table: ORDER_IMPRESSION (2 to 2 rows) joined on ORDER_PROC_ID
  -- Child table: ORDER_INSTANTIATED (1 to 1 rows) joined on ORDER_ID
  -- Child table: ORDER_MODALITY_TYPE (1 to 1 rows) joined on ORDER_ID
  -- Child table: ORDER_MYC_RELEASE (1 to 1 rows) joined on ORDER_PROC_ID
  -- Child table: ORDER_NARRATIVE (10 to 15 rows) joined on ORDER_PROC_ID
  -- Child table: ORDER_PENDING (1 to 2 rows) joined on ORDER_ID
  -- Child table: ORDER_RAD_ACC_NUM (1 to 2 rows) joined on ORDER_PROC_ID
  -- Child table: ORDER_RAD_READING (1 to 1 rows) joined on ORDER_PROC_ID
  -- Child table: ORDER_READ_ACK (1 to 3 rows) joined on ORDER_ID
  -- Child table: ORDER_RES_COMMENT (2 to 8 rows) joined on ORDER_ID
  -- Child table: ORDER_RESULTS (1 to 12 rows) joined on ORDER_PROC_ID
  -- Child table: ORDER_RPTD_SIG_HX (1 to 1 rows) joined on ORDER_ID
  -- Child table: ORDER_RPTD_SIG_INSTR (1 to 1 rows) joined on ORDER_ID
  -- Child table: ORDER_RPTD_SIG_TEXT (1 to 1 rows) joined on ORDER_ID
  -- Child table: ORDER_SIGNED_PROC (1 to 1 rows) joined on ORDER_PROC_ID
  -- Child table: ORDER_STATUS (1 to 3 rows) joined on ORDER_ID
  -- Child table: ORDER_SUMMARY (1 to 2 rows) joined on ORDER_ID
  -- Child table: ORD_INDICATIONS (1 to 1 rows) joined on ORDER_ID
  -- Child table: ORD_MED_ADMININSTR (2 to 2 rows) joined on ORDER_MED_ID
  -- Child table: ORD_MED_USER_ADMIN (2 to 2 rows) joined on ORDER_ID
  -- Child table: ORD_PROC_INSTR (9 to 9 rows) joined on ORDER_ID
  -- Child table: ORD_RSLT_COMPON_ID (1 to 1 rows) joined on ORDER_ID
  -- Child table: ORD_SPEC_QUEST (1 to 2 rows) joined on ORDER_ID
  -- Child table: PERFORMING_ORG_INFO (1 to 1 rows) joined on ORDER_ID
  -- Child table: PRESC_ID (1 to 1 rows) joined on ORDER_ID
  -- Child table: RIS_SGND_INFO (1 to 1 rows) joined on ORDER_PROC_ID
  -- Child table: SPEC_SOURCE_SNOMED (1 to 1 rows) joined on ORDER_ID
  -- Child table: SPEC_TYPE_SNOMED (1 to 1 rows) joined on ORDER_ID
  -- Child table: ORDER_REVIEW (1 to 3 rows) joined on ORDER_ID
  -- Child table: ORDER_DISP_INFO (1 to 1 rows) joined on ORDER_MED_ID

  ORDER_ID NUMERIC, -- The unique identifier for the order record.
  ORDER_TMPLTE_OTL_I TEXT, -- OTL ID (order template)
  MOD_FROM_OTL_YN TEXT, -- Flag whether this order is modified from its order template.
  ORD_DOSING_WEIGHT NUMERIC, -- Weight used for dosing. Always stored in kilograms.
  ORD_DW_REC_DTTM TEXT, -- The instant at which the weight was recorded.
  ORD_WT_SOURCE_C_NAME TEXT, -- This column contains the source of the patient weight used for dosing patient-controlled analgesia (PCA) medication.
  ORD_WT_COMMENTS TEXT, -- Generated comment for dosing weight.
  ORD_DOSING_HEIGHT NUMERIC, -- This column contains the patient height used for dosing PCA medication. The value stored is in inches for all orders after weight-based dosing was turned on, or starting in Spring 2008, whichever came first. Values are stored in centimeters for treatment plan orders made prior to that.
  ORD_HT_REC_DTTM TEXT, -- The instant at which the height was recorded.
  ORD_HT_SOURCE_C_NAME TEXT, -- This column contains the source of the patient height used for dosing patient-controlled analgesia (PCA) medication.
  ORD_HT_COMMENTS TEXT, -- Generated comment for dosing height.
  ORD_DOSING_BSA NUMERIC, -- The body surface area used for dosing.
  ORD_BSA_SRC_C_NAME TEXT, -- This column contains the source of the body surface area used for dosing patient-controlled analgesia (PCA) medication.
  ORD_BSA_CALC_DTL TEXT, -- The dosing body surface area calculation details with weight, height and recorded instants.
  ORD_DOSING_BSA_ORIG NUMERIC, -- The original (uncapped) BSA of an order
  PRIMARY KEY (ORDER_ID) -- Primary Key
);
