CREATE TABLE PAT_ENC_HSP ( -- This table is the primary table for hospital encounter information. A hospital encounter is a contact in the patient record created through an ADT workflow such as preadmission, admission, ED Arrival, discharge, and hospital outpatient visit (HOV) contacts. These contact types have the ADT flag (I EPT 10101) set to 1. This table excludes all other contacts.-- Child table: ASSOCIATED_REFERRALS (1 to 1 rows) joined on PAT_ENC_CSN_ID
  -- Child table: HSP_ADMIT_DIAG (1 to 1 rows) joined on PAT_ENC_CSN_ID
  -- Child table: HSP_ADMIT_PROC (1 to 1 rows) joined on PAT_ENC_CSN_ID
  -- Child table: HSP_ATND_PROV (1 to 1 rows) joined on PAT_ENC_CSN_ID
  -- Child table: PAT_ENC_ADMIT_DX_AUDIT (1 to 1 rows) joined on PAT_ENC_CSN_ID
  -- Child table: PAT_ENC_QNRS_ANS (1 to 1 rows) joined on PAT_ENC_CSN_ID

  PAT_ENC_CSN_ID NUMERIC, -- The unique contact serial number for this contact. This number is unique across all patient encounters in your system. If you use IntraConnect, this is the Unique Contact Identifier (UCI).
  ADT_PAT_CLASS_C_NAME TEXT, -- The category value corresponding to the patient classification for this patient contact.
  ADT_PATIENT_STAT_C_NAME TEXT, -- The category value corresponding to the patient status for this patient contact.
  EXP_ADMISSION_TIME TEXT, -- The date and time of the expected admission for this patient contact.
  ADMIT_SOURCE_C_NAME TEXT, -- The category value corresponding to the admission source for this patient contact.
  HOSP_ADMSN_TIME TEXT, -- The date and time that the patient was first admitted to the facility, bedded in the ED, or confirmed for an HOV for this contact, regardless of patient's base patient class.
  ADMIT_CONF_STAT_C_NAME TEXT, -- The category value corresponding to the (admission) confirmation status for this patient contact.
  HOSP_DISCH_TIME TEXT, -- The hospital discharge date and time for this patient contact.
  HOSP_ADMSN_TYPE_C_NAME TEXT, -- The category value corresponding to the admission type for the patient contact.
  DISCH_DISP_C_NAME TEXT, -- The category value corresponding to the discharge disposition for this patient contact.
  INPATIENT_DATA_ID TEXT, -- The unique ID of the Inpatient Data Store record.
  OP_ADM_DATE TEXT, -- The date and time during the hospital encounter when the patient first received a base patient class of outpatient.
  OP_ADM_EVENT_ID NUMERIC, -- The event record for the hospital encounter where the patient first received a base patient class of outpatient.
  HOSPITAL_AREA_ID NUMERIC, -- This field identifies the hospital area associated with the hospital unit in this patient contact.
  PAT_ENC_DATE_REAL NUMERIC, -- A unique contact date in decimal format. The integer portion of the number indicates the date of contact. The digits after the decimal distinguish different contacts on the same date and are unique for each contact on that date. For example, .00 is the first/only contact, .01 is the second contact, etc.
  CONTACT_DATE TEXT, -- The date of this contact in calendar format.
  ENC_CLOSED_OR_COMPLETED_DATE TEXT, -- The date that the encounter was closed or completed.
  HOSP_DISCH_UTC_DTTM TEXT, -- The hospital discharge date and time in UTC.
  HOSP_ADMSN_UTC_DTTM TEXT, -- The hospital admission date and time in UTC.
  PRIMARY KEY (PAT_ENC_CSN_ID) -- Primary Key, FOREIGN KEY (PAT_ENC_CSN_ID) REFERENCES PAT_ENC_DISP (PAT_ENC_CSN_ID) -- References PAT_ENC_DISP table
, FOREIGN KEY (PAT_ENC_CSN_ID) REFERENCES PAT_ENC (PAT_ENC_CSN_ID) -- References PAT_ENC table
, FOREIGN KEY (INPATIENT_DATA_ID) REFERENCES IP_DATA_STORE (INPATIENT_DATA_ID) -- References IP_DATA_STORE table
, FOREIGN KEY (OP_ADM_EVENT_ID) REFERENCES CLARITY_ADT (EVENT_ID) -- References CLARITY_ADT table
, FOREIGN KEY (HOSPITAL_AREA_ID) REFERENCES CLARITY_LOC (LOC_ID) -- References CLARITY_LOC table

);
