CREATE TABLE RECONCILE_CLM_OT ( -- This table contains over time single information for claim reconciliation records.
  CLAIM_REC_ID TEXT, -- The unique ID of the claim reconciliation record.
  CONTACT_DATE TEXT, -- The contact date of the claim, in external format.
  CONTACT_DATE_REAL NUMERIC, -- The contact date of the claim, in internal format.
  PAYR_CLM_AMT_SUBMT NUMERIC, -- The amount submitted by the payer on this claim.
  PAYOR_NAME TEXT, -- The payer name of the payer on the claim.
  CH_REF_NUM TEXT, -- This is the claim's clearinghouse reference number.
  CH_NAME TEXT, -- This is the claim's clearinghouse name.
  STATUS_DATE TEXT, -- The status date of the claim.
  FILE_NAME TEXT, -- This is the filename associated with this claim.
  CLM_CLOSED_REASON_C_NAME TEXT, -- The category value for the reason the claim was closed. This value can be used to link to the ZC_CLM_CLSD_REASON table to get more information on the category values.
  CH_SENT_DATE TEXT, -- Stores the date that the claim was sent out of the clearinghouse.
  PAYOR_RECV_DATE TEXT, -- Stores the date that the payer received the claim.
  CLAIM_SEL_ACT_C_NAME TEXT, -- Stores the action that was selected to be performed on the invoice.
  PERFORM_ACT_C_NAME TEXT, -- Will store the action that was performed on the invoice.
  PERFORM_ACT_RSN TEXT, -- Stores the reason for the action that was performed on the invoice.
  GROUP_CONTROL_NUM INTEGER, -- Stores the group control number (GS-06) value for an American National Standards Institute (ANSI) electronic claim file.
  SUPPORTED_ACTIONS_C_NAME TEXT, -- The claim actions supported by the interface profile at the time a claim status update is received from an external entity.
  PRIMARY KEY (CLAIM_REC_ID, CONTACT_DATE_REAL) -- Primary Key, FOREIGN KEY (CLAIM_REC_ID) REFERENCES RECONCILE_CLM (CLAIM_REC_ID)
, FOREIGN KEY (CLAIM_REC_ID) REFERENCES RECONCILE_CLM (CLAIM_REC_ID) -- References RECONCILE_CLM table

);
