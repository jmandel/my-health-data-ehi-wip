CREATE TABLE CLARITY_ADT ( -- The CLARITY_ADT table is the master table for ADT event history information. This table contains several foreign keys for other ADT tables.
  EVENT_ID NUMERIC, -- The unique ID number of the ADT event record.
  EVENT_TYPE_C_NAME TEXT, -- The category value corresponding to the type of the event record.
  EVENT_SUBTYPE_C_NAME TEXT, -- The category value indicating whether the event record has been changed or deleted.
  DEPARTMENT_ID NUMERIC, -- The ID number of the unit of the event record at the effective time.
  EFFECTIVE_TIME TEXT, -- The instant when the event was supposed to have happened.
  PAT_ID TEXT, -- The ID of the patient of the event record at the effective time.
  PAT_ENC_DATE_REAL NUMERIC, -- This is a numeric representation of the date of this encounter in your system. The integer portion of the number specifies the date of the encounter. The digits after the decimal point indicate multiple visits on one day.
  PAT_ENC_CSN_ID NUMERIC, -- The unique contact serial number for this contact. This number is unique across all patient encounters in your system. If you use IntraConnect, this is the Unique Contact Identifier (UCI).
  EVENT_TIME TEXT, -- The instant when the event record was actually created.
  USER_ID TEXT, -- The ID number of the user who created the event record.
  USER_ID_NAME TEXT, -- The name of the user record. This name may be hidden.
  PAT_CLASS_C_NAME TEXT, -- The category value corresponding to the classification for the patient of the event record at the effective time.
  ORIG_EVENT_TIME TEXT, -- The instant when the original subtype record for this event was actually created.
  ORIG_EFF_TIME TEXT, -- The instant when the original subtype record for this event was supposed to have happened.
  NEXT_OUT_EVENT_ID NUMERIC, -- The ID number of the next 'transfer out' or 'discharge' type record for this bed for the patient CSN of these records.
  LAST_IN_EVENT_ID NUMERIC, -- The ID number of the last 'transfer in' or 'admission' type record for this bed for the patient CSN of these records.
  BASE_PAT_CLASS_C_NAME TEXT, -- The category value corresponding to the base classification for the patient of the event record at the effective time.
  OUT_EVENT_TYPE_C_NAME TEXT, -- This column contains the outgoing event type as it would currently be interpreted
  FROM_BASE_CLASS_C_NAME TEXT, -- This column contains the base patient class that the patient had prior to this event.
  TO_BASE_CLASS_C_NAME TEXT, -- This column contains the base patient class that the patient had after this event.
  ORIGINAL_EVENT_ID NUMERIC, -- The unique ID of the original event that this event record replaces. If this event record has not been canceled or updated, this column will be equal to the EVENT_ID column. This column is not necessarily equal to the CANC_EVENT_ID column. If the original event has been updated multiple times, then this column will show the ID of the original event, not the ID of the event that was most recently replaced by this record.
  PRIMARY KEY (EVENT_ID) -- Primary Key, FOREIGN KEY (DEPARTMENT_ID) REFERENCES CLARITY_DEP (DEPARTMENT_ID) -- References CLARITY_DEP table
, FOREIGN KEY (PAT_ID) REFERENCES PATIENT (PAT_ID) -- References PATIENT table
, FOREIGN KEY (PAT_ENC_CSN_ID) REFERENCES PAT_ENC_DISP (PAT_ENC_CSN_ID) -- References PAT_ENC_DISP table
, FOREIGN KEY (PAT_ENC_CSN_ID) REFERENCES PAT_ENC_HSP (PAT_ENC_CSN_ID) -- References PAT_ENC_HSP table
, FOREIGN KEY (PAT_ENC_CSN_ID) REFERENCES PAT_ENC (PAT_ENC_CSN_ID) -- References PAT_ENC table
, FOREIGN KEY (USER_ID) REFERENCES CLARITY_EMP (USER_ID) -- References CLARITY_EMP table

);
