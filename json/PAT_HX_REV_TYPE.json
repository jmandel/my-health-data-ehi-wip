                "target": "PAT_ENC_CSN_ID"
              },
              {
                "source": "GROUP_LINE",
                "target": "LINE_COUNT"
              }
            ]
          },
          {
            "type": "has-parent-table",
            "source": "PAT_HX_REV_TYPE",
            "target": "SOCIAL_ADL_HX",
            "joinKeys": [
              {
                "source": "PAT_ENC_CSN_ID",
                "target": "PAT_ENC_CSN_ID"
              },
              {
                "source": "GROUP_LINE",
                "target": "LINE"
              }
            ]
          },
          {
            "type": "has-parent-table",
            "source": "PAT_HX_REV_TYPE",
            "target": "PAT_ENC",
            "joinKeys": [
