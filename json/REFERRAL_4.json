{
  "$meta": {
    "schemas": {
      "REFERRAL_4": {
        "name": "REFERRAL_4",
        "description": "The REFERRAL_4 table is a continuation of the REFERRAL_3 which is continuation of the REFERRAL_2 table. These tables are the primary tables for referral information stored in the system.",
        "primaryKey": [
          {
            "columnName": "REFERRAL_ID",
            "ordinalPosition": 1
          }
        ],
        "columns": [
          {
            "ordinalPosition": 1,
            "name": "REFERRAL_ID",
            "type": "NUMERIC",
            "discontinued": false,
            "description": "The referral ID for the referral record."
          },
          {
            "ordinalPosition": 18,
            "name": "RFL_DIRECTION_C_NAME",
            "type": "VARCHAR",
            "discontinued": false,
            "description": "The referral direction, as calculated by logic for transitions of care",
            "entries": [
              "Internal",
              "Incoming",
              "Outgoing"
            ]
          },
          {
            "ordinalPosition": 19,
            "name": "REFD_TO_LINK_PROV_YN",
            "type": "VARCHAR",
            "discontinued": false,
            "description": "Is the referred to provider an EpicCare Link provider",
            "entries": [
              "No",
              "Yes"
            ]
          },
          {
            "ordinalPosition": 20,
            "name": "REFD_TO_LINK_LOC_YN",
            "type": "VARCHAR",
            "discontinued": false,
            "description": "Is the referred to location an EpicCare Link location"
          },
          {
            "ordinalPosition": 21,
            "name": "IS_LEAKED_YN",
            "type": "VARCHAR",
            "discontinued": false,
            "description": "Holds whether the referral is considered leaked"
          },
          {
            "ordinalPosition": 25,
            "name": "RECENT_AUTH_RSN_C_NAME",
            "type": "VARCHAR",
            "discontinued": false,
            "description": "This item holds the most recent non-empty authorization reason for the referral. If this item is null, it means the referral was never authorized. If value is added/updated to the authorization code (I RFL 73), same value is copied into this item. But if authorization code (I RFL 73) is cleared, then this item is not updated."
          }
        ]
      }
    }
  },
