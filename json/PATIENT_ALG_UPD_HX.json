                "source": "PAT_ID",
                "target": "PAT_ID"
              },
              {
                "source": "LINE",
                "target": "GROUP_LINE"
              }
            ]
          },
          {
            "type": "has-parent-table",
            "source": "PATIENT_ALG_UPD_HX",
            "target": "PATIENT",
            "joinKeys": [
              {
                "source": "PAT_ID",
                "target": "PAT_ID"
              }
            ]
          }
        ]
      }
    }
  },
