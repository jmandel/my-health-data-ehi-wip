              "Denied - Registration",
              "Denied - Miscellaneous",
              "Other Allowance",
              "Estimate Accuracy Write Off",
              "Resident Trust Account",
              "Downgrade"
            ]
          },
          {
            "ordinalPosition": 54,
            "name": "WRITE_OFF_RSN_C_NAME",
            "type": "VARCHAR",
            "discontinued": false,
            "description": "The mapped write-off reason for the adjustment.",
            "entries": [
