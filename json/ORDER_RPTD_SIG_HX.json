{
  "$meta": {
    "schemas": {
      "ORDER_RPTD_SIG_HX": {
        "name": "ORDER_RPTD_SIG_HX",
        "description": "This table contains a history of sig-related data for prescriptions, both what the provider initially prescribed and what the patient later reported taking. Most commonly, the first row for any prescription represents the sig as the prescription was written, and subsequent rows will represent changes in what the patient reports taking.",
        "primaryKey": [
          {
            "columnName": "ORDER_ID",
            "ordinalPosition": 1
          },
          {
            "columnName": "LINE",
            "ordinalPosition": 2
          }
        ],
        "columns": [
          {
            "ordinalPosition": 1,
            "name": "ORDER_ID",
            "type": "NUMERIC",
            "discontinued": false,
            "description": "The unique identifier for the order record."
          },
          {
            "ordinalPosition": 2,
            "name": "LINE",
            "type": "INTEGER",
            "discontinued": false,
            "description": "The line number for the information associated with this record. Multiple pieces of information can be associated with this record."
          },
          {
            "ordinalPosition": 3,
            "name": "PAT_ENC_CSN_ID",
            "type": "NUMERIC",
            "discontinued": false,
            "description": "The unique contact serial number for this contact. This number is unique across all patient encounters in your system. If you use IntraConnect, this is the Unique Contact Identifier (UCI)."
          },
          {
            "ordinalPosition": 4,
            "name": "ENTRY_USER_ID",
            "type": "VARCHAR",
            "discontinued": false,
            "description": "The unique identifier of the user who entered the medication instructions."
          },
          {
            "ordinalPosition": 5,
            "name": "ENTRY_USER_ID_NAME",
            "type": "VARCHAR",
            "discontinued": false,
            "description": "The name of the user record. This name may be hidden."
          },
          {
            "ordinalPosition": 6,
            "name": "ENTRY_DTTM",
            "type": "DATETIME (Local) ",
            "discontinued": false,
            "description": "The date and time when these medication instructions were entered."
          },
          {
            "ordinalPosition": 7,
            "name": "ACTION_C_NAME",
            "type": "VARCHAR",
            "discontinued": false,
            "description": "The action represented by this sig - e.g. the initial prescription; the pharmacist has edited the sig; or the patient reports taking this medication differently from how it was prescribed.",
            "entries": [
              "Pended Reorder Invalid Defaults",
              "Historical Med Edited",
              "Initial Prescription",
              "Prescribed Sig Changed",
              "Taking Differently",
              "Taking As Reported",
              "Taking As Prescribed",
              "Not Taking"
            ]
          },
          {
            "ordinalPosition": 8,
            "name": "SOURCE_C_NAME",
            "type": "VARCHAR",
            "discontinued": false,
            "description": "The location from which this sig was entered.",
            "entries": [
              "Ord Rec PTA Med Review",
              "Meds and Orders Section",
              "Med Documentation Section",
              "Order Creation",
              "Ambulatory Pharmacy",
              "Marked as Taking",
              "EpicCare Remote",
              "Update Indications on Resume",
              "Community",
              "HH In Basket Med Rec",
              "Update Reported Sig on Resume",
              "RxFill Reconciliation",
              "Inpatient Pharmacy Verification"
            ]
          },
          {
            "ordinalPosition": 12,
