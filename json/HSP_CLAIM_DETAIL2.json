          {
            "ordinalPosition": 16,
            "name": "SG_LOC_ID",
            "type": "NUMERIC",
            "discontinued": false,
            "description": "The location for this claim."
          },
          {
            "ordinalPosition": 18,
            "name": "SG_POS_ID",
            "type": "NUMERIC",
            "discontinued": false,
            "description": "The place of service ID for this claim."
          },
          {
            "ordinalPosition": 21,
            "name": "CLAIM_CLASS_C_NAME",
            "type": "VARCHAR",
            "discontinued": false,
            "description": "The account class used to evaluate this claim."
