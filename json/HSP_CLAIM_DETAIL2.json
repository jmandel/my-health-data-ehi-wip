{
  "$meta": {
    "schemas": {
      "HSP_CLAIM_DETAIL2": {
        "name": "HSP_CLAIM_DETAIL2",
        "description": "This table contains detailed claim print record information for claims associated with the hospital liability bucket.",
        "primaryKey": [
          {
            "columnName": "CLAIM_PRINT_ID",
            "ordinalPosition": 1
          }
        ],
        "columns": [
          {
            "ordinalPosition": 1,
            "name": "CLAIM_PRINT_ID",
            "type": "NUMERIC",
            "discontinued": false,
            "description": "The ID of the claim record associated with a single hospital liability bucket."
          },
          {
            "ordinalPosition": 2,
            "name": "SA_ID",
            "type": "NUMERIC",
            "discontinued": false,
            "description": "This column holds the service area for the claim."
          },
          {
            "ordinalPosition": 3,
            "name": "INACTV_CLP_YN",
            "type": "VARCHAR",
            "discontinued": false,
            "description": "This column has a value of yes when the claim is inactive.",
            "entries": [
              "No",
              "Yes"
            ]
          },
          {
            "ordinalPosition": 4,
            "name": "CLAIM_ACCEPT_DTTM",
            "type": "DATETIME (Local) ",
            "discontinued": false,
            "description": "This column holds the instant the claim was accepted."
          },
          {
            "ordinalPosition": 5,
            "name": "SG_PAYOR_ID",
            "type": "NUMERIC",
            "discontinued": false,
            "description": "The payer ID for this claim."
          },
          {
            "ordinalPosition": 6,
            "name": "SG_PLAN_ID",
            "type": "NUMERIC",
            "discontinued": false,
            "description": "The plan ID for this claim."
          },
          {
            "ordinalPosition": 7,
            "name": "SG_CVG_ID",
            "type": "NUMERIC",
            "discontinued": false,
            "description": "The coverage ID for this claim."
          },
          {
            "ordinalPosition": 8,
            "name": "INVOICE_NUM",
            "type": "VARCHAR",
            "discontinued": false,
            "description": "The invoice number for this claim."
          },
          {
            "ordinalPosition": 10,
            "name": "SG_GR_ACCT_ID",
            "type": "NUMERIC",
            "discontinued": false,
            "description": "The guarantor account ID for this claim."
          },
          {
            "ordinalPosition": 11,
            "name": "HOSPITAL_ACCT_ID",
            "type": "NUMERIC",
            "discontinued": false,
            "description": "The hospital account ID for this claim."
          },
          {
            "ordinalPosition": 12,
            "name": "HLB_ID",
            "type": "NUMERIC",
            "discontinued": false,
