          {
            "ordinalPosition": 22,
            "name": "CLAIM_BASE_CLASS_C_NAME",
            "type": "VARCHAR",
            "discontinued": false,
            "description": "The base account class used to evaluate this claim.",
            "entries": [
              "Inpatient",
              "Outpatient",
              "Emergency"
            ]
          },
          {
            "ordinalPosition": 23,
            "name": "MIN_SERVICE_DT",
            "type": "DATETIME",
            "discontinued": false,
            "description": "The minimum service date for this claim."
          },
          {
            "ordinalPosition": 24,
            "name": "MAX_SERVICE_DT",
            "type": "DATETIME",
            "discontinued": false,
            "description": "The maximum service date for this claim."
