            "ordinalPosition": 32,
            "name": "TTL_PMT_AMT",
            "type": "NUMERIC",
            "discontinued": false,
            "description": "Total payment amount."
          },
          {
            "ordinalPosition": 34,
            "name": "UB_BILL_TYPE",
            "type": "VARCHAR",
            "discontinued": false,
            "description": "Type of bill."
          },
          {
            "ordinalPosition": 39,
            "name": "UB_CVD_DAYS",
            "type": "INTEGER",
            "discontinued": false,
            "description": "Covered days."
          },
          {
            "ordinalPosition": 40,
            "name": "UB_COINS_DAYS",
            "type": "INTEGER",
            "discontinued": false,
            "description": "Coinsurance days."
          },
          {
            "ordinalPosition": 41,
            "name": "UB_NON_CVD_DAYS",
            "type": "INTEGER",
            "discontinued": false,
            "description": "Non-covered days."
